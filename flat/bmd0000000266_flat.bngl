	X13() =  if((heat_shock==1),(16),(1))
	X15f() =  if((heat_shock==50),(1),(1))
	X12r() =  if((heat_shock==1),(16),(1))
	flux_X3_out() = 76.434*G1P_cell^0.593*G6P_cell^(-0.412)*X12r^0.718*X13^0.18*X15f^0.103
