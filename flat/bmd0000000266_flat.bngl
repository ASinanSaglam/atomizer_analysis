	cell 3 1.0
	external 3 1.0
end compartments
begin molecule types
	T6P()
	glucose()
	UDPG()
	glycogen()
	G1P()
	G6P()
	trehalose()
end molecule types
begin seed species
	@external:glucose() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #glucose #X0
	@cell:glucose() 1.8066000000000002e+19 #original 0.03milimole/L #glucose #X1
