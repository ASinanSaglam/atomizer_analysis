	@cell:G1P() 6.022000000000001e+19 #original 0.1milimole/L #G1P #X3
	@cell:UDPG() 4.2154e+20 #original 0.7milimole/L #UDPG #X4
	@cell:glycogen() 6.022e+20 #original 1.0milimole/L #glycogen #X5
