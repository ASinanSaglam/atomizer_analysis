begin model
begin parameters
	r1_kT 0.141000
	r2_kL 0.013000
	r3_kB 0.058000
end parameters
begin compartments
	#volume units: normalized volume
	extracellular 3 1.0
	endosome 3 1.0
	neuroplasm 3 1.0
end compartments
begin molecule types
	free_BoNT_A()
	bound_BoNT_A()
	transloc_BoNT_A()
	lytic_BoNT_A()
end molecule types
begin seed species
	@extracellular:free_BoNT_A() 6.022e+23 # 1.0mol/L * 6.022e23/mol *1.0L #free_BoNT_A #free
end seed species
begin observables
	Species free_BoNT_A_extracellular @extracellular:free_BoNT_A() #free_BoNT/A
	Species bound_BoNT_A_extracellular @extracellular:bound_BoNT_A() #bound_BoNT/A
	Species transloc_BoNT_A_endosome @endosome:transloc_BoNT_A() #transloc_BoNT/A
	Species lytic_BoNT_A_neuroplasm @neuroplasm:lytic_BoNT_A() #lytic_BoNT/A
end observables
begin functions
	tension() = 1 - lytic_BoNT_A_neuroplasm
end functions
begin reaction rules
	endocytosis: bound_BoNT_A()@extracellular -> transloc_BoNT_A()@endosome r1_kT
	translocation: transloc_BoNT_A()@endosome -> lytic_BoNT_A()@neuroplasm r2_kL
	binding: free_BoNT_A()@extracellular -> bound_BoNT_A()@extracellular r3_kB
end reaction rules
end model
