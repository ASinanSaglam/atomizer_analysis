###
begin model
begin parameters
	alpha1 3.0
	beta1 0.2
	alpha2 4.0
	beta2 0.02
	k1 0.24
	k2 0.0017
	g11 0.5
	g21 -0.5
	g12 1.0
	g22 0
end parameters
begin molecule types
	Osteoblasts()
	Osteoclasts()
	BoneMass()
end molecule types
begin seed species
	Osteoclasts() 6.660332000000001e+24 # 11.06mol/L * 6.022e23/mol *1.0L #Osteoclasts #C
	Osteoblasts() 1.27744686e+26 #original 212.13mol/L #Osteoblasts #B
	BoneMass() 6.022e+25 #original 100.0mol/L #BoneMass #z
end seed species
begin observables
	Species Osteoclasts Osteoclasts() #Osteoclasts
	Species Osteoblasts Osteoblasts() #Osteoblasts
	Species BoneMass BoneMass() #BoneMass
end observables
begin functions
	y1() =  if((Osteoclasts>C_bar),(Osteoclasts-C_bar),(0))
	y2() =  if((Osteoblasts>B_bar),(Osteoblasts-B_bar),(0))
	arRatez() = -k1*y1 + k2*y2
	gamma() = g12*g21 - (1 - g11)*(1 - g22)
	B_bar() = (beta1/alpha1)^(g12/gamma)*(beta2/alpha2)^((1 - g11)/gamma)
	C_bar() = (beta1/alpha1)^((1 - g22)/gamma)*(beta2/alpha2)^(g21/gamma)
	arRateB() = -Osteoblasts*beta2 + Osteoblasts^g22*Osteoclasts^g12*alpha2
	arRateC() = Osteoblasts^g21*Osteoclasts^g11*alpha1 - Osteoclasts*beta1
end functions
begin reaction rules
	0  -> Osteoclasts() arRateC #rateLaw
	0  -> Osteoblasts() arRateB #rateLaw
	0  -> BoneMass() arRatez #rateLaw
end reaction rules
end model
