	beta_mLVA -4.8916
	tau_mLVA 40.0
	theta_hLVA -59.2
	beta_hLVA 11.2326
	tau_hLVA 350.0
	theta_mHVA -10.0
	beta_mHVA -6.5
	theta_mNaP -40.0
	beta_mNaP -4.0
	theta_hNaP -54.0
