	re8_reactants_0: 0  -> ATF6T()@ERlumen -1*(fRate17())
	re8_products_0: 0  -> ATF6GB()@Golgi fRate17()
	rew1: ATF6T()@ERlumen -> 0  fRate18() #Modifiers(WFS1, WFS1, ATF6T)
	re9: ATF6GB()@Golgi -> 0  kdATF6GB #Modifiers(ATF6GB)
	re10: ATF6GB()@Golgi -> ATF6p50()@cytoplasm kcleave #Modifiers(ATF6GB)
	re11: ATF6p50()@cytoplasm -> 0  kdATF6p50 #Modifiers(ATF6p50)
	rew2: 0  -> mWFS1()@cytoplasm fRate22() #Modifiers(ATF6p50, ATF6p50)
	rew3: mWFS1()@cytoplasm -> 0  kdmWFS #Modifiers(mWFS1)
	rew4: 0  -> WFS1()@ERlumen fRate24() #Modifiers(mWFS1, mWFS1)
	rew5: WFS1()@ERlumen -> 0  kdWFS #Modifiers(WFS1)
	re12: 0  -> mXbp1u()@cytoplasm fRate26() #Modifiers(ATF6p50, ATF6p50)
	re13: mXbp1u()@cytoplasm -> 0  kdmXU #Modifiers(mXbp1u)
	re14_reactants_0: 0  -> mXbp1u()@cytoplasm -1*(fRate28())
	re14_products_0: 0  -> mXbp1s()@cytoplasm fRate28()
	re15: mXbp1s()@cytoplasm -> 0  kdmXS #Modifiers(mXbp1s)
	re16: 0  -> Xbp1s()@cytoplasm fRate30() #Modifiers(mXbp1s, mXbp1s)
	re17: Xbp1s()@cytoplasm -> 0  kdXS #Modifiers(Xbp1s)
	re18: 0  -> mBiPT()@cytoplasm fRate32() #Modifiers(Xbp1s, ATF6p50, Xbp1s, ATF6p50)
	re19: mBiPT()@cytoplasm -> 0  kdmBiP #Modifiers(mBiPT)
	re20: 0  -> BiPT()@ERlumen fRate34() #Modifiers(mBiPT, mBiPT)
	re21: BiPT()@ERlumen -> 0  kdBiP #Modifiers(BiPT)
	re23: 0  -> ATF4()@cytoplasm fRate36()
	re24: ATF4()@cytoplasm -> 0  kdATF4 #Modifiers(ATF4)
	re25: 0  -> mCHOP()@cytoplasm fRate38() #Modifiers(ATF4, ATF6p50, ATF4, ATF6p50)
	re26: mCHOP()@cytoplasm -> 0  kdmCHOP #Modifiers(mCHOP)
	re27: 0  -> CHOP()@cytoplasm fRate40() #Modifiers(mCHOP, mCHOP)
	re28: CHOP()@cytoplasm -> 0  kdCHOP #Modifiers(CHOP)
	re29: 0  -> mGADD34()@cytoplasm fRate42() #Modifiers(CHOP, CHOP)
	re30: mGADD34()@cytoplasm -> 0  kdmGADD34 #Modifiers(mGADD34)
	re31: 0  -> GADD34()@cytoplasm fRate44() #Modifiers(mGADD34, mGADD34)
	re32: GADD34()@cytoplasm -> 0  kdGADD34 #Modifiers(GADD34)
	rea1: 0  -> BCL2T()@mitochondria fRate46() #Modifiers(CHOP, CHOP)
	rea2: BCL2T()@mitochondria -> 0  kdbc #Modifiers(BCL2T)
	rea3: 0  -> BAXmT()@mitochondria fRate48()
	rea4: 0  -> BAXmT()@mitochondria fRate49()
	rea5: BAXmT()@mitochondria -> 0  kfx #Modifiers(BAXmT)
	rea6: BAXmT()@mitochondria -> 0  fRate51() #Modifiers(BAXmT)
	rea7: BAXmT()@mitochondria -> 0  kbx #Modifiers(BAXmT)
	rea8: 0  -> BH3T()@cytoplasm ks3
	rea9: 0  -> BH3T()@cytoplasm fRate54() #Modifiers(CHOP, CHOP)
	rea10: BH3T()@cytoplasm -> 0  kd3 #Modifiers(BH3T)
	rea11: 0  -> BAXmBCL2()@mitochondria fRate56()
	rea12: BAXmBCL2()@mitochondria -> 0  kdsx #Modifiers(BAXmBCL2)
	rea13: BAXmBCL2()@mitochondria -> 0  kbx #Modifiers(BAXmBCL2)
	rea14: 0  -> BH3BCL2()@mitochondria fRate59()
	rea15: BH3BCL2()@mitochondria -> 0  kds3 #Modifiers(BH3BCL2)
	rea16: BH3BCL2()@mitochondria -> 0  kd3 #Modifiers(BH3BCL2)
