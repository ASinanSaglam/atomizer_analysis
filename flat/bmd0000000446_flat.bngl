	kdXS 0.1 #units:rate1
	kdBiP 0.01 #units:rate1
	kdATF6 0.1 #units:rate1
	kdATF6GB 0.1 #units:rate1
	kdATF6p50 0.1 #units:rate1
	kdWFS 0.1 #units:rate1
	kdATF4 0.1 #units:rate1
	kdCHOP 0.1 #units:rate1
