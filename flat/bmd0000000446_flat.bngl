begin molecule types
	ATF6T()
	BAXmT()
	BAXmBCL2()
	mCHOP()
	ATF6GB()
	mGADD34()
	BiPT()
	GADD34()
	ATF4()
	BH3T()
	BiRE1()
	BiPER()
