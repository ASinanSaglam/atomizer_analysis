begin model
begin parameters
	tmr 10.0 #units:dimensionless
	IRE1T 1.0 #units:substance
	PERKT 1.0 #units:substance
	eIF2aT 1.0 #units:substance
	CReP 0.1 #units:substance
	kf 10.0 #units:rate2
	kr 1.0 #units:rate1
	n 4.0 #units:dimensionless
	nh 2.0 #units:dimensionless
	basalXBP 1.0 #units:substance
	basalBiP 1.0 #units:substance
	krcXU 5.0 #units:substance
	krcBiP 5.0 #units:substance
	krcWFS 1.0 #units:substance
	krcCHOP 1.0 #units:substance
	krcGADD34 1.0 #units:substance
	kmXbp 10.0 #units:dimensionless
	kmAtfsXBP 10.0 #units:dimensionless
