begin observables
	Species UFPT_ERlumen @ERlumen:UFPT() #UFPT
	Species BiUFP_ERlumen @ERlumen:BiUFP() #BiUFP
	Species BiRE1_ERlumen @ERlumen:BiRE1() #BiRE1
	Species BiATF_ERlumen @ERlumen:BiATF() #BiATF
	Species BiPER_ERlumen @ERlumen:BiPER() #BiPER
	Species IRE1A_ERlumen @ERlumen:IRE1A() #IRE1A
	Species PERKA_ERlumen @ERlumen:PERKA() #PERKA
	Species mXbp1u_cytoplasm @cytoplasm:mXbp1u() #mXbp1u
	Species mXbp1s_cytoplasm @cytoplasm:mXbp1s() #mXbp1s
	Species Xbp1s_cytoplasm @cytoplasm:Xbp1s() #Xbp1s
	Species mBiPT_cytoplasm @cytoplasm:mBiPT() #mBiPT
	Species BiPT_ERlumen @ERlumen:BiPT() #BiPT
	Species ATF6T_ERlumen @ERlumen:ATF6T() #ATF6T
	Species ATF6GB_Golgi @Golgi:ATF6GB() #ATF6GB
	Species ATF6p50_cytoplasm @cytoplasm:ATF6p50() #ATF6p50
	Species mWFS1_cytoplasm @cytoplasm:mWFS1() #mWFS1
	Species WFS1_ERlumen @ERlumen:WFS1() #WFS1
	Species ATF4_cytoplasm @cytoplasm:ATF4() #ATF4
	Species mCHOP_cytoplasm @cytoplasm:mCHOP() #mCHOP
	Species CHOP_cytoplasm @cytoplasm:CHOP() #CHOP
	Species mGADD34_cytoplasm @cytoplasm:mGADD34() #mGADD34
	Species GADD34_cytoplasm @cytoplasm:GADD34() #GADD34
