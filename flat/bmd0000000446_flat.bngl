	Species BAXmT_mitochondria @mitochondria:BAXmT() #BAXmT
	Species BH3T_cytoplasm @cytoplasm:BH3T() #BH3T
	Species BAXmBCL2_mitochondria @mitochondria:BAXmBCL2() #BAXmBCL2
	Species BH3BCL2_mitochondria @mitochondria:BH3BCL2() #BH3BCL2
end observables
begin functions
	mUFPT() = 13
	BAXm() = -BAXmBCL2_mitochondria + BAXmT_mitochondria
	BH3() = -BH3BCL2_mitochondria + BH3T_cytoplasm
	BCL2() = -BAXmBCL2_mitochondria + BCL2T_mitochondria - BH3BCL2_mitochondria
	eIF2a() =  eIF2aT*if(((0.5*PERKA_ERlumen*kphos*n)==0&&(kdephos*(CReP+GADD34_cytoplasm))==0),(0),(2*(kdephos*(CReP+GADD34_cytoplasm))*(K/eIF2aT)/(  ( (0.5*PERKA_ERlumen*kphos*n) )   - ( (kdephos*(CReP+GADD34_cytoplasm)) )   + ( (0.5*PERKA_ERlumen*kphos*n) )   * ( (J/eIF2aT) )   + ( (kdephos*(CReP+GADD34_cytoplasm)) )   * ( (K/eIF2aT) )    +((((  ( (0.5*PERKA_ERlumen*kphos*n) )   - ( (kdephos*(CReP+GADD34_cytoplasm)) )   + ( (0.5*PERKA_ERlumen*kphos*n) )   * ( (J/eIF2aT) )   + ( (kdephos*(CReP+GADD34_cytoplasm)) )   * ( (K/eIF2aT) )    )^(2))-4*((0.5*PERKA_ERlumen*kphos*n)-(kdephos*(CReP+GADD34_cytoplasm)))*(kdephos*(CReP+GADD34_cytoplasm))*(K/eIF2aT))^(1/2)))))
	spliceRate() = 0.5*ksplice*(0.5*IRE1A_ERlumen*n + krcSplice + mXbp1u_cytoplasm - 1.4142135623731*sqrt(-IRE1A_ERlumen*mXbp1u_cytoplasm*n + 0.5*(0.5*IRE1A_ERlumen*n + krcSplice + mXbp1u_cytoplasm)^2))
	ATF6() = ATF6T_ERlumen - BiATF_ERlumen
	PERK() = -BiPER_ERlumen - PERKA_ERlumen*n + PERKT
	IRE1() = -BiRE1_ERlumen - IRE1A_ERlumen*n + IRE1T
	BiP() = -BiATF_ERlumen - BiPER_ERlumen + BiPT_ERlumen - BiRE1_ERlumen - BiUFP_ERlumen
	UFP() = -BiUFP_ERlumen + UFPT_ERlumen
	fRate0 =  ktrUFP*mUFPT*if((extATT==1),(eIF2a/eIF2aT),(1))
	fRate2() = BiUFP_ERlumen*kbu
	fRate3() = BiP*UFP*kf*tmr
	fRate4() = kr*tmr
	fRate5() = BiP*IRE1*kf*tmr
	fRate6() = kr*tmr
	fRate7() = ATF6*BiP*kf*tmr
	fRate8() = kr*tmr
	fRate9() = BiP*PERK*kf*tmr
	fRate10() = kr*tmr
	fRate11() = IRE1^n*kff*tmr
	fRate12() = kr*tmr
	fRate13 =  PERK^n*kff*tmr*if((switch==1),(UFP),(1))
	fRate14() = kr*tmr/(UFP*extPERK + 1.0)
	fRate15() = ktrATF6*mATF6T
	fRate17() = ATF6*ktrans
	fRate18() = WFS1_ERlumen*kdeAW
	fRate22() = ATF6p50_cytoplasm*trcWFS/(ATF6p50_cytoplasm + krcWFS)
	fRate24() = ktrWFS*mWFS1_cytoplasm
	fRate26() = trcXU*(ATF6p50_cytoplasm*kmAtfsXBP + basalXBP)/(ATF6p50_cytoplasm*kmAtfsXBP + basalXBP + krcXU)
	fRate28() = spliceRate
	fRate30() = ktrXS*mXbp1s_cytoplasm
	fRate32() = trcBiP*(ATF6p50_cytoplasm*kmAtfsBiP + Xbp1s_cytoplasm*kmXbp + basalBiP)/(ATF6p50_cytoplasm*kmAtfsBiP + Xbp1s_cytoplasm*kmXbp + basalBiP + krcBiP)
	fRate34() = ktrBiP*mBiPT_cytoplasm
	fRate36() = ktrATF4*mATF4/((eIF2a/kATF4)^nh + 1.0)
	fRate38() = trcCHOP*(ATF4_cytoplasm*kmAtff + ATF6p50_cytoplasm*kmAtfs)/(ATF4_cytoplasm*kmAtff + ATF6p50_cytoplasm*kmAtfs + krcCHOP)
	fRate40() = ktrCHOP*mCHOP_cytoplasm
	fRate42() = CHOP_cytoplasm*kmChop*trcGADD34/(CHOP_cytoplasm*kmChop + krcGADD34)
	fRate44() = ktrGADD34*mGADD34_cytoplasm
	fRate46() = kfbc/(CHOP_cytoplasm*kmbc + 1.0)
	fRate48() = BAXT*kfx
	fRate49() = BAXT*BH3*kfxp
	fRate51() = BH3*kfxp
	fRate54() = CHOP_cytoplasm*ks3p*kstr
	fRate56() = BAXm*BCL2*kasx
	fRate59() = BCL2*BH3*kas3
end functions
begin reaction rules
	re2: 0  -> UFPT()@ERlumen fRate0()
	re3: UFPT()@ERlumen -> 0  kdUFP #Modifiers(UFPT)
	re4: UFPT()@ERlumen -> 0  fRate2() #Modifiers(BiUFP, BiUFP, UFPT)
	reu1: 0  -> BiUFP()@ERlumen fRate3()
	reu2: BiUFP()@ERlumen -> 0  fRate4() #Modifiers(BiUFP)
	reu3: 0  -> BiRE1()@ERlumen fRate5()
	reu4: BiRE1()@ERlumen -> 0  fRate6() #Modifiers(BiRE1)
	reu5: 0  -> BiATF()@ERlumen fRate7()
	reu6: BiATF()@ERlumen -> 0  fRate8() #Modifiers(BiATF)
	reu7: 0  -> BiPER()@ERlumen fRate9()
	reu8: BiPER()@ERlumen -> 0  fRate10() #Modifiers(BiPER)
	reu9: 0  -> IRE1A()@ERlumen fRate11()
	reu10: IRE1A()@ERlumen -> 0  fRate12() #Modifiers(IRE1A)
	reu11: 0  -> PERKA()@ERlumen fRate13()
