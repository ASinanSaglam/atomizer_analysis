###
begin model
begin parameters
	Kp 4.0
	Kd 10.0
	Kr 200.0
	n 4.0
	Kg 25.0
	m 2.0
	Ks 25.0
	Kc1 1000.0
	Kc2 2000.0
	w 3.0
	Ker 75.0
	Cplc_total 10.0
	k0 0.1
	k1 3.4
	k2 4.0
	k3 4.5
	k4 1.2
	k5 0.12
	k6 14.0
	k7 2.0
	k8 10500.0
	k9 600.0
	k10 3000.0
	k11 260.0
	PLC_Cytosol -5.41979999999999e+15 #original 1.0nano mole/L
	PLC -5.41979999999999e+15 #original 1.0nano mole/L
	Diacylglycerol_Cytosol 602200000000000.1 #original 1.0nano mole/L
	Diacylglycerol 602200000000000.1 #original 1.0nano mole/L
end parameters
begin compartments
	#volume units: L
	Cytosol 3 1.0
	ER 3 1.0
end compartments
begin molecule types
	Galpha_GTP()
	Calcium()
	APLC()
	IP3()
end molecule types
begin seed species
	@Cytosol:Galpha_GTP() 602200000000000.1 # (1.0 * 1e-09)mol/L * 6.022e23/mol *1.0L #Galpha_GTP #Galpha_GTP
	@Cytosol:APLC() 5419800000000001.0 #original 9.0nano mole/L #APLC #APLC
	@Cytosol:IP3() 602200000000000.1 #original 1.0nano mole/L #IP3 #IP3
	@ER:Calcium() 6.022000000000001e+17 #original 1000.0nano mole/L #Calcium #Ca_ER
	@Cytosol:Calcium() 1.2044000000000002e+17 #original 200.0nano mole/L #Calcium #Ca_Cyt
end seed species
begin observables
	Species Galpha_GTP_Cytosol @Cytosol:Galpha_GTP() #Galpha_GTP
	Species APLC_Cytosol @Cytosol:APLC() #APLC
	Species IP3_Cytosol @Cytosol:IP3() #IP3
	Species Calcium_ER @ER:Calcium() #Calcium
	Species Calcium_Cytosol @Cytosol:Calcium() #Calcium
end observables
begin functions
	Diacylglycerol_ar() = IP3_Cytosol
	Rdg() = Diacylglycerol_ar^m/(Diacylglycerol_ar^m + Kd^m)
	Rpkc() = Calcium_Cytosol*Diacylglycerol_ar/((Calcium_Cytosol + Kr)*(Diacylglycerol_ar + Kd))
	PLC_ar() = -APLC_Cytosol + Cplc_total
	Rer() = Calcium_Cytosol^w/(Calcium_Cytosol^w + Ker^w)
	Rcyt2() = Calcium_Cytosol/(Calcium_Cytosol + Kc2)
	Rcyt1() = Calcium_Cytosol/(Calcium_Cytosol + Kc1)
	Rip3() = IP3_Cytosol^3/(IP3_Cytosol^3 + Ks^3)
	Rgalpha_gtp() = Galpha_GTP_Cytosol^n/(Galpha_GTP_Cytosol^n + Kg^n)
	Raplc() = APLC_Cytosol/(APLC_Cytosol + Kp)
	fRate1() = Galpha_GTP_Cytosol*k1
	fRate2() = Raplc*k2
	fRate3() = Rpkc*k3
	fRate4() = PLC_Cytosol*Rdg*Rgalpha_gtp*k4
	fRate6() = APLC_Cytosol*k6
	fRate8() = -Rcyt1*k9 + Rer*Rip3*k8
	fRate9() = Rcyt2*k10
	fRate10() = k11
end functions
begin reaction rules
	R1: 0  -> Galpha_GTP()@Cytosol k0
	R2: 0  -> Galpha_GTP()@Cytosol fRate1()
	R3: Galpha_GTP()@Cytosol -> 0  fRate2()
	R4: Galpha_GTP()@Cytosol -> 0  fRate3()
	R5: 0  -> APLC()@Cytosol fRate4() #Modifiers(PLC)
	R6: APLC()@Cytosol -> 0  k5
	R7: 0  -> IP3()@Cytosol fRate6() #Modifiers(APLC)
	R8: IP3()@Cytosol -> 0  k7
	R9_reactants_0: 0  -> Calcium()@ER -1*0.001*(fRate8())
	R9_products_0: 0  -> Calcium()@Cytosol 0.01*fRate8()
	R10_reactants_0: 0  -> Calcium()@Cytosol -1*0.05*(fRate9())
	R11_products_0: 0  -> Calcium()@Cytosol 0.05*fRate10()
end reaction rules
end model
