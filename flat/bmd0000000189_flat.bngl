#@notes:'This is a plain translation of an SBML model created on 13/08/2021. The original model has 18 molecules and 14 reactions. The translated model has 14 molecules and 14 rules'
###
begin model
begin parameters
	ksynMdm2 0.000495 #units:psec
	kdegMdm2 0.000433 #units:psec
	ksynp53 0.078 #units:psec
	kdegp53 0.000825 #units:psec
	kbinMdm2p53 0.001155 #units:pmolepsec
	krelMdm2p53 1.155e-05 #units:psec
	ksynMdm2mRNA 0.0001 #units:psec
	kdegMdm2mRNA 0.0001 #units:psec
	kbinARFMdm2 0.01 #units:pmolepsec
	kdegARFMdm2 0.001 #units:psec
	kdegARF 0.0001 #units:psec
	kactARF 3.3e-05 #units:psec
	kdam 0.08 #units:molepsecpdGy
	krepair 2e-05 #units:psec
	kproteff 1.0 #units:dimensionless
	IR 0
	Sink 1.0
	Source 1.0
end parameters
begin molecule types
	ARF_Mdm2()
	totdamDNA()
	Mdm2()
	damDNA()
	Mdm2_p53()
	p53deg()
	mdm2syn()
	mdm2deg()
	ARF()
	Mdm2mRNAdeg()
	Mdm2mRNAsyn()
	p53()
	Mdm2_mRNA()
	p53syn()
end molecule types
begin seed species
	Mdm2() 5.0 #Mdm2 #Mdm2
	p53() 5.0 #p53 #p53
	Mdm2_p53() 95.0 #Mdm2_p53 #Mdm2_p53
end seed species
begin observables
	Species Mdm2 Mdm2() #Mdm2
	Species p53 p53() #p53
	Species Mdm2_p53 Mdm2_p53() #Mdm2_p53
	Species Mdm2_mRNA Mdm2_mRNA() #Mdm2_mRNA
	Species ARF ARF() #ARF
	Species ARF_Mdm2 ARF_Mdm2() #ARF_Mdm2
	Species damDNA damDNA() #damDNA
	Species p53deg p53deg() #p53deg
	Species p53syn p53syn() #p53syn
	Species mdm2deg mdm2deg() #mdm2deg
	Species mdm2syn mdm2syn() #mdm2syn
	Species Mdm2mRNAdeg Mdm2mRNAdeg() #Mdm2mRNAdeg
	Species Mdm2mRNAsyn Mdm2mRNAsyn() #Mdm2mRNAsyn
	Species totdamDNA totdamDNA() #totdamDNA
end observables
begin functions
	totMdm2_ar() = ARF_Mdm2 + Mdm2 + Mdm2_p53
	totp53_ar() = Mdm2_p53 + p53
	fRate3() = kdegMdm2*kproteff
	fRate4() = Source*ksynp53
	fRate5() = kdegp53*kproteff
	fRate8() = IR*kdam
	fRate12() = kdegARFMdm2*kproteff
	fRate13() = kdegARF*kproteff
end functions
begin reaction rules
	Mdm2Synthesis: Mdm2_mRNA() -> Mdm2_mRNA() + Mdm2() + mdm2syn() ksynMdm2
	Mdm2mRNASynthesis: p53() -> p53() + Mdm2_mRNA() + Mdm2mRNAsyn() ksynMdm2mRNA
	Mdm2mRNADegradation: Mdm2_mRNA() -> Mdm2mRNAdeg() kdegMdm2mRNA
	Mdm2Degradation: Mdm2() -> mdm2deg() fRate3()
	p53Synthesis: 0  -> p53() + p53syn() fRate4()
	p53Degradation: Mdm2_p53() -> Mdm2() + p53deg() fRate5()
	P53_Mdm2Binding: p53() + Mdm2() -> Mdm2_p53() kbinMdm2p53
	P53_Mdm2Release: Mdm2_p53() -> p53() + Mdm2() krelMdm2p53
	DNAdamage: 0  -> damDNA() + totdamDNA() fRate8()
	DNArepair: damDNA() -> 0  krepair
	ARFactivation: damDNA() -> damDNA() + ARF() kactARF
	ARF_Mdm2Binding: ARF() + Mdm2() -> ARF_Mdm2() kbinARFMdm2
	ARF_Mdm2Degradation: ARF_Mdm2() -> ARF() + mdm2deg() fRate12()
	ARFDegradation: ARF() -> 0  fRate13()
end reaction rules
end model
