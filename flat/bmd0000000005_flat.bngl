#@notes:'This is a plain translation of an SBML model created on 14/08/2021. The original model has 9 molecules and 9 reactions. The translated model has 6 molecules and 10 rules'
###
begin model
begin parameters
	r1_k6 1.000000
	r2_k8notP 1000000.000000
	r3_k9 1000.000000
	r4_k3 200.000000
	r5_k5notP 0.000000
	r6_k1aa 0.015000
	r7_k2 0.000000
	r8_k7 0.600000
	r9_k4 180.000000
	r9_k4prime 0.018000
end parameters
begin molecule types
	p_cyclin()
	cdc2k()
	p_cyclin_cdc2()
	cyclin()
	cdc2k_P()
	p_cyclin_cdc2_p()
end molecule types
begin seed species
	cdc2k_P() 0.75 #cdc2k_P #CP
	p_cyclin_cdc2_p() 0.25 #p_cyclin_cdc2_p #pM
end seed species
begin observables
	Species cdc2k cdc2k() #cdc2k
	Species cdc2k_P cdc2k_P() #cdc2k-P
	Species p_cyclin_cdc2 p_cyclin_cdc2() #p-cyclin_cdc2
	Species p_cyclin_cdc2_p p_cyclin_cdc2_p() #p-cyclin_cdc2-p
	Species cyclin cyclin() #cyclin
	Species p_cyclin p_cyclin() #p-cyclin
end observables
begin functions
	total_cdc2_ar() = cdc2k + cdc2k_P + p_cyclin_cdc2 + p_cyclin_cdc2_p
	total_cyclin_ar() = cyclin + p_cyclin + p_cyclin_cdc2 + p_cyclin_cdc2_p
	fRate8() = p_cyclin_cdc2_p*(180.0*p_cyclin_cdc2^2/total_cdc2_ar^2 + 0.018)
end functions
begin reaction rules
	Reaction1: p_cyclin_cdc2() -> cdc2k() + p_cyclin() r1_k6
	Reaction2: cdc2k() -> cdc2k_P() r2_k8notP
	Reaction3: cdc2k_P() -> cdc2k() r3_k9
	Reaction4: cdc2k_P() + cyclin() -> p_cyclin_cdc2_p() r4_k3
	Reaction5: p_cyclin_cdc2() -> p_cyclin_cdc2_p() r5_k5notP
	Reaction6: 0  -> cyclin() r6_k1aa
	Reaction7: cyclin() -> 0  r7_k2
	Reaction8: p_cyclin() -> 0  r8_k7
	Reaction9_reactants_0: 0  -> p_cyclin_cdc2_p() -1*(fRate8()) #Modifiers(CT)
	Reaction9_products_0: 0  -> p_cyclin_cdc2() fRate8() #Modifiers(CT)
end reaction rules
end model
