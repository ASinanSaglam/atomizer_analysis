begin model
begin parameters
	k1 0.3426
	k2 5.3
	k3 4.0
	k4 2.0
	k5 2.3
	GE 30.0
	b 0.1
end parameters
begin molecule types
	B()
	Gp()
	A()
	Gi()
end molecule types
begin seed species
	Gp() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #Gp #Gp
	Gi() 6.022e+20 #original 1.0substance/L  #Gi #Gi
	A() 6.022e+20 #original 1.0substance/L  #A #A
	B() 6.022e+31 #original 100000000000.0substance/L  #B #B
end seed species
begin observables
	Species Gp Gp() #Gp
	Species Gi Gi() #Gi
	Species A A() #A
	Species B B() #B
end observables
begin functions
	fRate0() = GE*Gp*k1
	fRate0m() = 1.0*A*k2
	fRate4() = A*B*Gp*b
end functions
begin reaction rules
	The_uptake_of_glutamate_and_Consumption_of_glutamate_and_ammonia: 0  <-> Gp() fRate0(),fRate0m() #Modifiers(A)
	Diffusion_of_glutamate: Gp() -> Gi() k4
	Consumption_of_glutamate_to_produce_ammonia: Gi() -> A() k5
	Diffusion_of_ammonia: A() -> 0  k3
	The_synthesis_of_biomass_from_ammonia_and_glutamate: 0  -> B() fRate4() #Modifiers(A, Gp)
end reaction rules
end model
