begin model
begin parameters
	k1 700.0 #units:peruMpersec
	k2 800.0 #units:persec
	k3 850.0 #units:persec
	k4 20.0 #units:persec
	k5 0.2 #units:persec
	k6 700.0 #units:peruMpersec
	k7 800.0 #units:persec
	k8 850.0 #units:persec
	k9 5.0 #units:peruMpersec
	k10 25.0 #units:persec
	k11 1.6 #units:persec
	k12 0.02 #units:persec
	e5c 0.11 #units:peruMpercm
	e5c_NO 0.025 #units:peruMpercm
	e6c_NO 0.064 #units:peruMpercm
end parameters
begin molecule types
	NO_sGCfast()
	sGCfast()
	NO_sGCfast_6coord()
	sGCslow()
	NO_sGCslow_6coord_NO_int()
	NO()
	NO_sGCslow()
	NO_sGCslow_6coord()
	NO_sGCslow_5coord()
	NO_sGCfast_5coord()
end molecule types
begin seed species
	NO() 3.011e+17 # (0.5 * 1e-06)mol/L * 6.022e23/mol *1.0L #NO #NO
	sGCfast() 6.74464e+16 #original 0.112micromole/L #sGCfast #sGCfast
	sGCslow() 1.734336e+17 #original 0.288micromole/L #sGCslow #sGCslow
end seed species
begin observables
	Species NO NO() #NO
	Species sGCfast sGCfast() #sGCfast
	Species NO_sGCfast NO_sGCfast() #NO_sGCfast
	Species NO_sGCfast_6coord NO_sGCfast_6coord() #NO_sGCfast_6coord
	Species NO_sGCfast_5coord NO_sGCfast_5coord() #NO_sGCfast_5coord
	Species sGCslow sGCslow() #sGCslow
	Species NO_sGCslow NO_sGCslow() #NO_sGCslow
	Species NO_sGCslow_6coord NO_sGCslow_6coord() #NO_sGCslow_6coord
	Species NO_sGCslow_6coord_NO_int NO_sGCslow_6coord_NO_int() #NO_sGCslow_6coord_NO_int
	Species NO_sGCslow_5coord NO_sGCslow_5coord() #NO_sGCslow_5coord
end observables
begin functions
	ext() = e5c*(NO_sGCfast + NO_sGCslow + sGCfast + sGCslow) + e5c_NO*(NO_sGCfast_5coord + NO_sGCslow_5coord) + e6c_NO*(NO_sGCfast_6coord + NO_sGCslow_6coord + NO_sGCslow_6coord_NO_int)
	sGC_inact_tot_ar() = NO_sGCfast + NO_sGCfast_6coord + NO_sGCslow + NO_sGCslow_6coord + NO_sGCslow_6coord_NO_int + sGCfast + sGCslow
	NO_sGC_5coord_tot_ar() = NO_sGCfast_5coord + NO_sGCslow_5coord
end functions
begin reaction rules
	r1fast: NO() + sGCfast() <-> NO_sGCfast() k1,k2
	r2fast: NO_sGCfast() -> NO_sGCfast_6coord() k3
	r3fast: NO_sGCfast_6coord() <-> NO_sGCfast_5coord() k4,k5
	r1slow: NO() + sGCslow() <-> NO_sGCslow() k6,k7
	r2slow: NO_sGCslow() -> NO_sGCslow_6coord() k8
	r3slow: NO() + NO_sGCslow_6coord() <-> NO_sGCslow_6coord_NO_int() k9,k10
	r4slow: NO_sGCslow_6coord_NO_int() <-> NO_sGCslow_5coord() k11,k12
end reaction rules
end model
