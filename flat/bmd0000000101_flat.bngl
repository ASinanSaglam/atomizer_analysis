begin model
begin parameters
	ka 1.0
	ligand 3e-05
	kcd 0.0277777778
	klid 0.25
	ki 0.3333333333333
	pRI 8.0
	kr 0.0333333333333333
	alpha 1.0
	pRII 4.0
end parameters
begin compartments
	#volume units: L
	PM 3 1.0 #Plasma membrane
	Endosome 3 1.0
end compartments
begin molecule types
	ligandreceptorcomplex_plasmamembrane()
	Receptor2endosome()
	ligandreceptorcomplex_endosome()
	Receptor2()
	Receptor1()
	Receptor1_endosome()
end molecule types
begin seed species
	@PM:Receptor1() 20.0 #Receptor1 #RI
	@PM:Receptor2() 20.0 #Receptor2 #RII
	@Endosome:ligandreceptorcomplex_endosome() 40.0 #ligandreceptorcomplex_endosome #lRIRII_endo
end seed species
begin observables
	Species Receptor1_PM @PM:Receptor1() #Receptor 1
	Species Receptor2_PM @PM:Receptor2() #Receptor 2
	Species ligandreceptorcomplex_plasmamembrane_PM @PM:ligandreceptorcomplex_plasmamembrane() #ligand receptor complex-plasma membrane
	Species ligandreceptorcomplex_endosome_Endosome @Endosome:ligandreceptorcomplex_endosome() #ligand receptor complex-endosome
	Species Receptor1_endosome_Endosome @Endosome:Receptor1_endosome() #Receptor 1-endosome
	Species Receptor2endosome_Endosome @Endosome:Receptor2endosome() #Receptor 2 endosome
end observables
begin functions
	fRate0() = ka*ligand
end functions
begin reaction rules
	v1: Receptor2()@PM + Receptor1()@PM -> ligandreceptorcomplex_plasmamembrane()@PM fRate0()
	v2: ligandreceptorcomplex_plasmamembrane()@PM -> 0  kcd
	v3: ligandreceptorcomplex_plasmamembrane()@PM -> 0  klid
	v4: ligandreceptorcomplex_plasmamembrane()@PM -> ligandreceptorcomplex_endosome()@Endosome ki
	v5: 0  -> Receptor1()@PM pRI
	v6: Receptor1()@PM -> 0  kcd
	v7: Receptor1()@PM -> Receptor1_endosome()@Endosome ki
	v8: Receptor1_endosome()@Endosome -> Receptor1()@PM kr
	v9: ligandreceptorcomplex_endosome()@Endosome -> Receptor1()@PM + Receptor2()@PM kr
	v10: 0  -> Receptor2()@PM pRII
	v11: Receptor2()@PM -> 0  kcd
	v12: Receptor2()@PM -> Receptor2endosome()@Endosome ki
	v13: Receptor2endosome()@Endosome -> Receptor2()@PM kr
end reaction rules
end model
