###
begin model
begin parameters
	alpha1 3.0
	alpha2 4.0
	beta1 0.2
	beta2 0.02
	g11 0.5
	g21 -0.5
	g12 1.0
	k1 0.24
	k2 0.0017
	g22 0
	flag_resorption 0
	flag_formation 0
end parameters
begin molecule types
	Bonemass()
	Osteoblast()
	Osteoclast()
end molecule types
begin seed species
	Osteoclast() 11.0 #Osteoclast #x1
	Osteoblast() 212.0 #Osteoblast #x2
	Bonemass() 100.0 #Bonemass #z
end seed species
begin observables
	Species Osteoclast Osteoclast() #Osteoclast
	Species Osteoblast Osteoblast() #Osteoblast
	Species Bonemass Bonemass() #Bone mass
end observables
begin functions
	gamma() = g12*g21 - (1 - g11)*(1 - g22)
	Steadystateosteoblast_ar() = (beta1/alpha1)^(g12/gamma)*(beta2/alpha2)^((1 - g11)/gamma)
	Steadystateosteoclast_ar() = (beta1/alpha1)^((1 - g22)/gamma)*(beta2/alpha2)^(g21/gamma)
	fRate0() = Osteoblast^g21*Osteoclast^g11*alpha1
	fRate2() = Osteoblast^g22*Osteoclast^g12*alpha2
	fRate4() = flag_resorption*k1*(Osteoclast - Steadystateosteoclast_ar)
	fRate5() = flag_formation*k2*(Osteoblast - Steadystateosteoblast_ar)
end functions
begin reaction rules
	R1: 0  -> Osteoclast() fRate0() #Modifiers(x2)
	R2: Osteoclast() -> 0  beta1
	R3: 0  -> Osteoblast() fRate2() #Modifiers(x1)
	R4: Osteoblast() -> 0  beta2
	R5_reactants_0: 0  -> Bonemass() -1*(fRate4()) #Modifiers(x1, x1_bar)
	R6_products_0: 0  -> Bonemass() fRate5() #Modifiers(x2, x2_bar)
end reaction rules
end model
