#@notes:'This is a plain translation of an SBML model created on 14/08/2021. The original model has 7 molecules and 9 reactions. The translated model has 7 molecules and 15 rules'
###
begin model
begin parameters
	V_max_MATI 561.0
	K_m_MATI 41.0
	K_i_MATI 50.0
	V_max_MATIII 22870.0
	K_m2_MATIII 21.1
	V_max_GNMT 10600.0
	K_m_GNMT 4500.0
	K_i_GNMT 20.0
	V_max_METH 4521.0
	K_m2_METH_A 10.0
	alpha_1 100.0
	alpha_2 10.0
	beta_1 1.7
	beta_2 30.0
	V_max_MS 500.0
	K_m_Hcy_MS 0.1
	K_m_5mTHF_MS 25.0
	K_d_MS 1.0
	V_max_BHMT 2500.0
	K_m_BHMT 12.0
	r9_k1 1.000000
end parameters
begin molecule types
	Metin()
	AdoMet()
	Methionine()
	Cystathionine()
	AdoHcy()
	Homocysteine()
	s5mTHF()
end molecule types
begin seed species
	Metin() 0 # (200.0 * 1e-06)mol/L * 6.022e23/mol *1.0L #Metin #Metin
	Methionine() 3.22177e+19 #original 53.5substance/volume #Methionine #Methionine
	AdoMet() 8.286272e+19 #original 137.6substance/volume #AdoMet #AdoMet
	AdoHcy() 7.94904e+18 #original 13.2substance/volume #AdoHcy #AdoHcy
	Homocysteine() 5.29936e+17 #original 0.88substance/volume #Homocysteine #Homocysteine
	$s5mTHF() 3.13144e+18 #original 5.2substance/volume #s5mTHF #_5mTHF
end seed species
begin observables
	Species Metin Metin() #Metin
	Species Methionine Methionine() #Methionine
	Species AdoMet AdoMet() #AdoMet
	Species AdoHcy AdoHcy() #AdoHcy
	Species Homocysteine Homocysteine() #Homocysteine
	Species s5mTHF s5mTHF() #5mTHF
	Species Cystathionine Cystathionine() #Cystathionine
end observables
begin functions
	K_m1_METH() = AdoHcy/4 + 1
	K_m1_MATIII() = 20000/(5.7*AdoMet^2/(AdoMet + 600)^2 + 1)
	Metin_ar() =  if((time()<2||time()>=8),(200),(if((time()>=2&&time()<5),(300),(if((time()>=5&&time()<8),(100),(200))))))
	fRate0() = V_max_MATI/(K_m_MATI*(AdoMet/K_i_MATI + 1)/Methionine + 1)
	fRate1() = V_max_MATIII/(K_m1_MATIII*K_m2_MATIII/(K_m2_MATIII*Methionine + Methionine^2) + 1)
	fRate2() = V_max_METH/(K_m2_METH_A + 1 + K_m1_METH*K_m2_METH_A/AdoMet + K_m1_METH/AdoMet)
	fRate3() = V_max_GNMT/((AdoHcy/K_i_GNMT + 1)*((K_m_GNMT/AdoMet)^2.3 + 1))
	fRate4m() = alpha_1*alpha_2
	fRate5() = V_max_MS/(Homocysteine*K_m_5mTHF_MS + Homocysteine*s5mTHF + K_d_MS*K_m_Hcy_MS + K_m_Hcy_MS*s5mTHF)
	fRate6() = Homocysteine*V_max_BHMT*(-0.025*AdoHcy - 0.025*AdoMet + 4.45)/(Homocysteine + K_m_BHMT)
	fRate7() = Homocysteine*(beta_1*(AdoHcy + AdoMet) - beta_2)
end functions
begin reaction rules
	V_MAT_I_reactants_0: 0  -> Methionine() -1*(fRate0())
	V_MAT_I_products_0: 0  -> AdoMet() fRate0()
	V_MAT_III_reactants_0: 0  -> Methionine() -1*(fRate1())
	V_MAT_III_products_0: 0  -> AdoMet() fRate1()
	V_METH_reactants_0: 0  -> AdoMet() -1*(fRate2())
	V_METH_products_0: 0  -> AdoHcy() fRate2()
	V_GNMT_reactants_0: 0  -> AdoMet() -1*(fRate3())
	V_GNMT_products_0: 0  -> AdoHcy() fRate3()
	V_AH: AdoHcy() <-> Homocysteine() alpha_1,fRate4m()
	V_MS: Homocysteine() + s5mTHF() -> Methionine() fRate5()
	V_BHMT_reactants_0: 0  -> Homocysteine() -1*(fRate6()) #Modifiers(AdoMet, AdoHcy)
	V_BHMT_products_0: 0  -> Methionine() fRate6() #Modifiers(AdoMet, AdoHcy)
	V_CBS_reactants_0: 0  -> Homocysteine() -1*(fRate7()) #Modifiers(AdoMet, AdoHcy)
	V_CBS_products_0: 0  -> Cystathionine() fRate7() #Modifiers(AdoMet, AdoHcy)
	METIN: Metin() -> Methionine() r9_k1
end reaction rules
end model
