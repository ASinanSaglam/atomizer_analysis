###
begin model
begin parameters
	kbd 0.0164812
	kbs 100.0
	kxd 1.08559e-09
	kxs 9.32517e-08
	kzd 1.34615e-07
	kzs 9.03538e-07
	kzx 0.00317772
end parameters
begin molecule types
	x()
	sigb()
	IPTG()
	lacz()
end molecule types
begin seed species
	$IPTG() 6.022e+25 # 100.0mol/L * 6.022e23/mol *1.0L #IPTG #IPTG
end seed species
begin observables
	Species IPTG IPTG() #IPTG
	Species sigb sigb() #sigb
	Species lacz lacz() #lacz
	Species x x() #x
end observables
begin functions
	fRate1() = kzs*sigb - lacz*(kzd + kzx*x)
end functions
begin reaction rules
	v1: IPTG() <-> sigb() kbs,kbd #Modifiers(IPTG, sigb)
	v2_reactants_0: 0  -> sigb() -1*(fRate1()) #Modifiers(x, sigb, lacz, x)
	v2_products_0: 0  -> lacz() fRate1() #Modifiers(x, sigb, lacz, x)
	v3: sigb() <-> x() kxs,kxd #Modifiers(sigb, x)
end reaction rules
end model
