	VakI_reactants_0: 0  -> Aspartate() -1*(fRate2()) #Modifiers(AKHSDHI, Thr)
	VakI_products_0: 0  -> Aspartyl_P() fRate2() #Modifiers(AKHSDHI, Thr)
	VakII_reactants_0: 0  -> Aspartate() -1*(fRate3()) #Modifiers(AKHSDHII, Thr)
	VakII_products_0: 0  -> Aspartyl_P() fRate3() #Modifiers(AKHSDHII, Thr)
	Vasadh: Aspartyl_P() <-> Aspartatesemialdehyde() fRate4(),fRate4m() #Modifiers(ASADH)
	Vdhdps1: Aspartatesemialdehyde() -> Lysine() fRate5() #Modifiers(DHDPS1, Lys)
	Vdhdps2: Aspartatesemialdehyde() -> Lysine() fRate6() #Modifiers(DHDPS2, Lys)
