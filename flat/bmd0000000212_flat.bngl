	Vhsdh2_reactants_0: 0  -> Aspartatesemialdehyde() -1*(fRate8()) #Modifiers(AKHSDHII, Thr)
	Vhsdh2_products_0: 0  -> Homoserine() fRate8() #Modifiers(AKHSDHII, Thr)
	Vhsk: Homoserine() -> Phosphohomoserine() fRate9() #Modifiers(HSK)
	Vts1_reactants_0: 0  -> Phosphohomoserine() -1*(fRate10()) #Modifiers(TS1, Phosphate, AdoMet)
	Vts1_products_0: 0  -> Threonine() fRate10() #Modifiers(TS1, Phosphate, AdoMet)
	Vcgs: Phosphohomoserine() -> Cystathione() fRate11() #Modifiers(CGS, Cys, Phosphate)
	Vtd: Threonine() -> Isoleucine() fRate12() #Modifiers(TD, Val, Ile)
