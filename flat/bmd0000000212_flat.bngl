	fRate1() = AK2*(3.15 - 0.86*Aspartyl_P)/(0.0333683745042907*Lysine^1.1 + 1)
	fRate2() = AKI_HSDHI*(0.36 - 0.15*Aspartyl_P)/(6.50364203954214e-5*Threonine^2.0 + 1)
	fRate3() = AKII_HSDHII*(1.35 - 0.22*Aspartyl_P)/(8.4167999326656e-5*Threonine^2.0 + 1)
	fRate4() = 0.9*ASADH
	fRate4m() = 0.23*ASADH
	fRate5() = 1.0*DHDPS1/(0.01*Lysine^2.0 + 1.0)
	fRate6() = 1.0*DHDPS2/(0.000918273645546373*Lysine^2.0 + 1.0)
	fRate7() = 0.84*AKI_HSDHI*Aspartatesemialdehyde*(0.14 + 0.86/(0.0025*Threonine + 1))
	fRate8() = 0.64*AKII_HSDHII*Aspartatesemialdehyde*(0.25 + 0.75/(0.000117647058823529*Threonine + 1))
	fRate9() = 2.8*HSK/(Homoserine + 14.0)
	fRate10() = Phosphohomoserine*TS1*(0.0479452054794521*S_adenosyl_methionine^2.0 + 0.42)/((Phosphohomoserine + (0.001*Phosphate + 1)*(500.0*S_adenosyl_methionine + 250.0)/((0.91743119266055*S_adenosyl_methionine + 1)*(0.00714285714285714*S_adenosyl_methionine^2.0 + 1)))*(0.0136986301369863*S_adenosyl_methionine^2.0 + 1))
	fRate11() = 60000.0*CGS*Cysteine/(2500.0*Cysteine*Phosphate + 2000.0*Cysteine*Phosphohomoserine + 5000000.0*Cysteine + 920000.0*Phosphohomoserine)
	fRate12() = 0.0124*TD/((Isoleucine*(Valine + 615.0)/(103.0*Valine + 18450.0))^3.0 + 1.0)
	fRate13() = V_Lys_RS/(Lysine + 25.0)
	fRate14() = V_Thr_RS/(Threonine + 100.0)
	fRate15() = V_Ile_RS/(Isoleucine + 20.0)
	fRate16() = 3.1*LKR/(Lysine + 13000.0)
	fRate17() = 1.7*THA/(Threonine + 7100.0)
end functions
