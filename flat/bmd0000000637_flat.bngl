begin model
begin parameters
	K_d_L_R 5.6
	lambda 1.0
	lambda_t 1.0
	lambda_d 1.0
	K_d_R_G 36.0
	eta 1.0
	rho 1.0
	k_Hf_Gt 0.002
	k_Ef_RG 0.00062
	k_Ef_LRG 1.5
	k_Hf_RGt 0.11
	Rtot 3308.0
	Gtot 2042.0
	k_off_L_R 0.001
	k_off_R_G 0.1
