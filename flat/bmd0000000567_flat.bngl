begin model
begin parameters
	k1 4e-05
	k2 1.57e-06
	k2A0 0.29
end parameters
begin molecule types
	A()
	B()
end molecule types
begin seed species
	$A() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #A #A
end seed species
begin observables
	Species B B() #B
	Species A A() #A
end observables
begin functions
	A0() = k2A0/k2
	B_ar() = A0 - (A0 + k1/k2)/(1 + k1*exp((A0*k2 + k1)*time())/(A0*k2))
end functions
begin reaction rules
	Nucleation: A() -> B() k1 #Modifiers(A)
	Growth: A() + B() -> B() + B() k2 #Modifiers(A, B)
end reaction rules
end model
