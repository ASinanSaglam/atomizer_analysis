begin model
begin parameters
	r1_alpha 0.001000
	r2_kd 1.000000
	r3_kappa 0.500000
	r4_phi 0.000005
	r4_delta 0.000010
	r4_gamma 0.000010
end parameters
begin molecule types
	ribosomeconcentration()
end molecule types
begin seed species
	ribosomeconcentration() 1.0 #ribosomeconcentration #c
end seed species
begin observables
	Species ribosomeconcentration ribosomeconcentration() #ribosome concentration
end observables
begin functions
	fRate2() = ribosomeconcentration/(ribosomeconcentration + 0.5)
	fRate3() = 5.0e-6/(1.0e-5*ribosomeconcentration + 1.0e-5)
end functions
begin reaction rules
	reaction_1: 0  -> ribosomeconcentration() r1_alpha
	reaction_3: ribosomeconcentration() -> 0  r2_kd #Modifiers(c)
	reaction_2: 0  -> ribosomeconcentration() fRate2() #Modifiers(c)
	reaction_4: ribosomeconcentration() -> 0  fRate3() #Modifiers(c)
end reaction rules
end model
