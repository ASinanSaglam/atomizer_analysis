begin model
begin parameters
	alpha_C 1.0
	delta_C 0.01
	alpha_F 1.0
	delta_F 0.1
	alpha_M 1.0
	delta_M 1.0
	k_P 10.0
	delta_P 0.1
	k_CM 10.0
	k_MF 0.1
	beta_FM 200.0
	gamma_FM 100.0
	n 8.0
	tau1 0.5
	tau2 0.5
end parameters
begin molecule types
	M()
	F()
	C()
	P()
end molecule types
begin seed species
end seed species
begin observables
	Species C C() #C
	Species M M() #M
	Species F F() #F
	Species P P() #P
end observables
begin functions
	fRate8() = beta_FM/((gamma_FM/delay(P, tau1))^n + 1.0)
	fRate10() = k_P*delay(F, tau2)
end functions
begin reaction rules
	Sponge_Production_Basal: 0  -> C() alpha_C
	miRNA_Production_Basal: 0  -> M() alpha_M
	TF_mRNA_Production_Basal: 0  -> F() alpha_F
	Sponge_Degradation: C() -> 0  delta_C
	miRNA_Degradation: M() -> 0  delta_M
	TF_mRNA_Degradation: F() -> 0  delta_F
	Sponge_Inhibition_miRNA: M() + C() -> 0  k_CM
	TF_mRNA_Inhibition_miRNA: M() + F() -> 0  k_MF
	miRNA_Activation_Transcription_Factor: 0  -> M() fRate8() #Modifiers(P)
	TF_Protein_Degradation: P() -> 0  delta_P
	TF_Protein_Translation: 0  -> P() fRate10() #Modifiers(F)
end reaction rules
end model
