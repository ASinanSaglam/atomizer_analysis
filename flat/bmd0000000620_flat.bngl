	fRate6 =  if((time()<91),(il1bH*k1*(1-placebo_on)),(k2*(-il1b0*placebo_on+il1b0-kplacebo*placebo_on*time()+kplacebo*time())))
	fRate7 =  if((time()<91),(IL1b*k1*(1-placebo_on)),(IL1b*k2*(1-placebo_on)))
	fRate8() = kplacebo*placebo_on
	fRate10() = Anakinrasc*kab/Vp
	fRate11() = CL/Vp
	fRate14() = Insulin*Kxgi
	fRate15() = 0.1*Kxgi*Proinsulin
