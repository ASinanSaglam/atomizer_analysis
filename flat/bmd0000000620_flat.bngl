	arRatea1c5() =  Glucose^lambda*Kglucose*rbc5 + Ktr*a1c4 -Ktr*a1c5
	arRaterbc4() =  -Glucose^lambda*Kglucose*rbc4 + Ktr*rbc3 -Ktr*rbc4
	arRatea1c4() =  Glucose^lambda*Kglucose*rbc4 + Ktr*a1c3 -Ktr*a1c4
