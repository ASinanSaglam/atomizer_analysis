	apoptosis() = ka*(IL1R^xha*vha/(IL1R^xha + kmha^xha) - IL1R^xla*vla/(IL1R^xla + kmla^xla) + 1)
	arRaterbc12() =  -Glucose^lambda*Kglucose*rbc12 + Ktr*rbc11 -Ktr*rbc12
