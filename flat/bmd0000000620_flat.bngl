	arRaterbc1() =  -Glucose^lambda*Kglucose*rbc1 + Kin -Ktr*rbc1
	arRatea1c1() =  Glucose^lambda*Kglucose*rbc1 -Ktr*a1c1
	PI_I() = Proinsulin/Insulin
	hba1c_ar() = (100*a1c1 + 100*a1c10 + 100*a1c11 + 100*a1c12 + 100*a1c2 + 100*a1c3 + 100*a1c4 + 100*a1c5 + 100*a1c6 + 100*a1c7 + 100*a1c8 + 100*a1c9)/(a1c1 + a1c10 + a1c11 + a1c12 + a1c2 + a1c3 + a1c4 + a1c5 + a1c6 + a1c7 + a1c8 + a1c9 + rbc1 + rbc10 + rbc11 + rbc12 + rbc2 + rbc3 + rbc4 + rbc5 + rbc6 + rbc7 + rbc8 + rbc9)
