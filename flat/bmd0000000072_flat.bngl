begin model
begin parameters
	r1_k1 0.000000
	r1_k2 0.010000
	r2_v 4.000000
	r3_k1 0.000400
	r4_k1 1.000000
	r5_k1 0.000010
	r6_k1 0.004000
	r7_k1 0.110000
end parameters
begin molecule types
	G_alpha_GTP()
	FreelevelsofG_beta_gamma()
	Ligand()
	InactiveheterotrimericG_protein()
	Receptor_Ligand()
	G_alpha_GDP()
	Receptor()
end molecule types
begin seed species
	Ligand() 3.625244e+41 # 6.02e+17mol/L * 6.022e23/mol *1.0L #Ligand #L
	Receptor() 6.022e+27 #original 10000.0/L  #Receptor #R
	InactiveheterotrimericG_protein() 4.2154000000000003e+27 #original 7000.0/L  #InactiveheterotrimericG_protein #G
	FreelevelsofG_beta_gamma() 1.8066e+27 #original 3000.0/L  #FreelevelsofG_beta_gamma #Gbg
	G_alpha_GDP() 1.8066e+27 #original 3000.0/L  #G_alpha_GDP #Gd
end seed species
begin observables
	Species Ligand Ligand() #Ligand
	Species Receptor Receptor() #Receptor
	Species InactiveheterotrimericG_protein InactiveheterotrimericG_protein() #Inactive heterotrimeric G-protein
	Species FreelevelsofG_beta_gamma FreelevelsofG_beta_gamma() #Free levels of G-beta-gamma
	Species G_alpha_GDP G_alpha_GDP() #G-alpha-GDP
	Species G_alpha_GTP G_alpha_GTP() #G-alpha-GTP
	Species Receptor_Ligand Receptor_Ligand() #Receptor-Ligand
end observables
begin functions
	fRate4() = 1.0e-5*Receptor_Ligand
end functions
begin reaction rules
	reaction_0: Ligand() + Receptor() <-> Receptor_Ligand() r1_k1,r1_k2
	reaction_1: 0  -> Receptor() r2_v
	reaction_2: Receptor() -> 0  r3_k1
	reaction_3: G_alpha_GDP() + FreelevelsofG_beta_gamma() -> InactiveheterotrimericG_protein() r4_k1
	reaction_4: InactiveheterotrimericG_protein() -> G_alpha_GTP() + FreelevelsofG_beta_gamma() fRate4() #Modifiers(RL)
	reaction_5: Receptor_Ligand() -> 0  r6_k1
	reaction_6: G_alpha_GTP() -> G_alpha_GDP() r7_k1
end reaction rules
end model
