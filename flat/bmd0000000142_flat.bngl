###
begin model
begin parameters
	a 0.2
	b 0.26
	c -65.0
	Vthresh 30.0
	d 0
end parameters
begin compartments
	#volume units: L
	cell 3 1.0
end compartments
begin molecule types
	v
	u
end molecule types
begin seed species
	v@cell -64.0
	u@cell -16.64
end seed species
begin observables
	Species v v
	Species u u
end observables
begin functions
	i() =  if((time()<30),(0),(0.015*time()-0.95))
	arRatev() = i - u + 0.04*v^2 + 5*v + 140
	arRateu() = a*(b*v - u)
end functions
begin reaction rules
	0  -> v()@cell arRatev #rateLaw
	0  -> u()@cell arRateu #rateLaw
end reaction rules
end model
