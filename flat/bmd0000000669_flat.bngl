begin model
begin parameters
	IR50 123000.0
	Imax_3 0.753
	k3 0.222
	k1 0.357
	k_apo 0.00394
	k2 0.114
	Imax_g 0.878
	C_g 20.0
	C_b 500.0
	Hi_g 4.34
	Hs_b 1.24
	Hi_b 1.0
	Hs_g 3.0
	Tlag_sg 38.7
	Hother_g 0.1
	Smax_b 3.72
	Smax_g 2.74
	SC50_g 23.6
	SC50_b 50.1
	k_comb2 0.000775
	k_comb1 0.000919
	k_repair 0.0495
	k_delay 18.6
	R_0 236000.0
	f_G1__0 48.1
	f_S__0 10.8
	f_apo__0 5.0
	f_other__0 1.5
	f1 0.467
	Imax_b 0.177
	IC50_g 6.0
	IC50_b 154.0
	k_tau 0.376
	Kother_g 1e-05
	Kother_b 0.0055
	Hother_b 1.0
	Psi_i if((neq((500.0),0)&&neq((20.0),0)),(0.949),(1))
	Psi_s if((neq((500.0),0)&&neq((20.0),0)),(1.26),(1))
	f_G2__0 100 -(5.0) -(48.1) -(10.8) -(1.5)
	Sti_apo_g 0
	Sti_apo_b 0
	CURVE_FIGURE_7c_f_i 0
	R_total 0 #original 236000.0substance/volume
	R_live 0 #original 220660.0substance/volume
end parameters
begin molecule types
	S()
	R_apo()
	G1()
	G2()
	R_other()
end molecule types
begin seed species
	G1()() (48.1) * (236000.0) / 100 #G1
	S()() (10.8) * (236000.0) / 100 #S
	G2()() (34.6) * (236000.0) / 100 #G2
	R_apo()() (5.0) * (236000.0) / 100 #R_apo
	R_other()() (1.5) * (236000.0) / 100 #R_other
end seed species
begin observables
	Species G1 G1() #G1
	Species S S() #S
	Species G2 G2() #G2
	Species R_apo R_apo() #R_apo
	Species R_other R_other() #R_other
end observables
begin functions
	R_live_ar() = G1 + G2 + S
	Inh_3() = Imax_3*R_live_ar/(IR50 + R_live_ar)
	Inh_b() = C_b^Hi_b*Imax_b/(C_b^Hi_b + IC50_b^Hi_b)
	Sti_other_g() = C_g^Hother_g*Kother_g
	Inh_g() =  if((C_b==0&&time()<=Tlag_r),(C_g^Hi_g*Imax_g/(C_g^Hi_g+IC50_g^Hi_g)),(if((C_b==0&&time()>Tlag_r),(C_g^Hi_g*Imax_g*exp(-k_repair*(-Tlag_r+time()))/(C_g^Hi_g+IC50_g^Hi_g)),(if((nC_b==0&&time()<=Tlag_re),(C_g^Hi_g*Imax_g/(C_g^Hi_g+(IC50_g*Psi_i)^Hi_g)),(if((nC_b==0&&time()>Tlag_re),(C_g^Hi_g*Imax_g*exp(k_repair*(-Tlag_re+time())*(C_b*k_comb2-1))/(C_g^Hi_g+(IC50_g*Psi_i)^Hi_g)),(0))))))))
	Sti_other_b() = C_b^Hother_b*Kother_b
	k_other() =  if((neq(C_b,0)&&neq(C_g,0)),(0.000297),(0))
	arRateG2() = -G2*k3*(1 - Inh_3)*(1 - Inh_b) - G2*k_apo*(Sti_apo_b + 1)*(Sti_apo_g + 1) - G2*k_other*(Sti_other_b + 1)*(Sti_other_g + 1) + S*k2*(1 - Inh_g)
	Inh_1() = R_live_ar/(IR50 + R_live_ar)
	arRateS() = G1*k1*(1 - Inh_1)*(1 - Inh_b) - S*k2*(1 - Inh_g) - S*k_apo*(Sti_apo_b + 1)*(Sti_apo_g + 1) - S*k_other*(Sti_other_b + 1)*(Sti_other_g + 1)
	arRateG1() = -G1*k1*(1 - Inh_1)*(1 - Inh_b) - G1*k_apo*(Sti_apo_b + 1)*(Sti_apo_g + 1) - G1*k_other*(Sti_other_b + 1)*(Sti_other_g + 1) + G2*k3*(1 - Inh_b)*(2 - 2*Inh_3)
	arRateR_other() = -R_other*k_other + k_other*(Sti_other_b + 1)*(Sti_other_g + 1)*(G1 + G2 + S)
	R_total_ar() = G1 + G2 + R_apo + R_other + S
	CURVE_FIGURE_7a_d_g() = R_live_ar/R_total_ar
	CURVE_FIGURE_7b_r_h() = R_apo/R_total_ar
	CURVE_G2() = G2/R_total_ar
	CURVE_S() = S/R_total_ar
	CURVE_G1() = G1/R_total_ar
	Tlag_r() =  if((C_g==0),(0),(k_delay* ln(C_g)))
	Tlag_re() = Tlag_r*(C_b*k_comb1 + 1)
	arRateR_apo() = -R_apo*f1*k_apo*(Sti_apo_b + 1)*(Sti_apo_g + 1) + k_apo*(Sti_apo_b + 1)*(Sti_apo_g + 1)*(G1 + G2 + S)
	Sti_b() = C_b^Hs_b*Smax_b/(C_b^Hs_b + (Psi_s*SC50_b)^Hs_b)
	Sti_g() =  if((time()<=Tlag_sg),(0),(C_g^Hs_g*Smax_g/(C_g^Hs_g+SC50_g^Hs_g)))
end functions
begin reaction rules
	0  -> G1() arRateG1 #rateLaw
	0  -> S() arRateS #rateLaw
	0  -> G2() arRateG2 #rateLaw
	0  -> R_apo() arRateR_apo #rateLaw
	0  -> R_other() arRateR_other #rateLaw
end reaction rules
end model
