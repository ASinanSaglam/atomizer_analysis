	frq_transcription_reactants_0: 0  -> frq_gene()@c2 -1*(fRate1()) #Modifiers(active_hypoWCCn, L_WCC, active_hypoWCCn, L_WCC)
	frq_transcription_products_0: 0  -> frq_mRNA()@c1 fRate1() #Modifiers(active_hypoWCCn, L_WCC, active_hypoWCCn, L_WCC)
	wc1_transcription_reactants_0: 0  -> wc1_gene()@c2 -1*(fRate2()) #Modifiers(active_hypoWCCn, L_WCC, active_hypoWCCn, L_WCC)
	wc1_transcription_products_0: 0  -> wc1_mRNA()@c1 fRate2() #Modifiers(active_hypoWCCn, L_WCC, active_hypoWCCn, L_WCC)
	wc2_transcription_reactants_0: 0  -> wc2_gene()@c2 -1*(fRate3()) #Modifiers(hypoFRQn, hypoWCCn, hypoWCCn, hypoFRQn)
