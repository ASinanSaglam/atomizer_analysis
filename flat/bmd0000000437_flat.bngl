begin functions
	total_hypoWCC_ar() = hypoWCCc_c1 + hypoWCCn_c2
	total_hyperWCC_ar() = hyperWCCc_c1 + hyperWCCn_c2
	hyper_hypo_WCC_ar() = total_hyperWCC_ar/total_hypoWCC_ar
	total_WCCn_ar() = L_WCC_c2 + active_hypoWCCn_c2 + hyperWCCn_c2 + hypoWCCn_c2
	total_WC2_ar() = L_WCC_c2 + WC2c_c1 + active_hypoWCCn_c2 + hyperWCCc_c1 + hyperWCCn_c2 + hypoWCCc_c1 + hypoWCCn_c2
	Period_ar() = -X_default + Y_default
	total_VVD_ar() = L_WCCVVDn_c2 + VVDc_c1 + VVDn_c2
	n_hypoFRQ_to_hyperFRQ_ar() = hypoFRQn_c2/hyperFRQn_c2
	c_hypoFRQ_to_hyperFRQ_ar() = hypoFRQc_c1/hyperFRQc_c1
	total_hyper_FRQ_ar() = hyperFRQc_c1 + hyperFRQn_c2
	total_hypo_FRQ_ar() = hypoFRQc_c1 + hypoFRQn_c2
	total_FRQn_ar() = hyperFRQn_c2 + hypoFRQn_c2
	total_FRQc_ar() = hyperFRQc_c1 + hypoFRQc_c1
	total_WC1_ar() = L_WCCVVDn_c2 + L_WCC_c2 + WC1c_c1 + active_hypoWCCn_c2 + hyperWCCc_c1 + hyperWCCn_c2 + hypoWCCc_c1 + hypoWCCn_c2
	total_FRQ_ar() = hyperFRQc_c1 + hyperFRQn_c2 + hypoFRQc_c1 + hypoFRQn_c2
	fRate0() = 1
	fRate1() = L_WCC_c2*kadd_light_frq + active_hypoWCCn_c2^A_active_hypoWCCn_frq*kmax_frq/(Km_frq^A_active_hypoWCCn_frq + active_hypoWCCn_c2^A_active_hypoWCCn_frq)
	fRate2() = L_WCC_c2*kadd_L_wc1 + active_hypoWCCn_c2*kadd_wc1 + k_min_wc1
	fRate3() = hypoFRQn_c2*kadd_wc2_transcription_hypoFRQn + kmax_wc2/(hypoWCCn_c2*ki_wc2_transcription + 1)
	fRate4() = L_WCC_c2*kadd_vvd_light_mRNA
	fRate9() = frq_mRNA_c1*(hypoFRQc_c1*kdfrq_hypoFRQc + kd_frq)
	fRate23() = hypoFRQn_c2^I_hypoFRQn_hyperWCCn*kmaxp_hypoWCCn/(Kmp_hypoFRQn_hyperWCCn^I_hypoFRQn_hyperWCCn + hypoFRQn_c2^I_hypoFRQn_hyperWCCn)
end functions
