	frq_translation: frq_mRNA()@c1 -> hypoFRQc()@c1 k_hypoFRQc #Modifiers(frq_mRNA)
	wc1_translation: wc1_mRNA()@c1 -> WC1c()@c1 k_WC1c #Modifiers(wc1_mRNA)
	wc2_translation: wc2_mRNA()@c1 -> WC2c()@c1 k_WC2c #Modifiers(wc2_mRNA)
	vvd_translation: vvd_mRNA()@c1 -> VVDc()@c1 k_VVDc #Modifiers(vvd_mRNA)
	frq_mRNA_degradation_reactants_0: 0  -> frq_mRNA()@c1 -1*(fRate9()) #Modifiers(hypoFRQc, frq_mRNA, hypoFRQc)
	frq_mRNA_degradation_products_0: 0  -> degraded_frq_mRNA()@c1 fRate9() #Modifiers(hypoFRQc, frq_mRNA, hypoFRQc)
	wc1_mRNA_degradation: wc1_mRNA()@c1 -> degraded_wc1_mRNA()@c1 kd_wc1 #Modifiers(wc1_mRNA)
	wc2_mRNA_degradation: wc2_mRNA()@c1 -> degraded_wc2_mRNA()@c1 kd_wc2 #Modifiers(wc2_mRNA)
	vvd_mRNA_degradation: vvd_mRNA()@c1 -> degraded_vvd_mRNA()@c1 kd_vvd_mRNA #Modifiers(vvd_mRNA)
	hypoWCCc_formation: WC1c()@c1 + WC2c()@c1 -> hypoWCCc()@c1 k_hypoWCCc #Modifiers(WC1c, WC2c)
	hypoFRQc_translocation_in_nucleus: hypoFRQc()@c1 -> hypoFRQn()@c2 kin_hypoFRQc #Modifiers(hypoFRQc)
	hypoWCCc_translocation_in_nucleus: hypoWCCc()@c1 -> hypoWCCn()@c2 kin_hypoWCCc #Modifiers(hypoWCCc)
	VVD_nuclear_localisation: VVDc()@c1 -> VVDn()@c2 kin_VVDc #Modifiers(VVDc)
	hypoFRQn_translocation_out_nucleus: hypoFRQn()@c2 -> hypoFRQc()@c1 kout_hypoFRQn #Modifiers(hypoFRQn)
	hyperFRQn_translocation_out_nucleus: hyperFRQn()@c2 -> hyperFRQc()@c1 kout_hyperFRQn #Modifiers(hyperFRQn)
	hyperWCCn_translocation_out_nucleus: hyperWCCn()@c2 -> hyperWCCc()@c1 kout_hyperWCCn #Modifiers(hyperWCCn)
	hypoFRQc_phosphorylation: hypoFRQc()@c1 -> hyperFRQc()@c1 kp_hypoFRQc #Modifiers(hypoFRQc)
	hypoFRQn_phosphorylation: hypoFRQn()@c2 -> hyperFRQn()@c2 kp_hypoFRQn #Modifiers(hypoFRQn)
	hypoWCCc_phosphorylation: hypoWCCc()@c1 -> hyperWCCc()@c1 kp_hypoWCCc #Modifiers(hypoWCCc)
	hypoWCCn_phosphorylation: hypoWCCn()@c2 -> hyperWCCn()@c2 fRate23() #Modifiers(hypoFRQn, hypoWCCn, hypoFRQn)
	hyperWCCc_dephosphorylation: hyperWCCc()@c1 -> hypoWCCc()@c1 kdp_hyperWCCc #Modifiers(hyperWCCc)
	hypoWCCn_activation: hypoWCCn()@c2 -> active_hypoWCCn()@c2 kact_hypoWCCn #Modifiers(hypoWCCn)
	L_WCC_formation: hypoWCCn()@c2 -> L_WCC()@c2 kact_L_WCC #Modifiers(hypoWCCn)
	WCCVVD_complex_formation: VVDn()@c2 + L_WCC()@c2 -> L_WCCVVDn()@c2 k_WCCVVD #Modifiers(VVDn, L_WCC)
	WCCVVD_complex_disassociation: L_WCCVVDn()@c2 -> hypoWCCn()@c2 + VVDn()@c2 k_dis_WCCVVD #Modifiers(L_WCCVVDn)
	hyperFRQc_degradation: hyperFRQc()@c1 -> degraded_hyperFRQc()@c1 kd_hyperFRQc #Modifiers(hyperFRQc)
	hyperFFCn_degradation: hyperFRQn()@c2 -> degraded_hyperFFCn()@c2 kd_hyperFRQn #Modifiers(hyperFRQn)
	WC1c_degradation: WC1c()@c1 -> degraded_WC1c()@c1 kd_WC1c #Modifiers(WC1c)
	WC2c_degradation: WC2c()@c1 -> degraded_WC2c()@c1 kd_WC2c #Modifiers(WC2c)
	hyperWCCc_degradation: hyperWCCc()@c1 -> degraded_hyperWCCc()@c1 kd_hyperWCCc #Modifiers(hyperWCCc)
	hyperWCCn_degradation: hyperWCCn()@c2 -> degraded_hyperWCCn()@c2 kd_hyperWCCn #Modifiers(hyperWCCn)
	active_hypoWCCn_degradation: active_hypoWCCn()@c2 -> degraded_active_hypoWCCn()@c2 kd_active_hypoWCCn #Modifiers(active_hypoWCCn)
	L_WCC_degradation: L_WCC()@c2 -> degraded_L_WCCCVVDn()@c2 kd_L_WCC #Modifiers(L_WCC)
	VVDc_degradation: VVDc()@c1 -> degraded_VVDc()@c1 kd_VVDc #Modifiers(VVDc)
	VVDn_degradation: VVDn()@c2 -> degraded_VVDn()@c2 kd_VVDn #Modifiers(VVDn)
	L_WCCVVD_degradation: L_WCCVVDn()@c2 -> degraded_L_WCCCVVDn()@c2 kd_WCCVVD #Modifiers(L_WCCVVDn)
end reaction rules
