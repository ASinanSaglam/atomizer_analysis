	R3: Paracetamol_APAP() -> 0  kG #Modifiers(Paracetamol_APAP)
	R4: NAPQI() -> Protein_adducts() kPSH #Modifiers(NAPQI)
	R5: NAPQI() + GSH() -> 0  kGSH #Modifiers(NAPQI, GSH)
	R6: GSH() -> 0  dG #Modifiers(GSH)
	R7: 0  -> GSH() bG
	R8: Paracetamol_APAP() + Sulphate_PAPS() -> 0  kS #Modifiers(Paracetamol_APAP, Sulphate__PAPS)
	R9: Sulphate_PAPS() -> 0  dS #Modifiers(Sulphate__PAPS)
	R10: 0  -> Sulphate_PAPS() bS
end reaction rules
end model
