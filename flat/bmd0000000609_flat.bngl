begin model
begin parameters
	P0 1.32e-13
	dG 2.0
	bG 1.374e-14
	kGSH 1.6e+18
	kG 2.99
	kS 226000000000000.0
	bS 2.65e-14
	dS 2.0
	k450 0.315
	kN 0.0315
	kPSH 110.0
end parameters
begin molecule types
	Protein_adducts()
	Paracetamol_APAP()
	GSH()
	NAPQI()
	Sulphate_PAPS()
end molecule types
begin seed species
	Sulphate_PAPS()() (2.65e-14) / (2.0) #Sulphate__PAPS
	GSH()() (1.374e-14) / (2.0) #GSH
	Paracetamol_APAP()() (1.32e-13) #Paracetamol_APAP
end seed species
begin observables
	Species Sulphate_PAPS Sulphate_PAPS() #Sulphate_PAPS
	Species GSH GSH() #GSH
	Species NAPQI NAPQI() #NAPQI
	Species Paracetamol_APAP Paracetamol_APAP() #Paracetamol_APAP
	Species Protein_adducts Protein_adducts() #Protein_adducts
