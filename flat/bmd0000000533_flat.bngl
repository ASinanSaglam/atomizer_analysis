###
begin model
begin parameters
	k0 0.59
	k1 0.672
	k2 0.678
	k3 0.0392
	k4 0.554
	q 2.0
	epsilon 0
	RC 5.305382e+22 #original 88.1substance/volume
	beta 7.16618e+21 #original 11.9substance/volume
end parameters
begin molecule types
	BTX()
	BN4()
	RCT0()
	BN3()
	BN2()
	BN1()
	alpha()
	RCT1()
	BM()
end molecule types
begin seed species
	RCT0() 5.305382e+22 # (88.1 * 0.001)mol/L * 6.022e23/mol *1.0L #RCT0 #RCT0
	BN1() 7.16618e+21 #original 11.9substance/volume #BN1 #BN1
end seed species
begin observables
	Species RCT0 RCT0() #RCT0
	Species alpha alpha() #alpha
	Species BN1 BN1() #BN1
	Species BN2 BN2() #BN2
	Species BN3 BN3() #BN3
	Species BN4 BN4() #BN4
	Species BTX BTX() #BTX
	Species BM BM() #BM
	Species RCT1 RCT1() #RCT1
end observables
begin functions
	arRateRCT1() = RCT0*k0*(BM + epsilon)
	arRateBM() = BTX*k4
	arRateBTX() = 4*BN4*k3 - BTX*k4
	arRateBN4() = 4*BN3*k3 - 4*BN4*k3
	arRateBN3() = 4*BN2*k3 - 4*BN3*k3
	arRateBN2() = 4*BN1*k3 - 4*BN2*k3
	arRateBN1() = -4*BN1*k3 + BTX^q*alpha*k2
	arRatealpha() = BTX*RCT0*k1 - BTX^q*alpha*k2
	arRateRCT0() = -RCT0*(BM*k0 + BTX*k1 + epsilon*k0)
	beta_ar() = BM + BN1 + BN2 + BN3 + BN4 + BTX
	RC_ar() = RCT0 + RCT1
end functions
begin reaction rules
	0  -> RCT0() arRateRCT0 #rateLaw
	0  -> alpha() arRatealpha #rateLaw
	0  -> BN1() arRateBN1 #rateLaw
	0  -> BN2() arRateBN2 #rateLaw
	0  -> BN3() arRateBN3 #rateLaw
	0  -> BN4() arRateBN4 #rateLaw
	0  -> BTX() arRateBTX #rateLaw
	0  -> BM() arRateBM #rateLaw
	0  -> RCT1() arRateRCT1 #rateLaw
end reaction rules
end model
