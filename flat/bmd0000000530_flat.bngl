###
begin model
begin parameters
	TF_mRNA 6.022e+20 #original 1.0substance/L
	TF_miRNA_1 6.022e+20 #original 1.0substance/L
	TF_miRNA_2 6.022e+20 #original 1.0substance/L
	r1_k1 0.000453
	r2_k1 0.000013
	r3_k1 0.999534
	r4_k_syn_mRNA 1.000000
	r5_k_syn_miRNA_1 1.000000
	r6_k_syn_miRNA_2 1.000000
	r7_k1 1.000000
	r8_k1 1.000000
	r9_k1 1.000000
	r10_k1 0.249955
	r11_k1 0.241033
	r12_k1 0.187796
	r13_k1 1.000000
	r14_k1 1.000000
	r15_k1 1.000000
	r16_k_syn_prot 1.000000
	r17_k1 1.000000
end parameters
begin molecule types
	mRNA()
	duplex_1()
	miRNA_1()
	duplex_2()
	miRNA_2()
	protein()
	triplex()
end molecule types
begin seed species
	mRNA() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #mRNA #species_1
	protein() 6.022e+20 #original 1.0substance/L  #protein #species_10
end seed species
begin observables
	Species mRNA mRNA() #mRNA
	Species miRNA_1 miRNA_1() #miRNA_1
	Species miRNA_2 miRNA_2() #miRNA_2
	Species duplex_1 duplex_1() #duplex_1
	Species duplex_2 duplex_2() #duplex_2
	Species triplex triplex() #triplex
	Species protein protein() #protein
end observables
begin functions
	fRate3() = 1.0*TF_mRNA
	fRate4() = 1.0*TF_miRNA_1
	fRate5() = 1.0*TF_miRNA_2
	fRate15() = 1.0*mRNA
end functions
begin reaction rules
	reaction_1: mRNA() + miRNA_1() -> duplex_1() r1_k1 #Modifiers(species_1, species_2)
	reaction_2: mRNA() + miRNA_2() -> duplex_2() r2_k1 #Modifiers(species_1, species_3)
	reaction_3: mRNA() + miRNA_1() + miRNA_2() -> triplex() r3_k1 #Modifiers(species_1, species_2, species_3)
	reaction_4: 0  -> mRNA() fRate3() #Modifiers(species_7, species_7)
	reaction_5: 0  -> miRNA_1() fRate4() #Modifiers(species_8, species_8)
	reaction_6: 0  -> miRNA_2() fRate5() #Modifiers(species_9, species_9)
	reaction_7: mRNA() -> 0  r7_k1 #Modifiers(species_1)
	reaction_8: miRNA_1() -> 0  r8_k1 #Modifiers(species_2)
	reaction_9: miRNA_2() -> 0  r9_k1 #Modifiers(species_3)
	reaction_10: duplex_1() -> miRNA_1() + mRNA() r10_k1 #Modifiers(species_4)
	reaction_11: duplex_2() -> miRNA_2() + mRNA() r11_k1 #Modifiers(species_5)
	reaction_12: triplex() -> miRNA_1() + miRNA_2() + mRNA() r12_k1 #Modifiers(species_6)
	reaction_13: duplex_1() -> 0  r13_k1 #Modifiers(species_4)
	reaction_14: duplex_2() -> 0  r14_k1 #Modifiers(species_5)
	reaction_15: triplex() -> 0  r15_k1 #Modifiers(species_6)
	reaction_16: 0  -> protein() fRate15() #Modifiers(species_1, species_1)
	reaction_17: protein() -> 0  r17_k1 #Modifiers(species_10)
end reaction rules
end model
