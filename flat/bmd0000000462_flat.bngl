###
begin model
begin parameters
	kprod 1.86e-05
	kdeg 2.1e-05
	kdimer 1.1783e-07
	kdedimer 8.4655e-06
	kdisagg 5.4357e-05
	kpf 2.785e-06
	kpg 0.00574
	kpghalf 4.0
	kdegNep 1.8e-10
	Source 1.0
	Sink 1.0
end parameters
begin molecule types
	AbetaMonomer()
	Neprilysin()
	AbetaDimer()
	AbetaPlaque()
end molecule types
begin seed species
	Neprilysin() 1000.0 #Neprilysin #Nep
end seed species
begin observables
	Species AbetaMonomer AbetaMonomer() #AbetaMonomer
	Species AbetaDimer AbetaDimer() #AbetaDimer
	Species AbetaPlaque AbetaPlaque() #AbetaPlaque
	Species Neprilysin Neprilysin() #Neprilysin
end observables
begin functions
	fRate0() = Source*kprod
	fRate1() = 0.001*kdeg
	fRate2() = 0.5*AbetaMonomer*kdimer*(AbetaMonomer - 1)
	fRate4() = 0.5*AbetaDimer*kpf*(AbetaDimer - 1)
	fRate5() = AbetaPlaque*kpg/(AbetaPlaque^2 + kpghalf^2)
end functions
begin reaction rules
	Abetaproduction: 0  -> AbetaMonomer() fRate0() #Modifiers(Source)
	AbetaDegradation: AbetaMonomer() + Neprilysin() -> Neprilysin() fRate1() #Modifiers(Abeta, Nep)
	AbetaDimerisation_reactants_0: 0  -> AbetaMonomer() -1*2.0*(fRate2()) #Modifiers(Abeta)
	AbetaDimerisation_products_0: 0  -> AbetaDimer() fRate2() #Modifiers(Abeta)
	AbetaDedimerisation: AbetaDimer() -> AbetaMonomer() + AbetaMonomer() kdedimer #Modifiers(AbDim)
	AbetaPlaqueFormation_reactants_0: 0  -> AbetaDimer() -1*2.0*(fRate4()) #Modifiers(AbDim)
	AbetaPlaqueFormation_products_0: 0  -> AbetaPlaque() 4.0*fRate4() #Modifiers(AbDim)
	AbetaPlaqueGrowth: AbetaMonomer() + AbetaPlaque() -> AbetaPlaque() + AbetaPlaque() fRate5() #Modifiers(Abeta, AbP)
	AbetaDisaggregation: AbetaPlaque() -> AbetaMonomer() kdisagg #Modifiers(AbP)
	NepDegradation: Neprilysin() -> 0  kdegNep #Modifiers(Nep)
end reaction rules
end model
