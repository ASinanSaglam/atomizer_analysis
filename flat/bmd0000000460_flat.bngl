###
begin model
begin parameters
	kbd 5.8e-09
	kbs 100.0
	kbx 8.4e-05
	kxd 1.2e-13
	kxs 2e-06
	kzd 0.052
	kzs 1.7e-06
end parameters
begin molecule types
	IPTG()
	lacz()
	sigb()
	x()
end molecule types
begin seed species
	$IPTG() 6.022e+25 # 100.0mol/L * 6.022e23/mol *1.0L #IPTG #IPTG
end seed species
begin observables
	Species IPTG IPTG() #IPTG
	Species sigb sigb() #sigb
	Species lacz lacz() #lacz
	Species x x() #x
end observables
begin functions
	fRate0() = IPTG*kbs - sigb*(kbd + kbx*x)
end functions
begin reaction rules
	v1_reactants_0: 0  -> IPTG() -1*(fRate0()) #Modifiers(x, IPTG, sigb, x)
	v1_products_0: 0  -> sigb() fRate0() #Modifiers(x, IPTG, sigb, x)
	v2: sigb() <-> lacz() kzs,kzd #Modifiers(lacz, sigb)
	v3: sigb() <-> x() kxs,kxd #Modifiers(sigb, x)
end reaction rules
end model
