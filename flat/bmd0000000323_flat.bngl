begin model
begin parameters
	parameter_1 1.0
	parameter_2 0.3
	parameter_3 5.0
	r1_Shalve 1.000000
	r2_Shalve 1.000000
	r3_Shalve 1.000000
end parameters
begin molecule types
	x2()
	x3()
	x1()
end molecule types
begin seed species
	x3() 1.98726e+23 # 0.33mol/L * 6.022e23/mol *1.0L #x3 #species_3
end seed species
begin observables
	Species x1 x1() #x1
	Species x2 x2() #x2
	Species x3 x3() #x3
end observables
begin functions
	fRate0() = parameter_1/(1.0^parameter_3 + x1^parameter_3)
	fRate1() = parameter_1/(1.0^parameter_3 + x2^parameter_3)
	fRate2() = parameter_1/(1.0^parameter_3 + x3^parameter_3)
	fRate3() = 1/(parameter_2 + x1)
	fRate4() = 1/(parameter_2 + x2)
	fRate5() = 1/(parameter_2 + x3)
end functions
begin reaction rules
	reaction_1_reactants_0: 0  -> x1() -1*(fRate0())
	reaction_1_products_0: 0  -> x1() fRate0()
	reaction_1_products_1: 0  -> x3() fRate0()
	reaction_2_reactants_0: 0  -> x2() -1*(fRate1())
	reaction_2_products_0: 0  -> x2() fRate1()
	reaction_2_products_1: 0  -> x1() fRate1()
	reaction_3_reactants_0: 0  -> x3() -1*(fRate2())
	reaction_3_products_0: 0  -> x3() fRate2()
	reaction_3_products_1: 0  -> x2() fRate2()
	reaction_4: x1() -> 0  fRate3()
	reaction_5: x2() -> 0  fRate4()
	reaction_6: x3() -> 0  fRate5()
end reaction rules
end model
