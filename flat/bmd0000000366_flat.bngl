###
begin model
begin parameters
	kcat_X 239.1
	km_X 23.65
	ki_Xa 4.531
	k_PT 122.9
	k_PL 801.4
	kcat_V 7.844
	km_V 149.7
	kcat_II 43.87
	km_II 62.25
	kcat_2 12.4
	km_2 0.06148
	ki_IIaAlpha2M 0.1762
	ki_IIaATIII 0.7859
	RVV 1.8066e+16 #original 0.03substance/L
end parameters
begin molecule types
	Xa()
	V()
	IIa_ATIII()
	Va()
	II()
	IIa_alpha2M()
	IIa()
	PT()
	Xa_ATIII()
	X()
	PL()
end molecule types
begin seed species
	X() 1.2043993978e+17 # (0.1999999 * 1e-06)mol/L * 6.022e23/mol *1.0L #X #X
	PL() 3.0109993978000004e+16 #original 0.04999999substance/L #PL #PL
	V() 1.8065993978e+16 #original 0.02999999substance/L #V #V
	II() 8.4308e+17 #original 1.4substance/L #II #II
end seed species
begin observables
	Species X X() #X
	Species Xa Xa() #Xa
	Species Xa_ATIII Xa_ATIII() #Xa_ATIII
	Species PL PL() #PL
	Species PT PT() #PT
	Species Va Va() #Va
	Species IIa IIa() #IIa
	Species V V() #V
	Species II II() #II
	Species IIa_alpha2M IIa_alpha2M() #IIa_alpha2M
	Species IIa_ATIII IIa_ATIII() #IIa_ATIII
end observables
begin functions
	AmAct() = IIa + 0.556*IIa_alpha2M
	fRate0() = RVV*kcat_X/(X + km_X)
	fRate3() = IIa*kcat_V/(V + km_V)
	fRate4() = PT*kcat_II/(II + km_II)
	fRate5() = Xa*kcat_2/(II + km_2)
end functions
begin reaction rules
	R1: X() -> Xa() fRate0() #Modifiers(RVV)
	R2: Xa() -> Xa_ATIII() ki_Xa
	R3: Va() + Xa() + PL() <-> PT() k_PT,k_PL
	R4: V() -> Va() fRate3() #Modifiers(IIa)
	R5: II() -> IIa() fRate4() #Modifiers(PT)
	R6: II() -> IIa() fRate5() #Modifiers(Xa)
	R7: IIa() -> IIa_alpha2M() ki_IIaAlpha2M
	R8: IIa() -> IIa_ATIII() ki_IIaATIII
end reaction rules
end model
