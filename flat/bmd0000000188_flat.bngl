#@notes:'This is a plain translation of an SBML model created on 14/08/2021. The original model has 20 molecules and 20 reactions. The translated model has 16 molecules and 20 rules'
###
begin model
begin parameters
	ksynMdm2 0.000495 #units:psec
	kdegMdm2 0.000433 #units:psec
	ksynp53 0.006 #units:psec
	kdegp53 0.000825 #units:psec
	kbinMdm2p53 0.001155 #units:pmolepsec
	krelMdm2p53 1.155e-05 #units:psec
	ksynMdm2mRNA 0.0001 #units:psec
	kdegMdm2mRNA 0.0001 #units:psec
	kactATM 0.0001 #units:pmolepsec
	kdegATMMdm2 0.0004 #units:psec
	kinactATM 0.0005 #units:psec
	kphosp53 0.0005 #units:pmolepsec
	kdephosp53 0.5 #units:psec
	kphosMdm2 2.0 #units:pmolepsec
	kdephosMdm2 0.5 #units:psec
	kdam 0.08 #units:molepsecpdGy
	krepair 2e-05 #units:psec
	kproteff 1.0 #units:dimensionless
	ksynp53mRNA 0.001 #units:psec
	kdegp53mRNA 0.0001 #units:psec
	IR 0
	Sink 1.0
	Source 1.0
end parameters
begin molecule types
	Mdm2_mRNA()
	mdm2syn()
	Mdm2mRNAdeg()
	ATMA()
	Mdm2mRNAsyn()
	p53()
	p53syn()
	p53_mRNA()
	mdm2deg()
	Mdm2_p53()
	Mdm2()
	ATMI()
	Mdm2_P()
	p53_P()
	p53deg()
	damDNA()
end molecule types
begin seed species
	Mdm2() 5.0 #Mdm2 #Mdm2
	p53() 5.0 #p53 #p53
	Mdm2_p53() 95.0 #Mdm2_p53 #Mdm2_p53
	Mdm2_mRNA() 10.0 #Mdm2_mRNA #Mdm2_mRNA
	p53_mRNA() 10.0 #p53_mRNA #p53_mRNA
	ATMI() 200.0 #ATMI #ATMI
end seed species
begin observables
	Species Mdm2 Mdm2() #Mdm2
	Species p53 p53() #p53
	Species Mdm2_p53 Mdm2_p53() #Mdm2_p53
	Species Mdm2_mRNA Mdm2_mRNA() #Mdm2_mRNA
	Species p53_mRNA p53_mRNA() #p53_mRNA
	Species ATMA ATMA() #ATMA
	Species ATMI ATMI() #ATMI
	Species p53_P p53_P() #p53_P
	Species Mdm2_P Mdm2_P() #Mdm2_P
	Species damDNA damDNA() #damDNA
	Species p53deg p53deg() #p53deg
	Species p53syn p53syn() #p53syn
	Species mdm2deg mdm2deg() #mdm2deg
	Species mdm2syn mdm2syn() #mdm2syn
	Species Mdm2mRNAdeg Mdm2mRNAdeg() #Mdm2mRNAdeg
	Species Mdm2mRNAsyn Mdm2mRNAsyn() #Mdm2mRNAsyn
end observables
begin functions
	totMdm2_ar() = Mdm2 + Mdm2_P + Mdm2_p53
	totp53_ar() = Mdm2_p53 + p53 + p53_P
	fRate0() = Source*ksynp53mRNA
	fRate6() = kdegMdm2*kproteff
	fRate8() = kdegp53*kproteff
	fRate11() = IR*kdam
end functions
begin reaction rules
	p53mRNASynthesis: 0  -> p53_mRNA() fRate0()
	p53mRNADegradation: p53_mRNA() -> 0  kdegp53mRNA
	Mdm2Synthesis: Mdm2_mRNA() -> Mdm2_mRNA() + Mdm2() + mdm2syn() ksynMdm2
	Mdm2mRNASynthesis1: p53() -> p53() + Mdm2_mRNA() + Mdm2mRNAsyn() ksynMdm2mRNA
	Mdm2mRNASynthesis2: p53_P() -> p53_P() + Mdm2_mRNA() + Mdm2mRNAsyn() ksynMdm2mRNA
	Mdm2mRNADegradation: Mdm2_mRNA() -> Mdm2mRNAdeg() kdegMdm2mRNA
	Mdm2Degradation: Mdm2() -> mdm2deg() fRate6()
	p53Synthesis: p53_mRNA() -> p53() + p53_mRNA() + p53syn() ksynp53
	p53Degradation: Mdm2_p53() -> Mdm2() + p53deg() fRate8()
	P53_Mdm2Binding: p53() + Mdm2() -> Mdm2_p53() kbinMdm2p53
	P53_Mdm2Release: Mdm2_p53() -> p53() + Mdm2() krelMdm2p53
	DNAdamage: 0  -> damDNA() fRate11()
	DNArepair: damDNA() -> 0  krepair
	ATMactivation: damDNA() + ATMI() -> damDNA() + ATMA() kactATM
	p53phoshorylation: p53() + ATMA() -> p53_P() + ATMA() kphosp53
	p53dephosorylation: p53_P() -> p53() kdephosp53
	Mdm2phoshorylation: Mdm2() + ATMA() -> Mdm2_P() + ATMA() kphosMdm2
	Mdm2dephosorylation: Mdm2_P() -> Mdm2() kdephosMdm2
	Mdm2Pdegradation: Mdm2_P() -> mdm2deg() kdegATMMdm2
	ATMInactivation: ATMA() -> ATMI() kinactATM
end reaction rules
end model
