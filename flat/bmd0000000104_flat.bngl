begin model
begin parameters
	E1 6.022e+23 #original 1.0mol/L
	Etot 6.022e+23 #original 1.0mol/L
	r1_k1 1.000000
	r2_k2 1.000000
end parameters
begin molecule types
	S()
	P()
	X1()
end molecule types
begin seed species
	S() 6.022e+23 # 1.0mol/L * 6.022e23/mol *1.0L #S #species_0
end seed species
begin observables
	Species S S() #S
	Species X1 X1() #X1
	Species P P() #P
end observables
begin functions
	E2_ar() =  -E1 + Etot
	fRate0 =  1.0*E1
	fRate1() = 1.0*E2_ar
end functions
begin reaction rules
	reaction_0: S() -> X1() fRate0() #Modifiers(species_2)
	reaction_1: X1() -> P() fRate1() #Modifiers(species_3)
end reaction rules
end model
