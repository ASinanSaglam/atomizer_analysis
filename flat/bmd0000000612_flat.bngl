begin model
begin parameters
	kactCreb 0.009
	kactOcy 0.004
	kactOcyPth 0.08
	kactTgfb 2e-07
	kactWnt 0.03
	kactWntPth 0.001
	kbinBaxBcl2 0.01
	kbinCrebRunx2 0.01
	kbinObmPTH 0.02
	kbinObpPTH 0.0003
	kbinObpTgfb 0.0002
	kbinOclpRANKL 0.001
	kbinOcyPTH 0.008
	kdeathOb 0.00024
	kdeathOcl 6.5e-05
	kdeathOclp 1e-05
	kdeathOcy 1e-08
	kdegBcl2 0.0025
	kdegBone 6.5e-09
	kdegMCSF 0.0001
	kdegOPG 4e-06
	kdegOPGRANKL 1e-05
	kdegPTH 0.002
	kdegRANKL 3e-05
	kdegRunx2 0.0001
	kdegRunx2PTH 0.003
	kdegSost 0.004
	kdegTgfb 5e-05
	kdegTgfbPTH 1.7e-05
	kdiffHSC 5.5e-05
	kdiffMSC 0.00065
	kdiffObP 0.0001
	kdiffObproTgfb 0.05
	kdiffOclP 8e-05
	kformBone 3.07e-06
	kinactCreb 0.0001
	kinactOcy 2e-05
	kinactWnt 0.8
	kinhibRANKL 0.001
	kmatOb 2e-09
	kmatObTgfb 1e-08
	krelBaxBcl2 0.5
	krelCrebRunx2 0.01
	krelObmPTH 0.005
	krelObpPTH 0.005
	krelObpTgfb 0.01
	krelOclpRANKL 0.001
	krelOcyPTH 0.005
	krelRANKL 0.001
	ksecMCSFbyMSC 1e-05
	ksecMCSFbyObm 1e-05
	ksecMCSFbyObp 1e-05
	ksecMCSFbyObpro 1e-05
	ksecOPGbyObm 1e-05
	ksecOPGbyObp 2e-06
	ksecOPGbyObpPTH 1e-06
	ksecRANKLbyMSC 1e-06
	ksecRANKLbyObm 1e-07
	ksecRANKLbyObmPTH 1e-06
	ksecRANKLbyObp 3e-06
	ksecRANKLbyObpPTH 2e-05
	ksecRANKLbyObpro 7e-06
	ksecRANKLbyObpTgfb 4e-06
	ksecRANKLbyOcy 1e-06
	ksecRANKLbyOcyI 1e-07
	ksecSost 0.00075
	ksecTgfb 5e-05
	ksynBcl2 0.005
	ksynPTH 0.02
	ksynRunx2 0.005
	ksynX 0.01157
	kunload 0.00035
	Source_ecm 1.0
	Source 1.0
	Sink_ecm 1.0
	Sink 1.0
end parameters
begin compartments
	#volume units: L
	bone 3 1.0
	ecm 3 1.0
	osteoblast 3 1.0
end compartments
begin molecule types
	Ocl_p_RANKL()
	CREB_P()
	Ocy_I_PTH()
	Runx2()
	newbone()
	Tgfb_A()
	Bax()
	HSC()
	Tgfb_I()
	CREB_Runx2()
	RANKL()
	X()
	OPG_RANKL()
	Ob_p_Tgfb_A()
	Ob_pro()
	LOAD()
	CREB()
	Ob_p()
	PTH()
	Ob_m()
	Bax_Bcl2()
	OPG()
	Ocl_p()
	Ocy_I()
	Ob_m_PTH()
	Bcl2()
	Ob_p_PTH()
	MCSF()
	Bone()
	Ocl_m()
	Wnt_A()
	Wnt_I()
	Ocy_A()
	MSC()
	Sost()
end molecule types
begin seed species
	@bone:Bone() 2000.0 #Bone #Bone
	@bone:HSC() 100.0 #HSC #HSC
	@bone:Ocy_I() 1800.0 #Ocy_I #Ocy_I
	@bone:MSC() 100.0 #MSC #MSC
	@ecm:MCSF() 5.0 #MCSF #MCSF
	@ecm:PTH() 170.0 #PTH #PTH
	@ecm:Tgfb_A() 5.0 #Tgfb_A #Tgfb_A
	@ecm:Tgfb_I() 500.0 #Tgfb_I #Tgfb_I
	@ecm:Wnt_I() 200.0 #Wnt_I #Wnt_I
	@osteoblast:Bax() 100.0 #Bax #Bax
	@osteoblast:CREB() 100.0 #CREB #CREB
	@osteoblast:Runx2() 10.0 #Runx2 #Runx2
end seed species
begin observables
	Species Bone_bone @bone:Bone() #Bone
	Species HSC_bone @bone:HSC() #HSC
	Species Ob_m_bone @bone:Ob_m() #Ob_m
	Species Ob_p_bone @bone:Ob_p() #Ob_p
	Species Ob_p_Tgfb_A_bone @bone:Ob_p_Tgfb_A() #Ob_p_Tgfb_A
	Species Ob_pro_bone @bone:Ob_pro() #Ob_pro
	Species Ocl_m_bone @bone:Ocl_m() #Ocl_m
	Species Ocl_p_bone @bone:Ocl_p() #Ocl_p
	Species Ocl_p_RANKL_bone @bone:Ocl_p_RANKL() #Ocl_p_RANKL
	Species Ocy_A_bone @bone:Ocy_A() #Ocy_A
	Species Ocy_I_bone @bone:Ocy_I() #Ocy_I
	Species Ocy_I_PTH_bone @bone:Ocy_I_PTH() #Ocy_I_PTH
	Species MSC_bone @bone:MSC() #MSC
	Species LOAD_ecm @ecm:LOAD() #LOAD
	Species MCSF_ecm @ecm:MCSF() #MCSF
	Species Ob_m_PTH_ecm @ecm:Ob_m_PTH() #Ob_m_PTH
	Species Ob_p_PTH_ecm @ecm:Ob_p_PTH() #Ob_p_PTH
	Species OPG_ecm @ecm:OPG() #OPG
	Species OPG_RANKL_ecm @ecm:OPG_RANKL() #OPG_RANKL
	Species PTH_ecm @ecm:PTH() #PTH
	Species RANKL_ecm @ecm:RANKL() #RANKL
	Species Sost_ecm @ecm:Sost() #Sost
	Species Tgfb_A_ecm @ecm:Tgfb_A() #Tgfb_A
	Species Tgfb_I_ecm @ecm:Tgfb_I() #Tgfb_I
	Species Wnt_A_ecm @ecm:Wnt_A() #Wnt_A
	Species Wnt_I_ecm @ecm:Wnt_I() #Wnt_I
	Species X_ecm @ecm:X() #X
	Species newbone_ecm @ecm:newbone() #newbone
	Species Bax_osteoblast @osteoblast:Bax() #Bax
	Species Bax_Bcl2_osteoblast @osteoblast:Bax_Bcl2() #Bax_Bcl2
	Species Bcl2_osteoblast @osteoblast:Bcl2() #Bcl2
	Species CREB_osteoblast @osteoblast:CREB() #CREB
	Species CREB_P_osteoblast @osteoblast:CREB_P() #CREB_P
	Species CREB_Runx2_osteoblast @osteoblast:CREB_Runx2() #CREB_Runx2
	Species Runx2_osteoblast @osteoblast:Runx2() #Runx2
end observables
begin functions
	fRate4() = PTH_ecm*kbinOcyPTH/(PTH_ecm^2 + 10000.0)
	fRate11() = Sost_ecm*kinactWnt/(Sost_ecm^2 + 2500.0)
	fRate12() = Wnt_A_ecm*kdiffMSC/(Wnt_A_ecm^2 + 2500.0)
	fRate13() = Tgfb_A_ecm*kdiffObproTgfb/(Tgfb_A_ecm^2 + 2500.0)
	fRate15() = PTH_ecm*kbinObpPTH/(PTH_ecm^2 + 10000.0)
	fRate20() = Tgfb_A_ecm*kmatObTgfb/(Tgfb_A_ecm^2 + 2500.0)
	fRate21() = PTH_ecm*kbinObmPTH/(PTH_ecm^2 + 10000.0)
	fRate23() = Bax_osteoblast*kdeathOb/(Bax_osteoblast^2 + 2500.0)
	fRate24() = Bax_osteoblast*kdeathOb/(Bax_osteoblast^2 + 2500.0)
	fRate25() = MCSF_ecm*kdiffHSC/(MCSF_ecm^2 + 2500.0)
	fRate59() = Source_ecm*ksynPTH
	fRate61() = Ob_m_PTH_ecm*kactCreb/(Ob_m_PTH_ecm^2 + 10000.0)
	fRate69() = Source_ecm*ksynRunx2
	fRate75() = Source_ecm*ksynX
end functions
begin reaction rules
	Unloading: LOAD()@ecm -> 0  kunload #Modifiers(LOAD)
	Osteocyte_activation: Ocy_I()@bone + LOAD()@ecm -> Ocy_A()@bone + LOAD()@ecm kactOcy #Modifiers(Ocy_I, LOAD)
	Osteocyte_activation_by_PTH: Ocy_I_PTH()@bone -> Ocy_A()@bone + PTH()@ecm kactOcyPth #Modifiers(Ocy_I_PTH)
	Osteocyte_inactivation: Ocy_A()@bone -> Ocy_I()@bone kinactOcy #Modifiers(Ocy_A)
	Ocy_I_bound_by_PTH: Ocy_I()@bone + PTH()@ecm -> Ocy_I_PTH()@bone fRate4() #Modifiers(Ocy_I, PTH)
	Ocy_I_PTH_release: Ocy_I_PTH()@bone -> Ocy_I()@bone + PTH()@ecm krelOcyPTH #Modifiers(Ocy_I_PTH)
	Ocy_apoptosis: Ocy_I()@bone -> 0  kdeathOcy #Modifiers(Ocy_I)
	Sost_secretion: Ocy_I()@bone -> Ocy_I()@bone + Sost()@ecm ksecSost #Modifiers(Ocy_I)
	Sost_degradation: Sost()@ecm -> 0  kdegSost #Modifiers(Sost)
	Wnt_activation: Wnt_I()@ecm -> Wnt_A()@ecm kactWnt #Modifiers(Wnt_I)
	Wnt_activation_by_PTH: Wnt_I()@ecm + Ob_m_PTH()@ecm -> Wnt_A()@ecm + Ob_m_PTH()@ecm kactWntPth #Modifiers(Wnt_I, Ob_m_PTH)
	Inactivation_of_Wnt_by_Sost: Wnt_A()@ecm + Sost()@ecm -> Wnt_I()@ecm + Sost()@ecm fRate11() #Modifiers(Wnt_A, Sost)
	MSC_differentiation_to_Ob_pro: MSC()@bone + Wnt_A()@ecm -> MSC()@bone + Wnt_A()@ecm + Ob_pro()@bone fRate12() #Modifiers(MSC, Wnt_A)
	Osteoblast_progenitor_differentiation_by_Tgfb: Ob_pro()@bone + Tgfb_A()@ecm -> Ob_p()@bone + Tgfb_A()@ecm fRate13() #Modifiers(Ob_pro, Tgfb_A)
	Ob_precursor_differentiation: Ob_p()@bone -> Ob_m()@bone kdiffObP #Modifiers(Ob_p)
	Ob_p_binding_by_PTH: Ob_p()@bone + PTH()@ecm -> Ob_p_PTH()@ecm fRate15() #Modifiers(Ob_p, PTH)
	Ob_p_PTH_release: Ob_p_PTH()@ecm -> Ob_p()@bone + PTH()@ecm krelObpPTH #Modifiers(Ob_p_PTH)
	Ob_p_Tgfb_binding: Ob_p()@bone + Tgfb_A()@ecm -> Ob_p_Tgfb_A()@bone kbinObpTgfb #Modifiers(Ob_p, Tgfb_A)
	Ob_p_Tgfb_release: Ob_p_Tgfb_A()@bone -> Ob_p()@bone + Tgfb_A()@ecm krelObpTgfb #Modifiers(Ob_p_Tgfb_A)
	Ob_maturation_to_Ocy: Ob_m()@bone -> Ocy_I()@bone kmatOb #Modifiers(Ob_m)
	Ob_maturation_to_Ocy_by_Tgfb: Ob_m()@bone + Tgfb_A()@ecm -> Ocy_I()@bone + Tgfb_A()@ecm fRate20() #Modifiers(Ob_m, Tgfb_A)
	Ob_m_bound_by_PTH: Ob_m()@bone + PTH()@ecm -> Ob_m_PTH()@ecm fRate21() #Modifiers(Ob_m, PTH)
	Ob_m_PTH_release: Ob_m_PTH()@ecm -> Ob_m()@bone + PTH()@ecm krelObmPTH #Modifiers(Ob_m_PTH)
	Ob_m_apoptosis: Ob_m()@bone + Bax()@osteoblast -> Bax()@osteoblast fRate23() #Modifiers(Ob_m, Bax)
	Ob_m_PTH_apoptosis: Ob_m_PTH()@ecm + Bax()@osteoblast -> Bax()@osteoblast + PTH()@ecm fRate24() #Modifiers(Ob_m_PTH, Bax)
	HSC_differentiation_to_Ocl_p: HSC()@bone + MCSF()@ecm -> HSC()@bone + MCSF()@ecm + Ocl_p()@bone fRate25() #Modifiers(HSC, MCSF)
	Ocl_p_apoptosis: Ocl_p()@bone -> 0  kdeathOclp #Modifiers(Ocl_p)
	Ocl_p_RANKL_binding: RANKL()@ecm + Ocl_p()@bone -> Ocl_p_RANKL()@bone kbinOclpRANKL #Modifiers(Ocl_p, RANKL)
	Ocl_p_RANKL_release: Ocl_p_RANKL()@bone -> Ocl_p()@bone + RANKL()@ecm krelOclpRANKL #Modifiers(Ocl_p_RANKL)
	Osteoclast_precursor_differentiation: Ocl_p_RANKL()@bone -> Ocl_m()@bone kdiffOclP #Modifiers(Ocl_p_RANKL)
	RANKL_inhibition: OPG()@ecm + RANKL()@ecm -> OPG_RANKL()@ecm kinhibRANKL #Modifiers(OPG, RANKL)
	OPG_RANKL_dissociation: OPG_RANKL()@ecm -> OPG()@ecm + RANKL()@ecm krelRANKL #Modifiers(OPG_RANKL)
	OPG_RANKL_degradation: OPG_RANKL()@ecm -> 0  kdegOPGRANKL #Modifiers(OPG_RANKL)
	Osteoclast_apoptosis: Ocl_m()@bone -> 0  kdeathOcl #Modifiers(Ocl_m)
	RANKL_degradation: RANKL()@ecm -> 0  kdegRANKL #Modifiers(RANKL)
	OPG_degradation: OPG()@ecm -> 0  kdegOPG #Modifiers(OPG)
	MCSF_secretion_by_MSC: MSC()@bone -> MSC()@bone + MCSF()@ecm ksecMCSFbyMSC #Modifiers(MSC)
	MCSF_secretion_by_Ob_pro: Ob_pro()@bone -> Ob_pro()@bone + MCSF()@ecm ksecMCSFbyObpro #Modifiers(Ob_pro)
	MCSF_secretion_by_Ob_p: Ob_p()@bone -> Ob_p()@bone + MCSF()@ecm ksecMCSFbyObp #Modifiers(Ob_p)
	MCSF_secretion_by_Ob_p_PTH: Ob_p_PTH()@ecm -> Ob_p_PTH()@ecm + MCSF()@ecm ksecMCSFbyObp #Modifiers(Ob_p_PTH)
	MCSF_secretion_by_Ob_m: Ob_m()@bone -> Ob_m()@bone + MCSF()@ecm ksecMCSFbyObm #Modifiers(Ob_m)
	MCSF_secretion_by_Ob_m_PTH: Ob_m_PTH()@ecm -> Ob_m_PTH()@ecm + MCSF()@ecm ksecMCSFbyObm #Modifiers(Ob_m_PTH)
	MCSF_degradation: MCSF()@ecm -> 0  kdegMCSF #Modifiers(MCSF)
	OPG_secretion_by_Ob_p: Ob_p()@bone -> Ob_p()@bone + OPG()@ecm ksecOPGbyObp #Modifiers(Ob_p)
	OPG_secretion_by_Ob_p_PTH: Ob_p_PTH()@ecm -> Ob_p_PTH()@ecm + OPG()@ecm ksecOPGbyObpPTH #Modifiers(Ob_p_PTH)
	OPG_secretion_by_Ob_m: Ob_m()@bone -> Ob_m()@bone + OPG()@ecm ksecOPGbyObm #Modifiers(Ob_m)
	RANKL_secretion_by_Ocy_A: Ocy_A()@bone -> Ocy_A()@bone + RANKL()@ecm ksecRANKLbyOcy #Modifiers(Ocy_A)
	RANKL_secretion_by_Ocy_I: Ocy_I()@bone -> Ocy_I()@bone + RANKL()@ecm ksecRANKLbyOcyI #Modifiers(Ocy_I)
	RANKL_secretion_by_MSCs: MSC()@bone -> MSC()@bone + RANKL()@ecm ksecRANKLbyMSC #Modifiers(MSC)
	RANKL_secretion_by_Ob_p: Ob_p()@bone -> Ob_p()@bone + RANKL()@ecm ksecRANKLbyObp #Modifiers(Ob_p)
	RANKL_secretion_by_Ob_p_Tgfb_A: Ob_p_Tgfb_A()@bone -> Ob_p_Tgfb_A()@bone + RANKL()@ecm ksecRANKLbyObpTgfb #Modifiers(Ob_p_Tgfb_A)
	RANKL_secretion_by_Ob_p_PTH: Ob_p_PTH()@ecm -> Ob_p_PTH()@ecm + RANKL()@ecm ksecRANKLbyObpPTH #Modifiers(Ob_p_PTH)
	RANKL_secretion_by_Ob_pro: Ob_pro()@bone -> Ob_pro()@bone + RANKL()@ecm ksecRANKLbyObpro #Modifiers(Ob_pro)
	RANKL_secretion_by_Ob_m: Ob_m()@bone -> Ob_m()@bone + RANKL()@ecm ksecRANKLbyObm #Modifiers(Ob_m)
	RANKL_secretion_by_Ob_m_PTH_enhanced: Ob_m_PTH()@ecm -> Ob_m_PTH()@ecm + RANKL()@ecm ksecRANKLbyObmPTH #Modifiers(Ob_m_PTH)
	Tgfb_secretion_by_Obm: Ob_m()@bone -> Ob_m()@bone + Tgfb_I()@ecm ksecTgfb #Modifiers(Ob_m)
	Tgfb_activation: Tgfb_I()@ecm + Ocl_m()@bone -> Tgfb_A()@ecm + Ocl_m()@bone kactTgfb #Modifiers(Tgfb_I, Ocl_m)
	Tgfb_degradation: Tgfb_A()@ecm -> 0  kdegTgfb #Modifiers(Tgfb_A)
	Tgfb_degradation_by_PTH: Tgfb_A()@ecm + Ob_m_PTH()@ecm -> Ob_m_PTH()@ecm kdegTgfbPTH #Modifiers(Tgfb_A, Ob_m_PTH)
	PTH_production: 0  -> PTH()@ecm fRate59() #Modifiers(Source)
	PTH_degradation: PTH()@ecm -> 0  kdegPTH #Modifiers(PTH)
	CREB_activation: Ob_m_PTH()@ecm + CREB()@osteoblast -> Ob_m_PTH()@ecm + CREB_P()@osteoblast fRate61() #Modifiers(CREB, Ob_m_PTH)
	CREB_inactivation: CREB_P()@osteoblast -> CREB()@osteoblast kinactCreb #Modifiers(CREB_P)
	CREB_Runx2_binding: CREB_P()@osteoblast + Runx2()@osteoblast -> CREB_Runx2()@osteoblast kbinCrebRunx2 #Modifiers(CREB_P, Runx2)
	CREB_Runx2_release: CREB_Runx2()@osteoblast -> CREB_P()@osteoblast + Runx2()@osteoblast krelCrebRunx2 #Modifiers(CREB_Runx2)
	Bcl2_synthesis: CREB_Runx2()@osteoblast -> CREB_Runx2()@osteoblast + Bcl2()@osteoblast ksynBcl2 #Modifiers(CREB_Runx2)
	Bcl2_degradation: Bcl2()@osteoblast -> 0  kdegBcl2 #Modifiers(Bcl2)
	Bax_Bcl2_binding: Bax()@osteoblast + Bcl2()@osteoblast -> Bax_Bcl2()@osteoblast kbinBaxBcl2 #Modifiers(Bax, Bcl2)
	Bax_Bcl2_release: Bax_Bcl2()@osteoblast -> Bax()@osteoblast + Bcl2()@osteoblast krelBaxBcl2 #Modifiers(Bax_Bcl2)
	Runx2_synthesis: 0  -> Runx2()@osteoblast fRate69() #Modifiers(Source)
	Runx2_degradation_via_PTH: Ob_m_PTH()@ecm + Runx2()@osteoblast -> Ob_m_PTH()@ecm kdegRunx2PTH #Modifiers(Runx2, Ob_m_PTH)
	Runx2_degradation: Runx2()@osteoblast -> 0  kdegRunx2 #Modifiers(Runx2)
	Bone_formation: Ob_m()@bone -> Ob_m()@bone + Bone()@bone + newbone()@ecm kformBone #Modifiers(Ob_m)
	Bone_formation_Obm_PTH: Ob_m_PTH()@ecm -> Ob_m_PTH()@ecm + Bone()@bone + newbone()@ecm kformBone #Modifiers(Ob_m_PTH)
	Bone_degradation: Ocl_m()@bone + Bone()@bone -> Ocl_m()@bone kdegBone #Modifiers(Ocl_m, Bone)
	Xsynthesis: 0  -> X()@ecm fRate75() #Modifiers(Source)
end reaction rules
end model
