###
begin model
begin parameters
	serA 6.925300000000001e+20 #original 1.15mmol/L
	serC 6.022000000000001e+19 #original 0.1mmol/L
	serB 1.5055e+20 #original 0.25mmol/L
	r1_kcatA 0.550000
	r1_KAp3g 1.200000
	r1_KAphp 0.003200
	r1_KiAser 0.003800
	r2_kcatC 1.750000
	r2_KCphp 0.001500
	r2_KCpser 0.001700
	r3_kcatB 1.430000
	r3_KBpser 0.001500
	r3_KBser 0.150000
end parameters
begin molecule types
	serine()
	phosphohydroxypyruvate()
	s3_phosphoglycerate()
	phosphoserine()
end molecule types
begin seed species
	$s3_phosphoglycerate() 1.4211920000000003e+21 # (2.36 * 0.001)mol/L * 6.022e23/mol *1.0L #s3_phosphoglycerate #p3g
	phosphohydroxypyruvate() 3.6132e+20 #original 0.6mmol/L  #phosphohydroxypyruvate #php
	phosphoserine() 5.4198e+19 #original 0.09mmol/L  #phosphoserine #pser
	$serine() 2.9507800000000007e+21 #original 4.9mmol/L  #serine #ser
end seed species
begin observables
	Species s3_phosphoglycerate s3_phosphoglycerate() #3-phosphoglycerate
	Species phosphohydroxypyruvate phosphohydroxypyruvate() #phosphohydroxypyruvate
	Species phosphoserine phosphoserine() #phosphoserine
	Species serine serine() #serine
end observables
begin functions
	fRate0() = 6.688e-6*serA/(1.2*phosphohydroxypyruvate*serine + 0.00456*phosphohydroxypyruvate + 1.216e-5*s3_phosphoglycerate + 0.0032*serine*(s3_phosphoglycerate + 1.2) + 1.4592e-5)
	fRate1() = 0.002975*serC/(0.0017*phosphohydroxypyruvate + 0.0015*phosphoserine + 2.55e-6)
	fRate2() = 0.2145*serB/(0.15*phosphoserine + 0.0015*serine + 0.000225)
end functions
begin reaction rules
	PDH: s3_phosphoglycerate() -> phosphohydroxypyruvate() fRate0() #Modifiers(serA, ser, serA, p3g, php, ser)
	PSA: phosphohydroxypyruvate() -> phosphoserine() fRate1() #Modifiers(serC, serC, php, pser)
	PSP: phosphoserine() -> serine() fRate2() #Modifiers(serB, serB, pser, ser)
end reaction rules
end model
