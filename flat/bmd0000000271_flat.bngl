###
begin model
begin parameters
	kt 0.0329366 #units:per_minute
	Bmax 516.0 #units:pM
	kon 0.00010496 #units:per_minute_per_pM
	koff 0.0172135 #units:per_minute
	ke 0.0748267 #units:per_minute
	kex 0.00993805 #units:per_minute
	kdi 0.00317871 #units:per_minute
	kde 0.0164042 #units:per_minute
end parameters
begin compartments
	#volume units: L
	medium 3 1.0
	cellsurface 3 1.0
	cell 3 1.0
end compartments
begin molecule types
	Epo_EpoR()
	dEpoi()
	dEpoe()
	EpoR()
	Epo_EpoRi()
	Epo()
end molecule types
begin seed species
	@cellsurface:EpoR() 310735200000000.0 # (516.0 * 1e-12)mol/L * 6.022e23/mol *1.0L #EpoR #EpoR
	@medium:Epo() 1222580418000000.0 #original 2030.19picomole/L #Epo #Epo
end seed species
begin observables
	Species EpoR_cellsurface @cellsurface:EpoR() #EpoR
	Species Epo_medium @medium:Epo() #Epo
	Species Epo_EpoR_cellsurface @cellsurface:Epo_EpoR() #Epo_EpoR
	Species Epo_EpoRi_cell @cell:Epo_EpoRi() #Epo_EpoRi
	Species dEpoi_cell @cell:dEpoi() #dEpoi
	Species dEpoe_medium @medium:dEpoe() #dEpoe
end observables
begin functions
	Epo_cells() = Epo_EpoRi_cell + dEpoi_cell
	Epo_medium_ar() = Epo_medium + dEpoe_medium
	fRate0() = Bmax*kt
end functions
begin reaction rules
	reaction_1: 0  -> EpoR()@cellsurface fRate0()
	reaction_2: EpoR()@cellsurface -> 0  kt
	reaction_3: Epo()@medium + EpoR()@cellsurface -> Epo_EpoR()@cellsurface kon
	reaction_4: Epo_EpoR()@cellsurface -> Epo()@medium + EpoR()@cellsurface koff
	reaction_5: Epo_EpoR()@cellsurface -> Epo_EpoRi()@cell ke
	reaction_6: Epo_EpoRi()@cell -> Epo()@medium + EpoR()@cellsurface kex
	reaction_7: Epo_EpoRi()@cell -> dEpoi()@cell kdi
	reaction_8: Epo_EpoRi()@cell -> dEpoe()@medium kde
end reaction rules
end model
