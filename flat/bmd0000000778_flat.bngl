###
begin model
begin parameters
	r 0.01 #units:unit_0
	K 1000.0 #units:unit_1
	d 0.1 #units:unit_0
	m 50.0 #units:unit_1
	beta 0.1 #units:unit_0
	k 1000.0 #units:unit_1
	a 0.03 #units:unit_0
	alpha 0.01 #units:unit_0
	b 100.0 #units:unit_1
	u 50.0 #units:unit_0
	l1 0
	l2 0
end parameters
begin molecule types
	T()
	C()
	I()
end molecule types
begin seed species
	T() 3.011e+20 # (0.5 * 0.001)mol/L * 6.022e23/mol *1.0L #T #T
	I() 6.022000000000001e+18 #original 0.01substance/volume #I #I
end seed species
begin observables
	Species T T() #T
	Species I I() #I
	Species C C() #C
end observables
begin functions
	T_plot() = T/100
	I_plot() = I/100000
	fRate0() = T*r
	fRate0m() = T*r/K
	fRate1() = C*d/(T + m)
	fRate2() = I*T*beta/(T + k)
	fRate5() = I*T*alpha/(T + b)
end functions
begin reaction rules
	tumor_growth: 0  <-> T() fRate0(),fRate0m()
	tumor_killing: T() -> 0  fRate1() #Modifiers(C)
	immune_cell_growth: 0  -> I() fRate2() #Modifiers(T)
	immune_cell_death: I() -> 0  a
	immunotherapy: 0  -> I() l1
	cytokine_production: 0  -> C() fRate5() #Modifiers(T, I)
	cytokine_decay: C() -> 0  u
	chemotherapy: 0  -> C() l2
end reaction rules
end model
