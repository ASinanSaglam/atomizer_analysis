###
begin model
begin parameters
	statPhosphatase_nuc 3.011e+16 #original 0.05/
	statPhosphatase 3.011e+16 #original 0.05/
	r1_Kf_PstatDimerisation 0.600000
	r1_Kr_PstatDimerisation 0.030000
	r2_Kcat_dephos 1.000000
	r2_Km_dephos 2.000000
	r3_Kcat_phos 1.000000
	r3_Km_phos 4.000000
	r4_Kf_PstatDimerisation 0.600000
	r4_Kr_PstatDimerisation 0.030000
	r5_PstatDimer_impMax 0.045000
	r5_Kpsd_imp 0.300000
	r6_stat_expMax -0.060000
	r6_Ks_exp 0.600000
	r7_stat_impMax 0.003000
	r7_Ks_imp 3.000000
end parameters
begin compartments
	#volume units: 
	nuc 3 1.0
	sol 3 14.625
	nm 2 1.0
end compartments
begin molecule types
	PstatDimer()
	stat()
	species_test()
	Pstat()
end molecule types
begin seed species
	@sol:stat() 8.807175e+18 # (1.0 * 1e-06)mol/L * 6.022e23/mol *14.625L #stat #stat_sol
end seed species
begin observables
	Species stat_sol @sol:stat() #stat_sol
	Species Pstat_sol @sol:Pstat() #Pstat_sol
	Species PstatDimer_sol @sol:PstatDimer() #PstatDimer_sol
	Species PstatDimer_nuc @nuc:PstatDimer() #PstatDimer_nuc
	Species stat_nuc @nuc:stat() #stat_nuc
	Species Pstat_nuc @nuc:Pstat() #Pstat_nuc
	Species species_test_sol @sol:species_test() #species_test
end observables
begin functions
	statKinase_ar() =  if((time()>500&&time()<2502.54614894971),(0.01*sin(0.001571*time()-0.7855)),(0))
	fRate1() = 1.0*statPhosphatase_nuc/(Pstat_nuc + 2.0)
	fRate2() = 1.0*statKinase_ar/(stat_nuc + 4.0)
	fRate4() = 0.045/(PstatDimer_nuc + 0.3)
	fRate5() = -0.06*stat_nuc/(stat_nuc + 0.6)
	fRate6() = 0.003/(stat_nuc + 3.0)
end functions
begin reaction rules
	PstatDimerisation: Pstat()@sol + Pstat()@sol <-> PstatDimer()@sol r1_Kf_PstatDimerisation,r1_Kr_PstatDimerisation
	statDephosphorylation: Pstat()@nuc -> stat()@nuc fRate1() #Modifiers(statPhosphatase_nuc)
	statPhosphorylation: stat()@sol -> Pstat()@sol + species_test()@sol fRate2() #Modifiers(statKinase_sol)
	PstatDimerisationNuc: Pstat()@nuc + Pstat()@nuc <-> PstatDimer()@nuc r4_Kf_PstatDimerisation,r4_Kr_PstatDimerisation
	PstatDimer__import: PstatDimer()@sol -> PstatDimer()@nuc fRate4()
	stat_export_reactants_0: 0  -> stat()@sol -1*(fRate5())
	stat_export_products_0: 0  -> stat()@nuc fRate5()
	stat_import: stat()@sol -> stat()@nuc fRate6()
end reaction rules
end model
