#@notes:'This is a plain translation of an SBML model created on 14/08/2021. The original model has 4 molecules and 4 reactions. The translated model has 3 molecules and 4 rules'
###
begin model
begin parameters
	k1 0.359
	k2 0.147
	k3 0.06
	k4 0.035
	stim_frequency 3.0
	stim_duration 1.0
	ModelValue_17 (1.0)
	ModelValue_13 (3.0)
	stimulus 0 #original 1.0substance/volume
end parameters
begin molecule types
	NFAT_dephosphorylated()
	NFAT_phosphorylated()
	NFAT_transported()
end molecule types
begin seed species
	NFAT_phosphorylated() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #NFAT_phosphorylated #NFAT_phosphorylated
end seed species
begin observables
	Species NFAT_phosphorylated NFAT_phosphorylated() #NFAT_phosphorylated
	Species NFAT_dephosphorylated NFAT_dephosphorylated() #NFAT_dephosphorylated
	Species NFAT_transported NFAT_transported() #NFAT_transported
end observables
begin functions
	NFAT_transported_ar() = -NFAT_dephosphorylated - NFAT_phosphorylated + 1
	tNFAT() = 100*NFAT_transported_ar
	stimulus_ar() =  if((-ModelValue_13*min(rint((time()/ModelValue_13)-0.5),rint((time()/ModelValue_13)+0.5))+time()<ModelValue_17),(1),(0))
	dNFAT() = 100*NFAT_dephosphorylated
	pNFAT() = 100*NFAT_phosphorylated
	fRate0() = k1*stimulus_ar
end functions
begin reaction rules
	Dephosphorylation: NFAT_phosphorylated() -> NFAT_dephosphorylated() fRate0() #Modifiers(stimulus)
	Phosphorylation: NFAT_dephosphorylated() -> NFAT_phosphorylated() k2
	Translocation: NFAT_dephosphorylated() -> NFAT_transported() k3
	Nuclear_export: NFAT_transported() -> NFAT_phosphorylated() k4
end reaction rules
end model
