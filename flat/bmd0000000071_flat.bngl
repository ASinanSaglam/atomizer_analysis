	fRate2m() = 339.2/(Vt*(0.4*Fructose6_phosphate_glycosome + 0.12*Glucose6_phosphate_glycosome + 0.048))
	fRate3() = 578048.780487805*ATPgly__glycosome*Fructose6_phosphate_glycosome/(Vt*(38.4615384615385*ATPgly__glycosome + 1)*(Fructose1_6_bisphosphate_glycosome + 15.8)*(0.0934579439252337*Fructose1_6_bisphosphate_glycosome + 1.21951219512195*Fructose6_phosphate_glycosome + 1))
	fRate4() = (-218462.686567164*DHAPgly__glycosome*Glyceraldehyde3_phosphate_glycosome + 184.5*Fructose1_6_bisphosphate_glycosome/(0.00349451147600472*ADPgly__glycosome + 0.0107695406929895*ATPgly__glycosome + 0.00246575342465753*sumAg + 0.009))/(Vt*(995.02487562189*DHAPgly__glycosome*Glyceraldehyde3_phosphate_glycosome + 66.6666666666667*DHAPgly__glycosome + 111.111111111111*Fructose1_6_bisphosphate_glycosome*Glyceraldehyde3_phosphate_glycosome/(0.0380513471831625*ADPgly__glycosome + 0.11726833199033*ATPgly__glycosome + 0.0268493150684932*sumAg + 0.098) + Fructose1_6_bisphosphate_glycosome/(0.00349451147600472*ADPgly__glycosome + 0.0107695406929895*ATPgly__glycosome + 0.00246575342465753*sumAg + 0.009) + 14.9253731343284*Glyceraldehyde3_phosphate_glycosome + 1))
	fRate5() = 842.0*TPIact*(0.833333333333333*DHAPgly__glycosome - 22.8*Glyceraldehyde3_phosphate_glycosome)/(Vt*(0.833333333333333*DHAPgly__glycosome + 4.0*Glyceraldehyde3_phosphate_glycosome + 1))
	fRate6() = 2.94/(Vt*(0.002*Glyceraldehyde3_phosphate_glycosome*NAD_glycosome + 0.0009*Glyceraldehyde3_phosphate_glycosome + 0.0675*NADH_glycosome*s1_3_bisphosphoglycerate_glycosome + 0.045*NADH_glycosome*(Glyceraldehyde3_phosphate_glycosome + 0.15) + 0.0003*NAD_glycosome + 0.003*s1_3_bisphosphoglycerate_glycosome*(NAD_glycosome + 0.45) + 0.000135))
	fRate6m() = 66.48075/(Vt*(0.002*Glyceraldehyde3_phosphate_glycosome*NAD_glycosome + 0.045*Glyceraldehyde3_phosphate_glycosome*(NADH_glycosome + 0.02) + 0.0675*NADH_glycosome*s1_3_bisphosphoglycerate_glycosome + 0.00675*NADH_glycosome + 0.003*NAD_glycosome*(s1_3_bisphosphoglycerate_glycosome + 0.1) + 0.00135*s1_3_bisphosphoglycerate_glycosome + 0.000135))
