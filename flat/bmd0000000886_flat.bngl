begin model
begin parameters
	alpha 0.18
	alpha_0 4.6
	alpha_1 0.101
	delta_2 0.008
	mu_1 1.5
	mu_10 0.2
	mu_11 0.3
	mu_2 1.4
	mu_20 0.0412
	beta_1 0.3
	beta_2 0.05
	mu_3 0.45
	mu_30 0.03
	gamma_1 0.4
	gamma_2 0.3
	mu_4 0.35
	mu_40 0.3
	delta_1 0.5
end parameters
begin molecule types
	T()
	B()
	Tc()
	Th()
	A()
end molecule types
begin seed species
	T() 5.419800000000001e+21 # (9.0 * 0.001)mol/L * 6.022e23/mol *1.0L #T #T
	Th() 4.2154000000000003e+21 #original 7.0substance/volume  #Th #Th
	Tc() 6.022e+20 #original 1.0substance/volume  #Tc #Tc
	B() 6.022e+20 #original 1.0substance/volume  #B #B
	A() 6.022e+20 #original 1.0substance/volume  #A #A
end seed species
begin observables
	Species T T() #T
	Species Th Th() #Th
	Species Tc Tc() #Tc
	Species B B() #B
	Species A A() #A
end observables
begin functions
	fRate0() = T*alpha
	fRate1() = 1.0*A*T*delta_2 + 1.0*T^2*alpha_0 + 1.0*T*Tc*alpha_1
	fRate2() = 1.0*T*(Th*mu_11 + 1.0*mu_1)
	fRate4() = 1.0*T*Tc*beta_1 + 1.0*T*mu_2 + 1.0*Tc*Th*beta_2
	fRate6() = 1.0*B*T*gamma_1 + 1.0*B*Th*gamma_2 + 1.0*T*mu_3
	fRate8() = B*mu_4
	fRate9() = 1.0*A*T*delta_1 + 1.0*A*mu_40
end functions
begin reaction rules
	Formation_of_tumor_cell: 0  -> T() fRate0()
	Removal_of_tumor_cell_from_the_system_reactants_0: 0  -> T() -1*(fRate1()) #Modifiers(Tc, A)
	Formation_of_T_helper_cell: 0  -> Th() fRate2() #Modifiers(T)
	Removal_of_T_helper_cell_from_the_system: Th() -> 0  mu_10
	formation_of_cytotoxic_T_cell: 0  -> Tc() fRate4() #Modifiers(T, Th)
	Removal_of_cytotoxic_T_cell: Tc() -> 0  mu_20
	Formation_of_B_cell: 0  -> B() fRate6() #Modifiers(T, Th)
	Removal_of_B_cell_from_the_system: B() -> 0  mu_30
	introduction_of_antibody_against_cancer: 0  -> A() fRate8() #Modifiers(B)
	Removal_of_antibody_from_the_system_reactants_0: 0  -> A() -1*(fRate9()) #Modifiers(T)
end reaction rules
end model
