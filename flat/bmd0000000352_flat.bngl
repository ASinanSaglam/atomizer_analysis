#@notes:'This is a plain translation of an SBML model created on 13/08/2021. The original model has 6 molecules and 12 reactions. The translated model has 5 molecules and 12 rules'
###
begin model
begin parameters
	pi_I 1.0
	d_r 0.007
	d_A 0.003
	d_II 0.003
	d_IA 0.003
	k_II 1.0
	k_IA 1.0
	B_d 100.0
	f_A 10.0
	gamma_I 10.0
	K_aux 1.0
	K_II 10.0
	K_IA 10.0
	f_c 10.0
	w_A 10.0
	w_I 10.0
	w_D 10.0
	d_I 0.05
	pi_A 1.0
	k_Am 10.0
	auxin 6.68442e+23 #original 1.11mol/L
end parameters
begin molecule types
	Aux_IAA_Aux_IAA()
	Aux_IAA()
	ARF()
	mRNA()
	Aux_IAA_ARF()
end molecule types
begin seed species
	Aux_IAA() 6.022e+24 # 10.0mol/L * 6.022e23/mol *1.0L #Aux_IAA #I
	ARF() 6.022e+24 #original 10.0mol/L #ARF #A
	Aux_IAA_Aux_IAA() 6.022e+24 #original 10.0mol/L #Aux_IAA_Aux_IAA #D_II
	Aux_IAA_ARF() 6.022e+24 #original 10.0mol/L #Aux_IAA_ARF #D_IA
	mRNA() 6.022e+23 #original 1.0mol/L #mRNA #R
end seed species
begin observables
	Species Aux_IAA Aux_IAA() #Aux/IAA
	Species ARF ARF() #ARF
	Species Aux_IAA_Aux_IAA Aux_IAA_Aux_IAA() #Aux/IAA:Aux/IAA
	Species Aux_IAA_ARF Aux_IAA_ARF() #Aux/IAA:ARF
	Species mRNA mRNA() #mRNA
end observables
begin functions
	aux_basal() = 1/(K_aux*(gamma_I - 1))
	kprime_II() = K_II*k_II
	kprime_IA() = K_IA*k_IA
	auxin_ar() = 1.11*sin(0.007854*time() - 1.5708) + 1.11
	fRate2() = K_aux*auxin_ar*d_I*gamma_I/(K_aux*auxin_ar + 1.0)
	fRate6() = K_aux*auxin_ar*d_I*gamma_I/(K_aux*auxin_ar + 1.0)
	fRate7() = K_aux*auxin_ar*d_I*gamma_I/(K_aux*auxin_ar + 1.0)
	fRate9() = mRNA*pi_I
	fRate11() = 1.0*K_IA*(ARF^2*f_A*f_c*w_A + ARF*B_d*f_c + B_d^2)/(ARF^2*K_IA*w_A + ARF*Aux_IAA*B_d*w_I + ARF*B_d*K_IA + Aux_IAA_ARF*B_d*K_IA*w_D + B_d^2*K_IA*k_Am + B_d^2*K_IA)
end functions
begin reaction rules
	reac_DIA: ARF() + Aux_IAA() <-> Aux_IAA_ARF() k_IA,kprime_IA
	reac_DII: Aux_IAA() + Aux_IAA() <-> Aux_IAA_Aux_IAA() k_II,kprime_II
	reac_degrI: Aux_IAA() -> 0  fRate2() #Modifiers(aux)
	reac_degrA: ARF() -> 0  d_A
	reac_degrDIA: Aux_IAA_ARF() -> 0  d_IA
	reac_degrDII: Aux_IAA_Aux_IAA() -> 0  d_II
	reac_DIAxA: Aux_IAA_ARF() -> ARF() fRate6() #Modifiers(aux)
	reac_DIIxI: Aux_IAA_Aux_IAA() -> Aux_IAA() fRate7() #Modifiers(aux)
	reac_degrR: mRNA() -> 0  d_r
	reac_prodI: 0  -> Aux_IAA() fRate9() #Modifiers(R)
	reac_prodA: 0  -> ARF() pi_A
	reac_prodR: 0  -> mRNA() fRate11() #Modifiers(A, D_IA, I)
end reaction rules
end model
