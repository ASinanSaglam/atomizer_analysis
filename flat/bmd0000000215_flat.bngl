	reaction_4: Tbet_Prot() -> 0  r5_delta_Tbet
	reaction_5: Rec_mRNA() -> 0  r6_gamma_Rec
	reaction_6: Rec_Prot() -> 0  r7_delta_Rec
	reaction_7: Ifn_mRNA() -> 0  r8_gamma_IFN
	reaction_8: Ifn_Prot() -> 0  r9_delta_IFN
	reaction_9: 0  -> Tbet_Prot() fRate9() #Modifiers(Tbet_mRNA)
	reaction_10: 0  -> Ifn_Prot() fRate10() #Modifiers(Ifn_mRNA)
	reaction_11: 0  -> Rec_Prot() fRate11() #Modifiers(Rec_mRNA)
	reaction_12: 0  -> Rec_mRNA() fRate12() #Modifiers(Tbet_Prot, Ag)
	reaction_13: 0  -> Ifn_mRNA() fRate13() #Modifiers(Tbet_Prot, Rec_Prot, Ag)
end reaction rules
end model
