begin functions
	Ag_ar() = 1 - time()^10/(time()^10 + 2064377754059776)
	fRate0() = 0.42*Ag_ar*Ifn_Prot/(Ag_ar*Ifn_Prot + 2.1*Ag_ar + 0.46*Ifn_Prot + 0.966)
	fRate1() = 0.00051*Rec_Prot
	fRate9() = 100.0*Tbet_mRNA
	fRate10() = 100.0*Ifn_mRNA
	fRate11() = 100.0*Rec_mRNA
	fRate12() = 3.64e-5*Tbet_Prot/(Ag_ar + 0.013)
	fRate13() = 3.7*Ag_ar*Rec_Prot*Tbet_Prot/(Ag_ar*Rec_Prot*Tbet_Prot + 0.029*Ag_ar*Rec_Prot + 66.0*Ag_ar*Tbet_Prot + 1.914*Ag_ar + 0.014*Rec_Prot*Tbet_Prot + 0.000406*Rec_Prot + 0.924*Tbet_Prot + 0.026796)
end functions
begin reaction rules
	vtrnldeg: 0  -> Tbet_mRNA() fRate0() #Modifiers(Ag, Ifn_Prot)
	reaction_1: 0  -> Tbet_mRNA() fRate1() #Modifiers(Rec_Prot)
	reaction_2: 0  -> Tbet_mRNA() r3_a1
