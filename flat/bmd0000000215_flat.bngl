begin molecule types
	Tbet_mRNA()
	Rec_Prot()
	Tbet_Prot()
	Ifn_mRNA()
	Rec_mRNA()
	Ifn_Prot()
end molecule types
begin seed species
	Tbet_mRNA() 2.649680000000006e+22 # 0.0440000000000001mol/L * 6.022e23/mol *1.0L #Tbet_mRNA #Tbet_mRNA
	Tbet_Prot() 2.649680000000006e+25 #original 44.0000000000001mol/L #Tbet_Prot #Tbet_Prot
