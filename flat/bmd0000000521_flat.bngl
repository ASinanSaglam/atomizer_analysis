#@notes:'This is a plain translation of an SBML model created on 13/08/2021. The original model has 4 molecules and 0 reactions. The translated model has 4 molecules and 4 rules'
###
begin model
begin parameters
	P0 7.13
	Q0 41.2
	lambda_P 0.121
	k_PQ 0.00295
	k_Qp_P 0.0031
	delta_QP 0.0087
	gamma 0.729
	KDE 0.24
	K 100.0
end parameters
begin compartments
	#volume units: L
	plama 3 1.0 #plasma
	tissue 3 1.0
end compartments
begin molecule types
	damagedquiescentcells()
	Proliferativetissue()
	nonproliferativequiescenttissue()
	PCV_plasma()
end molecule types
begin seed species
	@plama:PCV_plasma() 6.022e+23 # 1.0mol/L * 6.022e23/mol *1.0L #PCV_plasma #C
	@tissue:Proliferativetissue() 4.293686e+24 #original 7.13mol/L #Proliferativetissue #P
	@tissue:nonproliferativequiescenttissue() 2.4810640000000004e+25 #original 41.2mol/L #nonproliferativequiescenttissue #Q
end seed species
begin observables
	Species PCV_plasma_plama @plama:PCV_plasma() #PCV_plasma
	Species Proliferativetissue_tissue @tissue:Proliferativetissue() #Proliferative tissue
	Species nonproliferativequiescenttissue_tissue @tissue:nonproliferativequiescenttissue() #nonproliferative quiescent tissue
	Species damagedquiescentcells_tissue @tissue:damagedquiescentcells() #damaged quiescent cells
end observables
begin functions
	Pstar() = Proliferativetissue_tissue + damagedquiescentcells_tissue + nonproliferativequiescenttissue_tissue
	arRateP() = -KDE*PCV_plasma_plama*Proliferativetissue_tissue*gamma - Proliferativetissue_tissue*k_PQ + Proliferativetissue_tissue*lambda_P*(1 - Pstar/K) + damagedquiescentcells_tissue*k_Qp_P
	arRateQp() = KDE*PCV_plasma_plama*gamma*nonproliferativequiescenttissue_tissue - damagedquiescentcells_tissue*delta_QP - damagedquiescentcells_tissue*k_Qp_P
	arRateQ() = -KDE*PCV_plasma_plama*gamma*nonproliferativequiescenttissue_tissue + k_PQ
	arRateC() = -KDE*PCV_plasma_plama
end functions
begin reaction rules
	0  -> PCV_plasma()@plama arRateC #rateLaw
	0  -> Proliferativetissue()@tissue arRateP #rateLaw
	0  -> nonproliferativequiescenttissue()@tissue arRateQ #rateLaw
	0  -> damagedquiescentcells()@tissue arRateQp #rateLaw
end reaction rules
end model
