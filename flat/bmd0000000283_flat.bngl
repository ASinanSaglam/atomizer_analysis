begin model
begin parameters
	K3 0.5 #units:dimensionless
	K2 0
end parameters
begin molecule types
	P()
	E()
	Q()
	X()
end molecule types
begin seed species
	X() 4.8176e+24 # 8.0mol/L * 6.022e23/mol *1.0L #X #X
	E() 6.022e+23 #original 1.0/  #E #E
end seed species
begin observables
	Species X X() #X
	Species E E() #E
	Species P P() #P
	Species Q Q() #Q
end observables
begin reaction rules
	r1: X() + E() <-> P() 1.00000000000000,K2
	r2: P() -> E() + Q() K3
end reaction rules
end model
