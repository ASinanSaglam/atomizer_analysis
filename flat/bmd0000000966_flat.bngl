begin model
begin parameters
	r3 44100000000.0 #units:unit_0
	r4 0.009 #units:unit_1
	k_1 0.9 #units:unit_1
	k2 0.02 #units:unit_2
	k_2 0.3 #units:unit_1
	Zn 1e-05 #units:unit_3
	k1a 1.0 #units:unit_2
end parameters
begin molecule types
	Qw1()
	Dw()
	Py()
	Rw()
	Qw2()
	Zn2()
	Py1()
end molecule types
begin seed species
	Py() 1.5055000000000002e+16 # (25.0 * 1e-09)mol/L * 6.022e23/mol *1.0L #Py #Py
	Dw() 2408800000000000.5 #original 4.0substance/volume  #Dw #Dw
	Rw() 3.0110000000000004e+16 #original 50.0substance/volume  #Rw #Rw
	Zn2() 602200000000000.1 #original 1.0substance/volume  #Zn2 #Zn_2
end seed species
begin observables
	Species Py Py() #Py
	Species Py1 Py1() #Py1
	Species Dw Dw() #Dw
	Species Rw Rw() #Rw
	Species Qw1 Qw1() #Qw1
	Species Qw2 Qw2() #Qw2
	Species Zn2 Zn2() #Zn^2
end observables
begin functions
	fRate0() = Zn^2*r3
end functions
begin reaction rules
	Formation_of_the_Zn4Zur_complex: Py() -> Py1() fRate0()
	Dissociation_of_Zn4Zur_complex: Py1() -> Zn2() + Py() r4
	Formation_of_DNA_binding_complex: Dw() + Py1() -> Qw2() k1a
	Disassociation_of_DNA_binding_complex: Qw2() -> Dw() + Py1() k_1
	Formation_of_transcription_initiation_complex: Dw() + Rw() -> Qw1() k2
	Disassociation_of_transcription_initiation_complex: Qw1() -> Dw() + Rw() k_2
end reaction rules
end model
