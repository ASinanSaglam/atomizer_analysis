begin model
begin parameters
	pEGFR_scaleFactor 0.000181735 #units:ng
	pAkt_scaleFactor 60.0588 #units:ng
	pS6_scaleFactor 49886.2 #units:ng
	EGF_conc_step 30.0 #units:ng_per_ml
	EGFR_turnover 0.000106386 #units:per_sec
	EGF_binding_kf 0.00673816 #units:ml_per_ng_per_sec
	EGF_binding_kb 0.040749 #units:per_sec
	inhibitor_binding_kf 2.43466029020655e-05 #units:per_conc_per_sec
	inhibitor_binding_kb 5.25096686262403e-05 #units:per_sec
	pulse_time 60.0 #units:time
	ramp_time 3600.0 #units:time
	EGF_conc_pulse 0
	EGF_conc_ramp 0
	r2_k1 0.000016
	r2_k2 0.005175
	r3_k1 0.052814
	r4_k1 0.099719
	r5_k1 0.000002
	r5_k2 0.000000
	r6_k1 0.001215
	r7_k1 0.032796
	r8_k1 0.001131
	r10_k1 0.019239
end parameters
begin molecule types
	EGF()
	EGFR_i()
	pEGFR_Akt()
	EGF_EGFR_i()
	pAkt()
	EGF_EGFR()
	Akt()
	pEGFR()
	pS6()
	pAkt_S6()
	pro_EGFR()
	EGFR()
	Inhibitor()
	S6()
end molecule types
begin seed species
	EGF() 0 # 30.0mol/L * 6.022e23/mol *1.0L #EGF #EGF
	Akt() 2.6080679800000003e+22 #original 0.043309arbitrary_amount/ml #Akt #Akt
	S6() 2.133696974e+24 #original 3.54317arbitrary_amount/ml #S6 #S6
	$pro_EGFR() 4.1064138440000125e+28 #original 68190.2000000002arbitrary_amount/ml #pro_EGFR #pro_EGFR
	EGFR()() (68190.2000000002) / (1 + (2.43466029020655e-05) * (0.0) / ((5.25096686262403e-05) + (0.000106386))) #EGFR
	EGFR_i()() (68190.2000000002) -(68190.2000000002) #EGFR_i
end seed species
begin observables
	Species EGF EGF() #EGF
	Species EGFR EGFR() #EGFR
	Species pEGFR pEGFR() #pEGFR
	Species pEGFR_Akt pEGFR_Akt() #pEGFR_Akt
	Species Akt Akt() #Akt
	Species pAkt pAkt() #pAkt
	Species S6 S6() #S6
	Species pAkt_S6 pAkt_S6() #pAkt_S6
	Species pS6 pS6() #pS6
	Species pro_EGFR pro_EGFR() #pro_EGFR
	Species EGF_EGFR EGF_EGFR() #EGF_EGFR
	Species EGFR_i EGFR_i() #EGFR_i
	Species EGF_EGFR_i EGF_EGFR_i() #EGF_EGFR_i
	Species Inhibitor Inhibitor() #Inhibitor
end observables
begin functions
	pS6_total() = pS6*pS6_scaleFactor
	EGF_ar() =  EGF_conc_ramp*time()/ramp_time+EGF_conc_step+if((time()<=pulse_time),(EGF_conc_pulse),(0))
	pEGFR_total() = pEGFR_scaleFactor*(pEGFR + pEGFR_Akt)
	pAkt_total() = pAkt_scaleFactor*(pAkt + pAkt_S6)
end functions
begin reaction rules
	reaction_1: EGF() + EGFR() <-> EGF_EGFR() EGF_binding_kf,EGF_binding_kb
	reaction_2: pEGFR() + Akt() <-> pEGFR_Akt() r2_k1,r2_k2
	reaction_3: pEGFR_Akt() -> pEGFR() + pAkt() r3_k1
	reaction_4: pEGFR() -> 0  r4_k1
	reaction_5: pAkt() + S6() <-> pAkt_S6() r5_k1,r5_k2
	reaction_6: pAkt_S6() -> pAkt() + pS6() r6_k1
	reaction_7: pAkt() -> Akt() r7_k1
	reaction_8: pS6() -> S6() r8_k1
	reaction_9: pro_EGFR() -> EGFR() EGFR_turnover
	reaction_10: EGF_EGFR() -> pEGFR() r10_k1
	reaction_11: EGFR() -> 0  EGFR_turnover
	reaction_12: Inhibitor() + EGFR() <-> EGFR_i() inhibitor_binding_kf,inhibitor_binding_kb
	reaction_13: Inhibitor() + EGF_EGFR() <-> EGF_EGFR_i() inhibitor_binding_kf,inhibitor_binding_kb
	reaction_14: EGF() + EGFR_i() <-> EGF_EGFR_i() EGF_binding_kf,EGF_binding_kb
	reaction_15: EGFR_i() -> 0  EGFR_turnover
end reaction rules
end model
