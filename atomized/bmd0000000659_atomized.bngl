begin model
begin parameters
	numTissuePos 0.5 #units:unit_0
	numCaInputBaseline 0.754 #units:unit_0
	numCaInputAmp 0.092 #units:unit_0
	numCaMInputBaseline 0.363 #units:unit_0
	numCaMInputAmp 0.485 #units:unit_0
	numHillCoeff 1.4 #units:unit_0
	numHillWeight 1.0 #units:unit_0
	numHillMax 1.0 #units:unit_0
	numHillTau 1.0 #units:unit_0
	numHillEC50 0.5 #units:unit_0
	numMEKCytoToNucParam 0.05 #units:unit_0
	numMEKNucToCytoParam 0.5 #units:unit_0
	numERKCytoToNucParam 0.01 #units:unit_0
	numERKNucToCytoParam 0.01 #units:unit_0
	numHillBeta 2.56 #units:unit_0
	numHillK 1.37 #units:unit_0
	numTotalRafInputs 0
	Ca_c 4.5959904e+20 #original 0.7632substance/L
	Ca 4.5959904e+20 #original 0.7632substance/L
	CaM_c 4.3764885000000006e+20 #original 0.72675substance/L
	CaM 4.3764885000000006e+20 #original 0.72675substance/L
end parameters
begin compartments
	#volume units: L
	c 3 1.0 #cytoplasm
	n 3 0.5 #nucleus
end compartments
begin molecule types
	Rafc()
	MEKn()
	ERKc()
	ERKn()
	MEKc()
end molecule types
begin seed species
	@c:Rafc() 3.011e+20 # (0.5 * 0.001)mol/L * 6.022e23/mol *1.0L #Rafc #pRaf_cyto
	@c:MEKc() 4.8176000000000007e+20 #original 0.8substance/L #MEKc #pMEK_cyto
	@c:ERKc() 4.2154e+20 #original 0.7substance/L #ERKc #pERK_cyto
	@n:MEKn() 1.2044000000000002e+20 #original 0.4substance/L #MEKn #pMEK_nuc
	@n:ERKn() 6.022000000000001e+19 #original 0.2substance/L #ERKn #pERK_nuc
end seed species
begin observables
	Species Rafc_c @c:Rafc() #Rafc
	Species MEKc_c @c:MEKc() #MEKc
	Species ERKc_c @c:ERKc() #ERKc
	Species MEKn_n @n:MEKn() #MEKn
	Species ERKn_n @n:ERKn() #ERKn
end observables
begin functions
	funcHillMEKToERKNuc() = MEKn_n^numHillCoeff*numHillBeta*numHillWeight/(MEKn_n^numHillCoeff + numHillK^numHillCoeff)
	numCytoToNucVolRatio() = 5*numTissuePos/7 + 2
	funcHillERKToERKNuc() = ERKn_n^numHillCoeff*numHillBeta*numHillWeight/(ERKn_n^numHillCoeff + numHillK^numHillCoeff)
	arRatepERK_nuc() = (ERKc_c*numCytoToNucVolRatio*numERKNucToCytoParam - ERKn_n*numERKCytoToNucParam - ERKn_n + numHillMax*(-funcHillERKToERKNuc + funcHillMEKToERKNuc))/numHillTau
	funcHillMEKToERKCyto() = MEKc_c^numHillCoeff*numHillBeta*numHillWeight/(MEKc_c^numHillCoeff + numHillK^numHillCoeff)
	arRatepERK_cyto() = (-ERKc_c*numERKCytoToNucParam - ERKc_c + ERKn_n*numERKNucToCytoParam/numCytoToNucVolRatio + funcHillMEKToERKCyto*numHillMax)/numHillTau
	funcHillRafToMEK() = Rafc_c^numHillCoeff*numHillBeta*numHillWeight/(Rafc_c^numHillCoeff + numHillK^numHillCoeff)
	arRatepMEK_cyto() = (-MEKc_c*numMEKCytoToNucParam - MEKc_c + MEKn_n*numMEKNucToCytoParam/numCytoToNucVolRatio + funcHillRafToMEK*numHillMax)/numHillTau
	arRatepMEK_nuc() = (MEKc_c*numCytoToNucVolRatio*numMEKCytoToNucParam - MEKn_n*numMEKNucToCytoParam - MEKn_n)/numHillTau
	arRatepRaf_cyto() = (-Rafc_c + numHillMax*numTotalRafInputs)/numHillTau
	funcHillERKToRaf() = ERKn_n^numHillCoeff*numHillBeta*numHillWeight/(ERKn_n^numHillCoeff + numHillK^numHillCoeff)
	funcHillCaMToRaf() = CaM_c^numHillCoeff*numHillBeta*numHillWeight/(CaM_c^numHillCoeff + numHillK^numHillCoeff)
	funcHillCaToRaf() = Ca_c^numHillCoeff*numHillBeta*numHillWeight/(Ca_c^numHillCoeff + numHillK^numHillCoeff)
end functions
begin reaction rules
	0  -> Rafc()@c arRatepRaf_cyto #rateLaw
	0  -> MEKc()@c arRatepMEK_cyto #rateLaw
	0  -> ERKc()@c arRatepERK_cyto #rateLaw
	0  -> MEKn()@n arRatepMEK_nuc #rateLaw
	0  -> ERKn()@n arRatepERK_nuc #rateLaw
end reaction rules
end model
