	#^ bqbiol:encodes uniprot:Q6ZRP7
	SOX2_Gene()
	#^ bqbiol:encodes uniprot:Q8N7R0
	#^ bqbiol:isVersionOf ensembl:ENSG00000176654
	NANOG_Gene()
