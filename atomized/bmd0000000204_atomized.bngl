begin observables
	Species OCT4_Gene OCT4_Gene() #OCT4_Gene
	Species NANOG_Gene NANOG_Gene() #NANOG_Gene
	Species SOX2_Gene SOX2_Gene() #SOX2_Gene
	Species targetGene targetGene() #targetGene
	Species degradation OCT4(degradation~degradation,sox2) #degradation
	Species OCT4 OCT4(degradation~0,sox2) #OCT4
	Species SOX2 SOX2(oct4) #SOX2
	Species NANOG NANOG() #NANOG
	Species OCT4_SOX2 OCT4(degradation~0,sox2!350).SOX2(oct4!350) #OCT4_SOX2
	Species Protein Protein() #Protein
end observables
begin functions
	fRate0() = (A*a1 + NANOG*OCT4_SOX2*a3 + OCT4_SOX2*a2 + eta1)/(A*b1 + NANOG*OCT4_SOX2*b3 + OCT4_SOX2*b2 + eta1/f + 1)
	fRate2() = (NANOG*OCT4_SOX2*e2 + OCT4_SOX2*e1 + eta5)/(NANOG*OCT4_SOX2*f1 + OCT4_SOX2*f2 + eta5/f + f3*p53 + 1)
	fRate6() = (A*c1 + NANOG*OCT4_SOX2*c3 + OCT4_SOX2*c2 + eta3)/(A*d1 + NANOG*OCT4_SOX2*d3 + OCT4_SOX2*d2 + eta3/f + 1)
	fRate8() = (OCT4_SOX2*g1 + eta7)/(NANOG*OCT4_SOX2*h2 + OCT4_SOX2*h1 + eta7/f2 + 1)
end functions
begin reaction rules
	J0_reactants_0: 0  -> OCT4_Gene() -1*(fRate0()) #Modifiers(A, OCT4_SOX2, NANOG)
	J0_products_0: 0  -> OCT4(degradation~0,sox2) fRate0() #Modifiers(A, OCT4_SOX2, NANOG)
	J1: OCT4(degradation~0,sox2) -> OCT4(degradation~degradation,sox2) gamma1
	J2_reactants_0: 0  -> NANOG_Gene() -1*(fRate2()) #Modifiers(OCT4_SOX2, p53)
	J2_products_0: 0  -> NANOG() fRate2() #Modifiers(OCT4_SOX2, p53)
	J3: NANOG() -> OCT4(degradation~degradation,sox2) gamma2
	J4: OCT4(degradation~0,sox2) + SOX2(oct4) <-> OCT4(degradation~0,sox2!350).SOX2(oct4!350) k1c,k2c
	J5: OCT4(degradation~0,sox2!350).SOX2(oct4!350) -> OCT4(degradation~degradation,sox2) k3c
	J6_reactants_0: 0  -> SOX2_Gene() -1*(fRate6()) #Modifiers(A, OCT4_SOX2, NANOG)
	J6_products_0: 0  -> SOX2(oct4) fRate6() #Modifiers(A, OCT4_SOX2, NANOG)
	J7: SOX2(oct4) -> OCT4(degradation~degradation,sox2) gamma3
	J8_reactants_0: 0  -> targetGene() -1*(fRate8()) #Modifiers(OCT4_SOX2, NANOG)
	J8_products_0: 0  -> Protein() fRate8() #Modifiers(OCT4_SOX2, NANOG)
	J9: Protein() -> OCT4(degradation~degradation,sox2) gamma4
end reaction rules
