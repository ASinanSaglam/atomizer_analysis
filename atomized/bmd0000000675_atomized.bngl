begin functions
	Clb5_ar() = -Clb5_Sic1 + Clb5_T
	Cln3_ar() = Cln3_max*Dn3*mass/(Dn3*mass + Jn3)
	Clb2_ar() = -Clb2_Sic1 + Clb2_T
	Bck2_ar() = Bck2_0*mass
	Vd2_c1() = kd2_c1*(Bck2_ar*epsilonc1_k2 + Clb2_ar*epsilonc1_b2 + Clb5_ar*epsilonc1_b5 + Cln2 + Cln3_ar*epsilonc1_n3)
	Vd_b5() = Cdc20*kd_b5_ + kd_b5
	Sic1_ar() = -Clb2_Sic1 - Clb5_Sic1 + Sic1_T
	arRateClb5_Sic1() = -Clb5_Sic1*(Vd2_c1/(Jd2_c1 + Sic1_T) + Vd_b5 + kd1_c1 + kdi_b5) + Clb5_ar*Sic1_ar*kas_b5
	Vd_b2() = Cdc20*kd_b2__ + Hct1*kd_b2_ + kd_b2*(-Hct1 + Hct1_T)
	arRateClb2_Sic1() = -Clb2_Sic1*(Vd2_c1/(Jd2_c1 + Sic1_T) + Vd_b2 + kd1_c1 + kdi_b2) + Clb2_ar*Sic1_ar*kas_b2
	Va_sbf() = ka_sbf*(Clb5_ar*epsilonsbf_b5 + Cln2 + epsilonsbf_n3*(Bck2_ar + Cln3_ar))
	Vi_t1() = ki_t1 + ki_t1_*(Clb2_ar*epsiloni_t1_b2 + Clb5_ar*epsiloni_t1_b5 + Cln2*epsiloni_t1_n2 + Cln3_ar)
	arRateBUD() = -BUD*kd_bud + ks_bud*(Clb5_ar*epsilonbud_b5 + Cln2 + Cln3_ar)
	arRateORI() = -ORI*kd_ori + ks_ori*(Clb2_ar*epsilonori_b2 + Clb5_ar)
	Mcm1_ar() = 2*Clb2_ar*Ji_mcm*ka_mcm/(Clb2_ar*Ji_mcm*ka_mcm - Clb2_ar*ka_mcm + Ja_mcm*ki_mcm + ki_mcm + sqrt(-Clb2_ar*Ji_mcm*ka_mcm*(-4*Clb2_ar*ka_mcm + 4*ki_mcm) + (Clb2_ar*Ji_mcm*ka_mcm - Clb2_ar*ka_mcm + Ja_mcm*ki_mcm + ki_mcm)^2))
	arRateClb2_T() = -Clb2_T*Vd_b2 + mass*(Mcm1_ar*ks_b2_ + ks_b2)
	SBF_ar() = 2*Ji_sbf*Va_sbf/(Clb2_ar*ki_sbf_ + Ja_sbf*(Clb2_ar*ki_sbf_ + ki_sbf) + Ji_sbf*Va_sbf - Va_sbf + ki_sbf + sqrt(-4*Ji_sbf*Va_sbf*(Clb2_ar*ki_sbf_ - Va_sbf + ki_sbf) + (Clb2_ar*ki_sbf_ + Ja_sbf*(Clb2_ar*ki_sbf_ + ki_sbf) + Ji_sbf*Va_sbf - Va_sbf + ki_sbf)^2))
	MBF_ar() = SBF_ar
	arRateClb5_T() = -Clb5_T*Vd_b5 + mass*(MBF_ar*ks_b5_ + ks_b5)
	Swi5_ar() = 2*Cdc20*Ji_swi*ka_swi/(Cdc20*Ji_swi*ka_swi - Cdc20*ka_swi + Clb2_ar*ki_swi_ + Ja_swi*(Clb2_ar*ki_swi_ + ki_swi) + ki_swi + sqrt(-Cdc20*Ji_swi*ka_swi*(-4*Cdc20*ka_swi + 4*Clb2_ar*ki_swi_ + 4*ki_swi) + (Cdc20*Ji_swi*ka_swi - Cdc20*ka_swi + Clb2_ar*ki_swi_ + Ja_swi*(Clb2_ar*ki_swi_ + ki_swi) + ki_swi)^2))
	arRateSic1_T() = -Sic1_T*(Vd2_c1/(Jd2_c1 + Sic1_T) + kd1_c1) + Swi5_ar*ks_c1_ + ks_c1
	arRateSPN() = Clb2_ar*ks_spn/(Clb2_ar + J_spn) - SPN*kd_spn
	arRateHct1() = -Hct1*Vi_t1/(Hct1 + Ji_t1) + (-Hct1 + Hct1_T)*(Cdc20*ka_t1_ + ka_t1)/(-Hct1 + Hct1_T + Ja_t1)
	arRateCdc20_T() = -Cdc20_T*kd_20 + Clb2_ar*ks_20_ + ks_20
	Vi_20() =  if((time()>=START_S&&time()<END_M),(10),(if((time()>=END_M&&time()<END_M+12),(0.825*END_M-0.825*time()+10),(0.1))))
	arRateCdc20() = -Cdc20*(Vi_20 + kd_20) + ka_20*(-Cdc20 + Cdc20_T)
	arRateCln2() = -Cln2*kd_n2 + mass*(SBF_ar*ks_n2_ + ks_n2)
	arRatemass() = mass*mu
	D() = -32 + 1.026/mu
end functions
