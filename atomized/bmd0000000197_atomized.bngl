end observables
begin functions
	intracellularBSP_ar() = boundintracellularBSP_cell + freeintracellularBSP_cell
	totalBSP_ar() = apicalBSP_apical + basolateralboundBSP_basolat + boundintracellularBSP_cell + freebasolateralBSP_basolat + freeintracellularBSP_cell
	fRate5() = p6*p8
	fRate5m() = freebasolateralBSP_basolat*p6
	fRate7() = p11*p9
	fRate7m() = freeintracellularBSP_cell*p9
