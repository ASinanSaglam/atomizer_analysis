	Species freebasolateralBSP_basolat @basolat:basolateral(boundbsp~0,freebsp~FREEBSP)@basolat #free basolateral BSP
	Species basolateralboundBSP_basolat @basolat:basolateral(boundbsp~BOUNDBSP,freebsp~0)@basolat #basolateral bound BSP
