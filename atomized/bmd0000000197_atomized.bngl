	ABCC2: intracellularBSP(bound~0,free~FREE)@cell -> apicalBSP()@apical p2
	endo_in_bl: basolateral(boundbsp~0,freebsp~FREEBSP)@basolat -> intracellularBSP(bound~0,free~FREE)@cell p3
	endo_ex_bl: intracellularBSP(bound~0,free~FREE)@cell -> basolateral(boundbsp~0,freebsp~FREEBSP)@basolat p4
