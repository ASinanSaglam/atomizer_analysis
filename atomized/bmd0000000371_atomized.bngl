begin model
begin parameters
	tau_membrane 20.0
	g_Ca 3.6
	V_Ca 25.0
	V_m -20.0
	theta_m 12.0
	V_K -75.0
	g_K 10.0
	V_n -17.0
	theta_n 5.6
	lamda 0.8
	tau_potassium_current_n_gate 20.0
	g_s 4.0
	V_s -22.0
	theta_s 8.0
	tau_s 20000.0
	g_K_ATP 1.2
	p 0.5
end parameters
begin molecule types
	s()
	V_membrane()
	n()
end molecule types
begin seed species
	n() 0.05 #n #n
	s() 0.025 #s #s
end seed species
begin observables
	Species V_membrane V_membrane() #V_membrane
	Species n n() #n
	Species s s() #s
end observables
begin functions
	i_s() = g_s*s*(-V_K + V_membrane)
	m_infinity() = 1/(exp((V_m - V_membrane)/theta_m) + 1)
	i_Ca() = g_Ca*m_infinity*(-V_Ca + V_membrane)
	i_K() = g_K*n*(-V_K + V_membrane)
	i_K_ATP() = g_K_ATP*p*(-V_K + V_membrane)
	arRateV_membrane() = (-i_Ca - i_K - i_K_ATP - i_s)/tau_membrane
	s_infinity() = 1/(exp((-V_membrane + V_s)/theta_s) + 1)
	arRates() = (-s + s_infinity)/tau_s
	n_infinity() = 1/(exp((-V_membrane + V_n)/theta_n) + 1)
	arRaten() = lamda*(-n + n_infinity)/tau_potassium_current_n_gate
end functions
begin reaction rules
	0  -> V_membrane() arRateV_membrane #rateLaw
	0  -> n() arRaten #rateLaw
	0  -> s() arRates #rateLaw
end reaction rules
end model
