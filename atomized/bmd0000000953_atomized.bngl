	fRate1() = clb2*(cdc20*kd_clb2_p + cdh1*kd_clb2_pp + kd_clb2)
	fRate3() = cdc20*(cdh1*kd_cdc20_p + kd_cdc20)
	fRate4() = (-cdh1 + cdh1_total)*(cdc14_ar*kd_cdh_p + kd_cdh)/(J_cdh - cdh1 + cdh1_total)
	fRate5() = cdk_ar*kp_cdh_p/(J_cdh + cdh1)
	fRate7() = securin_total*(cdc20*kd_securin_p + kd_securin)
	fRate10() = la_securin*securin_ar*separase_ar
