	fRate12() = net1p_ar*(cdc14_ar*kd_p + kd*pp2a_ar)/(Jnet + net1p_ar)
	fRate13() = net1*(cdk_ar*kp + kp_p*men)/(Jnet + net1 + net1cdc14)
	fRate15() = net1cdc14*(cdk_ar*kp + kp_p*men)/(Jnet + net1 + net1cdc14)
	fRate17() = polo_total*(cdh1*kd_polo_p + kd_polo)
	fRate18() = (-polo + polo_total)*(cdk_ar*ka_polo_p + ka_polo)/(J_polo - polo + polo_total)
	fRate19() = ki_polo/(J_polo + polo)
	fRate20() = polo*(cdh1*kd_polo_p + kd_polo)
	fRate21() = (ka_tem + ka_tem_p*polo)*(-tem1 + tem1_total)/(J_tem1 - tem1 + tem1_total)
	fRate22() = tem1*(ki_tem + kitem_p*pp2a_ar)/(J_tem1 + tem1)
	fRate23() = (-cdc15 + cdc15_total)*(cdc14_ar*ka_cdc15_p + ka_cdc15)/(J_cdc15 - cdc15 + cdc15_total)
	fRate24() = cdc15*(cdk_ar*ki_cdc15_p + ki_cdc15)/(J_cdc15 + cdc15)
	fRate25() = la_men*(cdc15 - men)*(-men + tem1)
	fRate27() = men*(ki_tem + kitem_p*pp2a_ar)/(J_tem1 + tem1)
	fRate28() = men*(cdk_ar*ki_cdc15_p + ki_cdc15)/(J_cdc15 + cdc15)
end functions
