###
begin model
begin parameters
	kprod 1.86e-05
	kdeg 2.1e-05
	kdimer 1.1783e-07
	kdedimer 8.4655e-06
	kdisagg 5.4357e-05
	kpf 2.785e-06
	kpg 0.00574
	kpghalf 4.0
	kdegNep 1.8e-10
	Source 1.0
	Sink 1.0
end parameters
begin molecule types
	#^ bqbiol:isVersionOf chebi:CHEBI:64645, bto:BTO:0002774
	AbetaPlaque(AbetaMonomer~AbetaMonomer~0,abetaplaque)
	#^ bqbiol:isVersionOf uniprot:P08473
	Neprilysin()
end molecule types
begin seed species
	Neprilysin() 1000.0 #Neprilysin #Nep
end seed species
begin observables
	Species AbetaMonomer AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) #AbetaMonomer
	Species AbetaDimer AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804).AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804) #AbetaDimer
	Species AbetaPlaque AbetaPlaque(AbetaMonomer~0,abetaplaque) #AbetaPlaque
	Species Neprilysin Neprilysin() #Neprilysin
end observables
begin functions
	fRate0() = Source*kprod
	fRate1() = 0.001*kdeg
	fRate2() = 0.5*AbetaMonomer*kdimer*(AbetaMonomer - 1)
	fRate4() = 0.5*AbetaDimer*kpf*(AbetaDimer - 1)
	fRate5() = AbetaPlaque*kpg/(AbetaPlaque^2 + kpghalf^2)
end functions
begin reaction rules
	Abetaproduction: 0  -> AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) fRate0() #Modifiers(Source)
	AbetaDegradation: AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) + Neprilysin() -> Neprilysin() fRate1() #Modifiers(Abeta, Nep)
	AbetaDimerisation_reactants_0: 0  -> AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) -1*2.0*(fRate2()) #Modifiers(Abeta)
	AbetaDimerisation_products_0: 0  -> AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804).AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804) fRate2() #Modifiers(Abeta)
	AbetaDedimerisation: AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804).AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804) -> AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) + AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) kdedimer #Modifiers(AbDim)
	AbetaPlaqueFormation_reactants_0: 0  -> AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804).AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque!804) -1*2.0*(fRate4()) #Modifiers(AbDim)
	AbetaPlaqueFormation_products_0: 0  -> AbetaPlaque(AbetaMonomer~0,abetaplaque) 4.0*fRate4() #Modifiers(AbDim)
	AbetaPlaqueGrowth: AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) + AbetaPlaque(AbetaMonomer~0,abetaplaque) -> AbetaPlaque(AbetaMonomer~0,abetaplaque) + AbetaPlaque(AbetaMonomer~0,abetaplaque) fRate5() #Modifiers(Abeta, AbP)
	AbetaDisaggregation: AbetaPlaque(AbetaMonomer~0,abetaplaque) -> AbetaPlaque(AbetaMonomer~AbetaMonomer,abetaplaque) kdisagg #Modifiers(AbP)
	NepDegradation: Neprilysin() -> 0  kdegNep #Modifiers(Nep)
end reaction rules
end model
