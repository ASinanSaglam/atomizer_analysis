###
begin model
begin parameters
	A 0.025
	N 1.0
	I 0.01
	K_l 17.857
	delta_l 0.1
	K_m 25.0
	xi 2.0
	S 62.5
end parameters
begin molecule types
	mutatedcell()
	macrophage()
	normalcell()
end molecule types
begin seed species
	macrophage() 6.02199999999991e+22 # 0.0999999999999985mol/L * 6.022e23/mol *1.0L #macrophage #l
	normalcell() 5.4197999999999284e+23 #original 0.899999999999988mol/L  #normalcell #n
	mutatedcell() 5.4197999999999284e+23 #original 0.899999999999988mol/L  #mutatedcell #m
end seed species
begin observables
	Species macrophage macrophage() #macrophage
	Species normalcell normalcell() #normal cell
	Species mutatedcell mutatedcell() #mutated cell
end observables
begin functions
	arRatem() = -K_m*macrophage*mutatedcell^2 + mutatedcell*xi*(N + 1)/(N + macrophage + mutatedcell + normalcell) - mutatedcell
	arRaten() = normalcell*(N + 1)/(N + macrophage + mutatedcell + normalcell) - normalcell
	arRatel() = A*macrophage*mutatedcell*(N + 1)/(N + macrophage + mutatedcell + normalcell) + I*(S*mutatedcell + 1) - K_l*macrophage*mutatedcell^2 - delta_l*macrophage
end functions
begin reaction rules
	0  -> macrophage() arRatel #rateLaw
	0  -> normalcell() arRaten #rateLaw
	0  -> mutatedcell() arRatem #rateLaw
end reaction rules
end model
