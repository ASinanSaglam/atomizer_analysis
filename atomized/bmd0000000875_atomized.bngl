begin model
begin parameters
	k 3.43e-08 #units:unit_0
	delta 0.5 #units:unit_1
	N 480.0 #units:unit_2
	c 3.0 #units:unit_1
	np 0.5 #units:unit_2
	lambda 10.0 #units:unit_4
	delta1 0.03 #units:unit_1
end parameters
begin molecule types
	V_NI()
	#^ bqbiol:hasProperty ncit:C14283
	V_I()
	#^ bmbiol:hasInstance efo:0001460
	T(m~M~0)
end molecule types
begin seed species
	T(m~0) 1.08396e+29 # 180000.0mol/L * 6.022e23/mol *1.0L #T #T
	T(m~M) 1.008685e+27 #original 1675.0substance/volume #Tm #T_i
	V_I() 8.06948e+28 #original 134000.0substance/volume #V_I #V_I
end seed species
begin observables
	Species T T(m~0) #T
	Species Tm T(m~M) #T*
	Species V_I V_I() #V_I
	Species V_NI V_NI() #V_NI
end observables
begin functions
	HIV() = V_I + V_NI
	fRate2() = V_I*k
	fRate4() = N*Tm*delta*(1 - np)
	fRate5() = N*Tm*delta*np
end functions
begin reaction rules
	T_cell_source: 0  -> T(m~0) lambda
	T_cell_loss: T(m~0) -> 0  delta1
	infection: T(m~0) -> T(m~M) fRate2() #Modifiers(V_I)
	T_i_lysis: T(m~M) -> 0  delta
	T_lysis_infect_products_0: 0  -> V_I() fRate4() #Modifiers(T_i)
	T_lysis_noninf: 0  -> V_NI() fRate5() #Modifiers(T_i)
	V_I_clearance: V_I() -> 0  c
	V_NI_clearance: V_NI() -> 0  c
end reaction rules
end model
