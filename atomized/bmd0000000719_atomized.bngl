begin model
begin parameters
	k_synth 1.5
	k_dest 0.4
	ec50_cdc25 30.0
	n_cdc25 11.0
	ec50_wee1 35.0
	n_wee1 3.5
	k_cdk1_on 0.0354
	k_cdk1_off 0.0354
	p 5.0
	ec50_plx 60.0
	ec50_apc 0.5
	n_apc 4.0
	n_plx 5.0
	k_plxon 1.5
	k_plx_off 0.125
	k_apc_on 1.5
	k_apc_off 0.15
	tau 4.0
	Plx1_total 6.022e+20 #original 1.0substance/volume
	APC_Ctotal 6.022e+20 #original 1.0substance/volume
	CyclinB1Cdk1complextotal 3.6132e+22 #original 60.0substance/volume
end parameters
begin molecule types
	#^ bqbiol:isVersionOf uniprot:P70032
	#^ bqbiol:hasProperty pato:PATO:0002354
	Plx1_active()
	#^ bqbiol:hasPart uniprot:P13350, uniprot:P35567
	#^ bqbiol:is go:GO:0032991
	CyclinB1Cdk1complexunphosphorylated()
	#^ bqbiol:hasPart uniprot:P13350, uniprot:P35567
	#^ bqbiol:is go:GO:0032991
	CyclinB1Cdk1complexphosphorylated()
	#^ bqbiol:hasPart uniprot:Q6GQ63
	#^ bqbiol:hasProperty pato:PATO:0002354
	APC_Cactive()
end molecule types
begin seed species
	CyclinB1Cdk1complexphosphorylated() 3.6132e+22 # (60.0 * 0.001)mol/L * 6.022e23/mol *1.0L #CyclinB1Cdk1complexphosphorylated #Cyclin_B1_Cdk1_complex_phosphorylated
	APC_Cactive() 6.022e+20 #original 1.0substance/volume #APC_Cactive #APC_C_active
end seed species
begin observables
	Species CyclinB1Cdk1complexphosphorylated CyclinB1Cdk1complexphosphorylated() #Cyclin B1 Cdk1 complex phosphorylated
	Species CyclinB1Cdk1complexunphosphorylated CyclinB1Cdk1complexunphosphorylated() #Cyclin B1 Cdk1 complex unphosphorylated
	Species Plx1_active Plx1_active() #Plx1_active
	Species APC_Cactive APC_Cactive() #APC/C active
end observables
begin functions
	r() = 1/(2*(1 + 3/(exp((70 - time())/tau) + 1))^2)
	CyclinB1Cdk1complextotal_ar() = CyclinB1Cdk1complexphosphorylated + CyclinB1Cdk1complexunphosphorylated
	fRate1() = APC_Cactive*k_dest
	fRate2() = APC_Cactive*k_dest
	fRate3() = CyclinB1Cdk1complexphosphorylated*k_cdk1_off*sqrt(r)*(p/((CyclinB1Cdk1complexphosphorylated/ec50_wee1)^n_wee1 + 1) + 1)
	fRate4() = CyclinB1Cdk1complexunphosphorylated*k_cdk1_on*(p/((ec50_cdc25/CyclinB1Cdk1complexphosphorylated)^n_cdc25 + 1) + 1)/sqrt(r)
	fRate5() = Plx1_total*k_plxon/((ec50_plx/CyclinB1Cdk1complexphosphorylated)^n_plx + 1.0)
	fRate5m() = k_plxon/((ec50_plx/CyclinB1Cdk1complexphosphorylated)^n_plx + 1.0)
	fRate7() = APC_Ctotal*k_apc_on/((ec50_apc/Plx1_active)^n_apc + 1.0)
	fRate7m() = k_apc_on/((ec50_apc/Plx1_active)^n_apc + 1.0)
end functions
begin reaction rules
	Cyclin_B1_Cdk1_phosphorylated_synthesis: 0  -> CyclinB1Cdk1complexphosphorylated() k_synth
	Cyclin_B1_Cdk1_unphosphorylated_degradation: CyclinB1Cdk1complexunphosphorylated() -> 0  fRate1() #Modifiers(APC_C_active)
	Cyclin_B1_Cdk1_phosphorylated_degradation: CyclinB1Cdk1complexphosphorylated() -> 0  fRate2() #Modifiers(APC_C_active)
	Cyclin_B1_Cdk1_phosphorylated_dephosphorylation_reactants_0: 0  -> CyclinB1Cdk1complexphosphorylated() -1*(fRate3())
	Cyclin_B1_Cdk1_phosphorylated_dephosphorylation_products_0: 0  -> CyclinB1Cdk1complexunphosphorylated() fRate3()
	Cyclin_B1_Cdk1_phosphorylated_phosphorylation_reactants_0: 0  -> CyclinB1Cdk1complexunphosphorylated() -1*(fRate4())
	Cyclin_B1_Cdk1_phosphorylated_phosphorylation_products_0: 0  -> CyclinB1Cdk1complexphosphorylated() fRate4()
	Plx1_activation_by_active_Cdk1: 0  <-> Plx1_active() fRate5(),fRate5m() #Modifiers(Cyclin_B1_Cdk1_complex_phosphorylated, Plx1_total)
	Plx1_inactivation_after_Cdk1_inactivated: Plx1_active() -> 0  k_plx_off
	Activation_of_APC_CCdc20_by_active_Plx1: 0  <-> APC_Cactive() fRate7(),fRate7m() #Modifiers(Plx1_active, APC_C_total)
	inactivation_of_APC_CCdc20_after_Plx1_is_inactivated: APC_Cactive() -> 0  k_apc_off
end reaction rules
end model
