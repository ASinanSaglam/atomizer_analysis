###
begin model
begin parameters
	epsilon 0.1
	delta 0.9
	a1 0.05
	a2 0.009
	a3 0.675
	a4 0.01
	a5 0.005
	b1 0.1
	b2 0.3
	b3 0.01
	k1 0.1
	k2 0.5
	k3 0.025
end parameters
begin molecule types
	activeosteoclasts()
	activeosteoblasts()
	PTH()
end molecule types
begin seed species
	PTH() 1.2044e+24 # 2.0mol/L * 6.022e23/mol *1.0L #PTH #x
	activeosteoclasts() 6.022e+23 #original 1.0mol/L  #activeosteoclasts #y
	activeosteoblasts() 9.033e+22 #original 0.15mol/L  #activeosteoblasts #z
end seed species
begin observables
	Species PTH PTH() #PTH
	Species activeosteoclasts activeosteoclasts() #active osteoclasts
	Species activeosteoblasts activeosteoblasts() #active osteoblasts
end observables
begin functions
	arRatez() = delta*epsilon*(PTH*a4 - PTH*a5*activeosteoblasts/(PTH + k3) - activeosteoblasts*b3)
	arRatey() = epsilon*(activeosteoblasts*activeosteoclasts*(PTH*a3 + a2)/(PTH^2 + k2) - activeosteoclasts*b2)
	arRatex() = -PTH*b1 + a1/(activeosteoclasts + k1)
end functions
begin reaction rules
	0  -> PTH() arRatex #rateLaw
	0  -> activeosteoclasts() arRatey #rateLaw
	0  -> activeosteoblasts() arRatez #rateLaw
end reaction rules
end model
