###
begin model
begin parameters
	C_s 0.005
	D_A 0.7
	d_B 0.7
	D_C 0.0021
	D_R 0.0007
	f0 0.05
	K 10.0
	k1 0.01
	k2 10.0
	k3 0.00058
	k4 0.017
	k5 0.02
	k6 3.0
	k_B 0.189
	K_L_P 3000000.0
	kO 0.35
	K_O_P 200000.0
	k_P 86.0
	r_L 1000.0
	S_P 250.0
	I_L 0
	I_O 0
	I_P 0
end parameters
begin molecule types
	Responding_Osteoblasts()
	Active_Osteoblasts()
	Active_Osteoclasts()
end molecule types
begin seed species
	Responding_Osteoblasts() 4.6574148000000003e+20 # 0.0007734mol/L * 6.022e23/mol *1.0L #Responding_Osteoblasts #R
	Active_Osteoblasts() 4.3852204e+20 #original 0.0007282mol/L #Active_Osteoblasts #B
	Active_Osteoclasts() 5.4962794000000005e+20 #original 0.0009127mol/L #Active_Osteoclasts #C
end seed species
begin observables
	Species Responding_Osteoblasts Responding_Osteoblasts() #Responding_Osteoblasts
	Species Active_Osteoblasts Active_Osteoblasts() #Active_Osteoblasts
	Species Active_Osteoclasts Active_Osteoclasts() #Active_Osteoclasts
end observables
begin functions
	P_S() = k6/k5
	Pbar() = I_P/k_P
	P_O() = S_P/k_P
	Phi_P() = (P_O + Pbar)/(P_S + Pbar)
	Phi_L() = Active_Osteoblasts*K_L_P*Phi_P*k3*(I_L/r_L + 1)/(k4*(K*k3/k4 + k1*(I_O + K_O_P*Responding_Osteoblasts/Phi_P)/(k2*kO) + 1))
	Phi_C() = (Active_Osteoclasts + C_s*f0)/(Active_Osteoclasts + C_s)
	arRateC() = -Active_Osteoclasts*D_A*Phi_C + D_C*Phi_L
	D_B() = d_B*f0
	arRateB() = -Active_Osteoblasts*k_B + D_B*Responding_Osteoblasts/Phi_C
	arRateR() = -D_B*Responding_Osteoblasts/Phi_C + D_R*Phi_C
end functions
begin reaction rules
	0  -> Responding_Osteoblasts() arRateR #rateLaw
	0  -> Active_Osteoblasts() arRateB #rateLaw
	0  -> Active_Osteoclasts() arRateC #rateLaw
end reaction rules
end model
