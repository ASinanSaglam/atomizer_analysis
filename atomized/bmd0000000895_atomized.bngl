begin functions
	fRate1() = Mr*gbMr
	fRate1m() = -Mr*gbMr/(ccMr*(p1 - 1))
	fRate2() = Se*vrecMr/(Se + kmrecMr)
	fRate3() = CD4*daMa/(CD4 + cdaMa)
	fRate4() = iMr/(Se + cSeMri)
	fRate5() = Se*aMr/(Se + cSeMr)
	fRate9() = CD4^(ngbCD4 + 1)*gbCD4/(CD4^ngbCD4 + k2CD4^ngbCD4)
	fRate9m() = CD4^(ngbCD4 + 1)*gbCD4/(cc1CD4*(CD4^ngbCD4 + k2CD4^ngbCD4))
	fRate10() = Se*vrecCD4/(Se + kmrecCD4)
	fRate12() = CD4n*Se^ndCD4/(Se^ndCD4 + kSedCD4^ndCD4)
	fRate15() = CD8^2*gbCD8/(CD8 + k2CD8)
	fRate15m() = CD8^2*gbCD8/(cc1CD8*(CD8 + k2CD8))
	fRate16() = CD4^ncompCD4*Se*compCD8/(CD4^ncompCD4*Se + CD4^ncompCD4*w1 + Se*kcompCD4^ncompCD4 + kcompCD4^ncompCD4*w1)
	fRate20() = Mi*Si^mMi*bMi/(Si^mMi + (Mi*N)^mMi)
	fRate21() = lMi*(CD4 + CD8/Mi)*(-Si*apop/(Mi + N + Si) + 1)/(CD4 + CD8/Mi + cCD4CD8)
	fRate23() = Se*pSe
	fRate23m() = Se*pSe/ccSe
	fRate26() = Si*pSi
	fRate26m() = Si*pSi/(Mi*N + Si)
	fRate27() = Mr*Si*iMr/(Si + cSeMr)
	fRate28() = lMi*(CD4 + CD8/Mi)/(CD4 + CD8/Mi + cCD4CD8)
end functions
