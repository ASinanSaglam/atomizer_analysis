	CD8_Competition: CD8() -> 0  fRate16() #Modifiers(Se, CD4)
	CD8_Death: CD8() -> 0  drCD8
	Ma_Death: Ma() -> 0  drMa
	Se_Killing_Ma: Ma() + Se(mr) -> 0  kSeMa
	Mi_Bursting_reactants_0: 0  -> Si() -1*(fRate20())
	Mi_Bursting_reactants_1: 0  -> Mr(rec~0,se!92).Se(mr!92) -1*(fRate20())
	Mi_Bursting_products_0: 0  -> Se(mr) fRate20()
	Mi_Lysis_reactants_0: 0  -> Mr(rec~0,se!92).Se(mr!92) -1*(fRate21()) #Modifiers(CD4, CD8, Si)
	Mi_Death: Mr(rec~0,se!92).Se(mr!92) -> 0  drMi
	Se_Proliferation: 0  <-> Se(mr) fRate23(),fRate23m()
	Se_Death: Se(mr) -> 0  drSe
	Si_Death: Si() -> 0  drSi
	Si_Proliferation: 0  <-> Si() fRate26(),fRate26m() #Modifiers(Mi)
	Mr_Infection_Si: 0  -> Si() fRate27() #Modifiers(Mr)
	Mi_Lysis_Si_reactants_0: 0  -> Si() -1*(fRate28()) #Modifiers(CD4, CD8, Mi)
end reaction rules
