	Mr_Recruitment: Mr(rec~REC,se) -> Mr(rec~0,se) fRate2() #Modifiers(Se)
	Ma_Deactivation: Ma() -> Mr(rec~0,se) fRate3() #Modifiers(CD4)
	Mr_Infection: Mr(rec~0,se) + Se(mr) -> Mr(rec~0,se!92).Se(mr!92) fRate4()
	Mr_Activation: Mr(rec~0,se) -> Ma() fRate5() #Modifiers(Se)
	Se_Killing_Mr: Mr(rec~0,se) + Se(mr) -> 0  kSeMr
