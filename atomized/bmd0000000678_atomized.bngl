begin model
begin parameters
	k1 0.359
	k2 0.147
	k3 0.06
	k4 0.035
	stim_frequency 3.0
	stim_duration 1.0
	ModelValue_17 (1.0)
	ModelValue_13 (3.0)
	stimulus 0 #original 1.0substance/volume
end parameters
begin molecule types
	#^ bqbiol:isVersionOf uniprot:Q12968, pr:PR:000003112
	#^ bqbiol:occursIn go:GO:0005634
	NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated~0,de~DE~0)
end molecule types
begin seed species
	NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~0) 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #NFAT_phosphorylated #NFAT_phosphorylated
end seed species
begin observables
	Species NFAT_phosphorylated NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~0) #NFAT_phosphorylated
	Species NFAT_dephosphorylated NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~DE) #NFAT_dephosphorylated
	Species NFAT_transported NFAT_transported(NFAT_phosphorylated~0,de~0) #NFAT_transported
end observables
begin functions
	NFAT_transported_ar() = -NFAT_dephosphorylated - NFAT_phosphorylated + 1
	tNFAT() = 100*NFAT_transported_ar
	stimulus_ar() =  if((-ModelValue_13*min(rint((time()/ModelValue_13)-0.5),rint((time()/ModelValue_13)+0.5))+time()<ModelValue_17),(1),(0))
	dNFAT() = 100*NFAT_dephosphorylated
	pNFAT() = 100*NFAT_phosphorylated
	fRate0() = k1*stimulus_ar
end functions
begin reaction rules
	Dephosphorylation: NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~0) -> NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~DE) fRate0() #Modifiers(stimulus)
	Phosphorylation: NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~DE) -> NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~0) k2
	Translocation: NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~DE) -> NFAT_transported(NFAT_phosphorylated~0,de~0) k3
	Nuclear_export: NFAT_transported(NFAT_phosphorylated~0,de~0) -> NFAT_transported(NFAT_phosphorylated~NFAT_phosphorylated,de~0) k4
end reaction rules
end model
