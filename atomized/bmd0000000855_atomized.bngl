begin model
begin parameters
	kpn 35.03
	kpm 34.8
	Ninf 0.11
	kptn 2.03
	kptm 3.16
	mupt 0.37
	knp 17.58
	knpt 2.97
	snr 0.38
	munr 3.3
	mun 1.02
	knm 6.42
	kmp 18.52
	kmm 0.06
	kmn 10.58
	kmpt 41.5
	smr 0.17
	mumr 0.54
	mum 0.5
	ENinf 6.8
	ken 5.37
	EMinf 5.0
	kem 4.97
	Cn 175.72
	Cm1 72.84
	Cm2 13.06
	kpg 14.4
	pinf 20.0
	kbp 0.2
	sb 0.12
	mub 0.048
	kpb 14.4
	C 0
	E 0
end parameters
begin molecule types
	#^ bqbiol:is ncit:C80324
	P()
	#^ bqbiol:is cl:CL:0000775
	N(Pt~Pt~0)
	#^ bqbiol:is cl:CL:0000235
	M()
end molecule types
begin seed species
	P() 6.022000000000001e+22 # 0.1mol/L * 6.022e23/mol *1.0L #P #P
	N(Pt~Pt) 1.2044e+24 #original 2.0substance/volume #Pt #Pt
	N(Pt~0) 6.022000000000001e+22 #original 0.1substance/volume #N #N
end seed species
begin observables
	Species P P() #P
	Species Pt N(Pt~Pt) #Pt
	Species M M() #M
	Species N N(Pt~0) #N
end observables
begin functions
	R2() = (P*knp + Pt*knpt)/(C^2/Cn^2 + 1)
	fEN() = R2*snr/((R2 + munr)*(E^2/ENinf^2 + 1))
	fEM_Kmm_E() = kmm/(E^2/EMinf^2 + 1)
	R1() = (M*fEM_Kmm_E + N*kmn + P*kmp + Pt*kmpt)/(C^2/Cm2^2 + 1)
	fi2() = M/(C^2/Cm1^2 + N^2/Ninf^2 + 1)
	fRate0() = P*kpg
	fRate0m() = P*kpg/pinf
	fRate1() = kpb*sb/(P*kbp + mub)
	fRate2() = N*P*kpn*(E*ken + 1)
	fRate3() = P*fi2*kpm*(E*kem + 1)
	fRate4() = N*Pt*kptn*(N*ken + 1)
	fRate5() = Pt*fi2*kptm*(E*kem + 1)
	fRate8() = R1*smr/(R1 + mumr)
	fRate11() = fi2*knm
end functions
begin reaction rules
	Pathogen_Growth: 0  <-> P() fRate0(),fRate0m()
	Pathogen_Destruction_Background_Response: P() -> 0  fRate1()
	Pathogen_Destruction_Neutrophils_reactants_0: 0  -> P() -1*(fRate2()) #Modifiers(N)
	Pathogen_Destruction_Macrophages_reactants_0: 0  -> P() -1*(fRate3())
	Debris_Destruction_Neutrophils_reactants_0: 0  -> N(Pt~Pt) -1*(fRate4()) #Modifiers(N)
	Debris_Destruction_Macrophages_reactants_0: 0  -> N(Pt~Pt) -1*(fRate5())
	Debris_Neutrophil_Decay: N(Pt~0) -> N(Pt~Pt) mun
	Debris_Decay_Intrinsic: N(Pt~Pt) -> 0  mupt
	Macrophage_Recruitment: 0  -> M() fRate8()
	Macrophage_Activation: M() -> 0  mum
	Neutrophil_Activation: 0  -> N(Pt~0) fEN
	Neutrophil_Destruction_Macrophage: N(Pt~0) -> 0  fRate11()
end reaction rules
end model
