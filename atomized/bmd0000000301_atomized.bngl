	k_pT3 3.006
	n3 0.8892
	km3 7.9075
	d_mGFP 0.07
	s0_pT7 0.0003
	s_pT7k 0.0766
	s0_pGFP 0.1007
	s_pGFPk 0.9923
	d_pT7 0.0056
	d_pGFP 0.003
	s0_mT3cr 0.0003
	k_pT7 3.8009
	n7 2.602
	km7 3.0455
	d_mT3 0.0701
	s_pT3k 0.0115
	d_pT3 0.0069
	pulse_interval 20.0
	pulse1_start 0.01
	pulse1_length 11.0
	pulse_conc 0.01
	pulse2_length 11.0
	pulse3_length 22.0
