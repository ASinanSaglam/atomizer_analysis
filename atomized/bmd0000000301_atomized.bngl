	pT7()
	#^ bqbiol:encodes interpro:IPR000786
	#^ bqbiol:isVersionOf chebi:CHEBI:33699
	mGFPcr()
	#^ bqbiol:isVersionOf uniprot:P07659
	#^ bqbiol:isPartOf go:GO:0000428
	pT3()
	#^ bqbiol:hasProperty go:GO:0045975
	#^ bqbiol:isVersionOf chebi:CHEBI:33697
	taRNA()
	#^ bqbiol:encodes uniprot:P00573
	#^ bqbiol:isVersionOf chebi:CHEBI:33699
	mT7cr()
	#^ bqbiol:is kegg.compound:C00259, chebi:CHEBI:17535
	ara()
	#^ bqbiol:isHomologTo uniprot:P42212
	#^ bqbiol:isVersionOf interpro:IPR000786
	pGFP()
end molecule types
begin seed species
	taRNA() 4.0931180972294e+21 # 0.006796941377mol/L * 6.022e23/mol *1.0L #taRNA #taRNA
	mT7cr() 2.1494957506178e+23 #original 0.3569405099mol/L #mT7cr #mT7cr
	mGFPcr() 1.0658417832380001e+23 #original 0.176991329mol/L #mGFPcr #mGFPcr
	pT7() 3.1499544053464e+22 #original 0.05230744612mol/L #pT7 #pT7
	pGFP() 3.8172983351982e+24 #original 6.338921181mol/L #pGFP #pGFP
	pT3() 3.864160828e+19 #original 6.41674e-05mol/L #pT3 #pT3
	mT3cr() 3.4110896360000004e+21 #original 0.00566438mol/L #mT3cr #mT3cr
end seed species
begin observables
	Species taRNA taRNA() #taRNA
	Species mT7cr mT7cr() #mT7cr
	Species mGFPcr mGFPcr() #mGFPcr
	Species pT7 pT7() #pT7
	Species pGFP pGFP() #pGFP
	Species ara ara() #ara
	Species pT3 pT3() #pT3
	Species mT3cr mT3cr() #mT3cr
end observables
begin functions
	pulse2_start() = pulse1_length + pulse1_start + pulse_interval
	pulse3_start() = pulse2_length + pulse2_start + pulse_interval
	fRate0 =  if((pulse_flag==1&&ara>0),(cAra),(ara*dAra))
	fRate1() = (ara*sT + s0_taRNA*(ara + k_ara))/(ara + k_ara)
	fRate5() = (k_pT3*pT3^n3 + s0_mGFPcr*(km3^n3 + pT3^n3))/(km3^n3 + pT3^n3)
	fRate7() = mT7cr*(s0_pT7 + s_pT7k*taRNA)
	fRate8() = mGFPcr*(s0_pGFP + s_pGFPk*taRNA)
	fRate11() = (k_pT7*pT7^n7 + s0_mT3cr*(km7^n7 + pT7^n7))/(km7^n7 + pT7^n7)
	fRate13() = mT3cr*(s0_pT3 + s_pT3k*taRNA)
end functions
begin reaction rules
	r0_reactants_0: 0  -> ara() -1*(fRate0())
	r1a: 0  -> taRNA() fRate1() #Modifiers(ara)
	r1b: taRNA() -> 0  d_taRNA
	r2a: 0  -> mT7cr() s0_mT7cr
	r2b: mT7cr() -> 0  d_mT7
	r3a: 0  -> mGFPcr() fRate5() #Modifiers(pT3)
	r3b: mGFPcr() -> 0  d_mGFP
	r6: 0  -> pT7() fRate7() #Modifiers(taRNA, mT7cr)
	r7: 0  -> pGFP() fRate8() #Modifiers(taRNA, mGFPcr)
	r8: pT7() -> 0  d_pT7
	r9: pGFP() -> 0  d_pGFP
	r10a: 0  -> mT3cr() fRate11() #Modifiers(pT7)
	r10b: mT3cr() -> 0  d_mT3
	r11: 0  -> pT3() fRate13() #Modifiers(taRNA, mT3cr)
	r12: pT3() -> 0  d_pT3
end reaction rules
end model
