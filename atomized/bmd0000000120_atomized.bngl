begin model
begin parameters
	n1 1.0 #units:sec_inv_item_inv
	k1 0.01 #units:sec_inv
	m1 1.0
	d0 0.15 #units:sec_inv
	d1 0.15 #units:sec_inv
	k2 0.01 #units:sec_inv
	m2 1.0 #units:sec_inv_item_inv
	n2 0.02 #units:sec_inv
	n 1.95 #units:dimensionless
	d2 0
	r_l 0
end parameters
begin molecule types
	#^ bqbiol:isVersionOf uniprot:P06239
	Activelck(in~IN~0)
	#^ bqbiol:isVersionOf uniprot:P29350
	Activephosphatase(in~IN~0)
end molecule types
begin seed species
	Activephosphatase(in~IN) 0.6 #Inactivephosphatase #phosphatase_inactive
	Activephosphatase(in~0) 0.6 #Activephosphatase #phosphatase_active
end seed species
begin observables
	Species Inactivelck Activelck(in~IN) #Inactive lck
	Species Activelck Activelck(in~0) #Active lck
	Species Inactivephosphatase Activephosphatase(in~IN) #Inactive phosphatase
	Species Activephosphatase Activephosphatase(in~0) #Active phosphatase
end observables
begin functions
	Totalkinase_ar() = Activelck + Inactivelck
	fRate1() = Activephosphatase*n1
	fRate3() = Activelck^n*m1
	fRate7() = Activelck*m2
end functions
begin reaction rules
	v1: 0  -> Activelck(in~IN) r_l
	v2: Activelck(in~0) -> Activelck(in~IN) fRate1() #Modifiers(phosphatase_active)
	v3: Activelck(in~IN) -> Activelck(in~0) k1
	v4: Activelck(in~IN) -> Activelck(in~0) fRate3()
	v5: Activelck(in~IN) -> 0  d0
	v6: Activelck(in~0) -> 0  d1
	v7: Activephosphatase(in~IN) -> Activephosphatase(in~0) k2
	v8: Activephosphatase(in~IN) -> Activephosphatase(in~0) fRate7() #Modifiers(lck_active)
	v9: Activephosphatase(in~0) -> Activephosphatase(in~IN) n2
	v10: Activephosphatase(in~0) -> 0  d2
end reaction rules
end model
