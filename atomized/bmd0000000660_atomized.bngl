begin seed species
	MrnaP21() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #MrnaP21 #MrnaP21
	MrnaCy() 6.022e+20 #original 1.0substance/volume #MrnaCy #MrnaCy
	MrnaP53() 6.022e+20 #original 1.0substance/volume #MrnaP53 #MrnaP53
	P21(apcna,cy) 4.33584e+20 #original 0.72substance/volume #P21 #P21
	Cy(p21) 2.4088000000000003e+20 #original 0.4substance/volume #Cy #Cy
	aPcna(p21,rc) 3.011e+20 #original 0.5substance/volume #aPcna #aPcna
	Rc(apcna,p~0) 6.022e+20 #original 1.0substance/volume #Rc #Rc
end seed species
begin observables
	Species MrnaP21 MrnaP21() #MrnaP21
	Species MrnaCy MrnaCy() #MrnaCy
	Species MrnaP53 MrnaP53() #MrnaP53
	Species P21 P21(apcna,cy) #P21
	Species Cy Cy(p21) #Cy
	Species CyP21 P21(apcna,cy!290).Cy(p21!290) #CyP21
	Species aPcna aPcna(p21,rc) #aPcna
	Species Rc Rc(apcna,p~0) #Rc
	Species pRc Rc(apcna,p~P) #pRc
	Species aRc aPcna(p21,rc!292).Rc(apcna!292,p~P) #aRc
	Species iRc P21(apcna!291,cy).aPcna(p21!291,rc!292).Rc(apcna!292,p~P) #iRc
