begin functions
	tPcna_ar() = aPcna + aRc + iPcna + iRc
	tCy_ar() = Cy + CyP21
	tP21_ar() = CyP21 + P21 + iPcna + iRc
	fRate8() = P21*(Cdt2*aRc*kDeP21aRc + Cy*Skp2*kDeP21Cy + kDeP21)
	fRate10() = Cy*(Cy*Skp2*kDeCyCy + kDeCy)
	fRate12() = CyP21*(Cy*Skp2*kDeCyCy + kDeCy)
	fRate13() = CyP21*(Cdt2*aRc*kDeP21aRc + Cy*Skp2*kDeP21Cy + kDeP21)
	fRate18() = iPcna*(Cdt2*aRc*kDeP21aRc + Cy*Skp2*kDeP21Cy + kDeP21)
	fRate19() = Cy^n*kPhRc/(Cy^n + jCy^n)
	fRate24() = iRc*(Cdt2*aRc*kDeP21aRc + Cy*Skp2*kDeP21Cy + kDeP21)
	fRate26 =   if(( ( Dna )   <1),(0),(if(( ( Dna )   >1),(1* ( Rc )   ),(0.5* ( Rc )   )))) 
	fRate27 =   if(( ( Dna )   <1),(0),(if(( ( Dna )   >1),(1* ( pRc )   ),(0.5* ( pRc )   )))) 
	fRate28 =   if(( ( Dna )   <1),(0),(if(( ( Dna )   >1),(1* ( aRc )   ),(0.5* ( aRc )   )))) 
	fRate29 =   if(( ( Dna )   <1),(0),(if(( ( Dna )   >1),(1* ( iRc )   ),(0.5* ( iRc )   )))) 
	fRate31() = kDeP53/(Dam + jP53)
	fRate34() = Dam*(P53*kReDamP53/(Dam + jDam) + kReDam)
end functions
begin reaction rules
	Synthesis_of_p21_mRNAs: 0  -> MrnaP21() kSyMrna
	Synthesis_of_p21_mRNAs_by_p53: P53() -> MrnaP21() + P53() kSyMrnaP53
	Synthesis_of_cyclin_mRNAs: 0  -> MrnaCy() kSyMrna
	Synthesis_of_p53_mRNAs: 0  -> MrnaP53() kSyMrna
	Degradation_of_p21_mRNAs: MrnaP21() -> 0  kDeMrna
	Degradation_of_cyclin_mRNAs: MrnaCy() -> 0  kDeMrna
	Degradation_of_p53_mRNAs: MrnaP53() -> 0  kDeMrna
	Synthesis_of_p21: MrnaP21() -> MrnaP21() + P21(apcna,cy) kSyP21
	Degradation_of_p21_reactants_0: 0  -> P21(apcna,cy) -1*(fRate8()) #Modifiers(Skp2, Cy, Cdt2, aRc)
	Synthesis_of_cyclins: MrnaCy() -> MrnaCy() + Cy(p21) kSyCy
	Degradation_of_cyclins_reactants_0: 0  -> Cy(p21) -1*(fRate10()) #Modifiers(Skp2, Cy)
	Association_and_dissociation_of_CDK2_Cyclin_and_p21: Cy(p21) + P21(apcna,cy) <-> P21(apcna,cy!290).Cy(p21!290) kAsCyP21,kDsCyP21
	Degradation_of_cyclin_in_CDK2_Cyclin_p21_complexes_reactants_0: 0  -> P21(apcna,cy!290).Cy(p21!290) -1*(fRate12()) #Modifiers(Skp2, Cy)
	Degradation_of_cyclin_in_CDK2_Cyclin_p21_complexes_products_0: 0  -> P21(apcna,cy) fRate12() #Modifiers(Skp2, Cy)
	Degradation_of_p21_in_CDK2_Cyclin_p21_complexes_reactants_0: 0  -> P21(apcna,cy!290).Cy(p21!290) -1*(fRate13()) #Modifiers(Skp2, Cy, Cdt2, aRc)
	Degradation_of_p21_in_CDK2_Cyclin_p21_complexes_products_0: 0  -> Cy(p21) fRate13() #Modifiers(Skp2, Cy, Cdt2, aRc)
	Import_of_active_PCNA: 0  -> aPcna(p21,rc) kImPc
	Export_of_active_PCNA: aPcna(p21,rc) -> 0  kExPc
	Export_of_inactive_PCNA: aPcna(p21!291,rc).P21(apcna!291,cy) -> P21(apcna,cy) kExPc
	Association_and_dissociation_of_PCNA_and_p21: aPcna(p21,rc) + P21(apcna,cy) <-> aPcna(p21!291,rc).P21(apcna!291,cy) kAsPcP21,kDsPcP21
	Degradation_of_p21_in_PCNA_p21_complexes_reactants_0: 0  -> aPcna(p21!291,rc).P21(apcna!291,cy) -1*(fRate18()) #Modifiers(Skp2, Cy, Cdt2, aRc)
