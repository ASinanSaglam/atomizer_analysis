	Phosphorylation_priming_of_replication_complexes: Rc(apcna,p~0) -> Rc(apcna,p~P) fRate19() #Modifiers(Cy)
	Dephosphorylation_of_replication_complexes: Rc(apcna,p~P) -> Rc(apcna,p~0) kDpRc
	Association_and_dissociation_of_active_PCNA_and_replication_complexes: aPcna(p21,rc) + Rc(apcna,p~P) <-> aPcna(p21,rc!292).Rc(apcna!292,p~P) kAsRcPc,kDsRcPc
	Association_and_dissociation_of_inactive_PCNA_and_replication_complexes: aPcna(p21!291,rc).P21(apcna!291,cy) + Rc(apcna,p~P) <-> P21(apcna!291,cy).aPcna(p21!291,rc!292).Rc(apcna!292,p~P) kAsRcPc,kDsRcPc
	Association_and_dissociation_of_p21_and_replication_complexes: aPcna(p21,rc!292).Rc(apcna!292,p~P) + P21(apcna,cy) <-> P21(apcna!291,cy).aPcna(p21!291,rc!292).Rc(apcna!292,p~P) kAsPcP21,kDsPcP21
	Degradation_of_p21_in_inactive_replication_complexes_reactants_0: 0  -> P21(apcna!291,cy).aPcna(p21!291,rc!292).Rc(apcna!292,p~P) -1*(fRate24()) #Modifiers(Skp2, Cy, Cdt2, aRc)
	Degradation_of_p21_in_inactive_replication_complexes_products_0: 0  -> aPcna(p21,rc!292).Rc(apcna!292,p~P) fRate24() #Modifiers(Skp2, Cy, Cdt2, aRc)
	Synthesis_of_DNA: aPcna(p21,rc!292).Rc(apcna!292,p~P) -> aPcna(p21,rc!292).Rc(apcna!292,p~P) + Dna() kSyDna
	Dissassembly_of_RC_reactants_0: 0  -> Rc(apcna,p~0) -1*(fRate26()) #Modifiers(Dna)
	Dissassembly_of_pRC_reactants_0: 0  -> Rc(apcna,p~P) -1*(fRate27()) #Modifiers(Dna)
	Dissassembly_of_aRC_reactants_0: 0  -> aPcna(p21,rc!292).Rc(apcna!292,p~P) -1*(fRate28()) #Modifiers(Dna)
	Dissassembly_of_aRC_products_0: 0  -> aPcna(p21,rc) fRate28() #Modifiers(Dna)
	Dissassembly_of_iRC_reactants_0: 0  -> P21(apcna!291,cy).aPcna(p21!291,rc!292).Rc(apcna!292,p~P) -1*(fRate29()) #Modifiers(Dna)
	Dissassembly_of_iRC_products_0: 0  -> aPcna(p21!291,rc).P21(apcna!291,cy) fRate29() #Modifiers(Dna)
	Synthesis_of_p53: MrnaP53() -> MrnaP53() + P53() kSyP53
