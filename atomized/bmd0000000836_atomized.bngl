begin model
begin parameters
	alpha 6.0
	beta 2.8
	delta 1.0
	C 10.0
	gamma 0
end parameters
begin molecule types
	protectionplprevention_intensity()
	panic_intensity()
end molecule types
begin seed species
	panic_intensity() 6.022000000000001e+22 # 0.1mol/L * 6.022e23/mol *1.0L #panic_intensity #S
	protectionplprevention_intensity() 6.022000000000001e+22 #original 0.1mol/L  #protectionplprevention_intensity #P
end seed species
begin observables
	Species panic_intensity panic_intensity() #panic_intensity
	Species protectionplprevention_intensity protectionplprevention_intensity() #protection+prevention_intensity
end observables
begin functions
	arRateP() = protectionplprevention_intensity*(delta*panic_intensity - gamma)
	arRateS() = panic_intensity*(alpha*(1 - panic_intensity/C) - beta*protectionplprevention_intensity)
end functions
begin reaction rules
	0  -> panic_intensity() arRateS #rateLaw
	0  -> protectionplprevention_intensity() arRateP #rateLaw
end reaction rules
end model
