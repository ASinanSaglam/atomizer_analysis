begin model
begin parameters
	beta_x 0.9
	psi 1.0
	alpha_xy 1.4
	beta_y 1.0
	alpha_y 0.7
	tau 0.9
	beta_S 0.9
	alpha_S 2.7
	n 4.0
end parameters
begin molecule types
	#^ bqbiol:isVersionOf uniprot:P04637
	p53()
	#^ bqbiol:isVersionOf uniprot:Q00987
	Mdm2()
	S
end molecule types
begin seed species
	Mdm2() 5.4198e+23 # 0.9mol/L * 6.022e23/mol *1.0L #Mdm2 #y
	S 0
end seed species
begin observables
	Species p53 p53() #p53
	Species Mdm2 Mdm2() #Mdm2
	Species S S
end observables
begin functions
	arRateS() = -Mdm2*S*alpha_S + beta_S
	fRate0() = S^n*beta_x*psi/(S^n + 1.0)
	fRate1() = Mdm2*alpha_xy
	fRate2() = beta_y*psi*delay(p53, tau)
end functions
begin reaction rules
	R1: 0  -> p53() fRate0()
	R3: p53() -> 0  fRate1() #Modifiers(y)
	R4: 0  -> Mdm2() fRate2() #Modifiers(x)
	R5: Mdm2() -> 0  alpha_y
	0  -> S() arRateS #rateLaw
end reaction rules
end model
