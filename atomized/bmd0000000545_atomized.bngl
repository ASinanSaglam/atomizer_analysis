begin functions
	COP1_Total() = 2*CDCS + CS + 2*UCS
	UM_Total() = 2*UCS + UR + UVR8M
	fRate0() = -CS*kdr1*(UV*n1 + 1) + ks1*(UV*n3*(FHY3 + HY5) + 1)
