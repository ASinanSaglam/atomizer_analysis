	mw530793e3_76b2_4483_be11_e94364306712: CaI(CaM~0,CaS~0)@cytosol -> 0  fRate7()
	mwfaf5e05a_b642_4ee2_a069_3c2fc783fba4: CaI(CaM~0,CaS~0)@cytosol -> CaI(CaM~CaM,CaS~0)@mitochondria fRate8()
	mw69f19152_7258_45b0_bf9e_b196f19d7e03: CaI(CaM~CaM,CaS~0)@mitochondria -> CaI(CaM~0,CaS~0)@cytosol fRate9()
	ERtoMito_reactants_0: 0  -> CaI(CaM~0,CaS~CaS)@ER_store -1*(fRate10()) #Modifiers(CaI, g, IP3)
	ERtoMito_products_0: 0  -> CaI(CaM~CaM,CaS~0)@mitochondria fRate10() #Modifiers(CaI, g, IP3)
end reaction rules
end model
