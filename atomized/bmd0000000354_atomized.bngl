begin functions
	fRate0() = CaI_cytosol*R
	fRate1() = B*CaI_cytosol/(CaI_cytosol^2 + k2^2)
	fRate2() = CaS_ER_store*(1 - mwc714c217_c8fd_4024_912c_681cd6931f59)*(A*CaI_cytosol^2*IP3_cytosol^2*(1 - g_cytosol)/((CaI_cytosol^2 + mw78dd80b8_e003_4c62_81d1_547d001767af^2)*(IP3_cytosol^2 + kIP3R^2)) + L)
	fRate4() = CaI_cytosol^4*E/(CaI_cytosol^4 + mwf998b218_be11_4aa4_81ae_41141861fb42^4)
	fRate4m() = CaI_cytosol^4*E/(CaI_cytosol^4 + mwf998b218_be11_4aa4_81ae_41141861fb42^4)
	fRate6() = mw004dcb62_da5f_41c7_a7bd_033574894f48*(PMleak*(CaS_ER_store^8 + kSTIM^8) + kSTIM^8)/(CaS_ER_store^8 + kSTIM^8)
	fRate7() = CaI_cytosol*mwd21d3f76_d133_4053_8e44_02a538657e0a/(CaI_cytosol^2 + mw3a93c3a6_623a_44fe_84e9_a47823defd1f^2)
	fRate8() = CaI_cytosol^3*mw886be93a_22c7_4966_a1fa_113afd832ae3/(CaI_cytosol^4 + mwc8d6bdb5_59d4_43fa_b96d_7426f4857e0d^4)
	fRate9() = mwd90ce3ea_f8d5_4f0a_8093_e39a2d3dbf33/(CaM_mitochondria + 0.01)
	fRate10() = CaS_ER_store*mwc714c217_c8fd_4024_912c_681cd6931f59*(A*CaI_cytosol^2*IP3_cytosol^2*(1 - g_cytosol)/((CaI_cytosol^2 + mw78dd80b8_e003_4c62_81d1_547d001767af^2)*(IP3_cytosol^2 + kIP3R^2)) + L)
end functions
begin reaction rules
	kPLC: 0  -> IP3()@cytosol fRate0() #Modifiers(CaI)
	JPump: CaI(CaM~0,CaS~0)@cytosol -> CaI(CaM~0,CaS~CaS)@ER_store fRate1()
	JChannel_reactants_0: 0  -> CaI(CaM~0,CaS~CaS)@ER_store -1*(fRate2()) #Modifiers(g, IP3)
	JChannel_products_0: 0  -> CaI(CaM~0,CaS~0)@cytosol fRate2() #Modifiers(g, IP3)
	kPhosphatase: IP3()@cytosol -> 0  D
	inhibition_parameter1: 0  <-> g()@cytosol fRate4(),fRate4m() #Modifiers(CaI)
	inhibition_parameter2: g()@cytosol -> 0  F
