begin model
begin parameters
	Imax 0.048
	Io 0.004
	Irmax 0.4
	Iro 0.02
	K 7.0
	Kdim 12.0
	Kr 7.0
	Kr1 7.0
	R 7.0
	V 1.0
	ka 0.025
	kd 0.1
	kdeg 0.0004
	kdegA 0.0001
	kdegm 0.01
	ksyn 0.04
	ksyn1 0.04
	p 0.15
	q 0.004
	ratio 0.026666667
end parameters
begin molecule types
	#^ bqbiol:encodes uniprot:P04189
	mAprE()
	#^ bqbiol:encodes uniprot:P13800
	mDegU()
	#^ bqbiol:isVersionOf uniprot:P13800
	DegU(degu,p~P~0)
	#^ bqbiol:isVersionOf uniprot:P04189
	AprE()
end molecule types
begin seed species
	DegU(degu,p~0) 10.0 #DegU #DegU
end seed species
begin observables
	Species AprE AprE() #AprE
	Species DegU DegU(degu,p~0) #DegU
	Species DegUP DegU(degu,p~P) #DegUP
	Species Dim DegU(degu!450,p~P).DegU(degu!450,p~P) #Dim
	Species mAprE mAprE() #mAprE
	Species mDegU mDegU() #mDegU
end observables
begin functions
	DegU_Total() = DegU + DegUP + 2*Dim
	kdephos() = q/ratio
	kphos() = p*ratio
	fRate1() = ksyn*mAprE
	fRate10() = Kr*Kr1*(1.0*Dim^2*Irmax + 1.0*Dim*Iro*Kdim + Iro*Kdim^2)/(1.0*Dim^2*Kr*Kr1 + 1.0*Dim^2*Kr*R + 1.0*Dim*Kdim*Kr*Kr1 + 1.0*Dim*Kdim*Kr*R + 1.0*Kdim^2*Kr*Kr1 + 1.0*Kdim^2*Kr*R + 1.0*Kdim^2*Kr1*R + 1.0*Kdim^2*R^2)
	fRate12() = ksyn1*mDegU
	fRate13() = (1.0*Dim*Imax + Io*K)/(1.0*Dim + 1.0*K)
end functions
begin reaction rules
	AprEdeg: AprE() -> 0  kdeg
	AprEsyn: 0  -> AprE() fRate1() #Modifiers(mAprE)
	DimerAss: DegU(degu,p~P) + DegU(degu,p~P) -> DegU(degu!450,p~P).DegU(degu!450,p~P) ka
	DimerDis: DegU(degu!450,p~P).DegU(degu!450,p~P) -> DegU(degu,p~P) + DegU(degu,p~P) kd
	degradation1: DegU(degu,p~0) -> 0  kdeg
	degradation2: DegU(degu,p~P) -> 0  kdeg
	degradation3: DegU(degu!450,p~P).DegU(degu!450,p~P) -> 0  kdeg
	degradationmRNA: mDegU() -> 0  kdegm
	dephosphorylation: DegU(degu,p~P) -> DegU(degu,p~0) kdephos
	mRNAAprEdeg: mAprE() -> 0  kdegm
	mRNAAprEsyn: 0  -> mAprE() fRate10() #Modifiers(Dim)
	phosphorylation: DegU(degu,p~0) -> DegU(degu,p~P) kphos
	synthesisDegU: 0  -> DegU(degu,p~0) fRate12() #Modifiers(mDegU)
	synthesismRNA: 0  -> mDegU() fRate13() #Modifiers(Dim)
end reaction rules
end model
