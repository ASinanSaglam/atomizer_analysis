begin model
begin parameters
	p1 0.7 #units:unit_0
	r1 0.01 #units:unit_0
	p2 0.4 #units:unit_0
	r2 0.02 #units:unit_0
	q 0.4 #units:unit_0
	a 0.0025 #units:unit_0
	h1 0.0001 #units:unit_0
	h2 0.0001 #units:unit_0
	h3 0.0001 #units:unit_0
	h4 0.0001 #units:unit_0
	h5 0.01 #units:unit_0
	k1 1.0 #units:unit_0
	k2 1.0 #units:unit_0
	k3 1.0 #units:unit_0
	k4 1.0 #units:unit_0
	k5 1.0 #units:unit_0
end parameters
begin molecule types
	#^ bqbiol:isVersionOf cl:CL:0000000
	T()
	#^ bqbiol:isVersionOf cl:CL:0000034
	S()
	#^ bqbiol:isVersionOf cl:CL:0000000
	D()
end molecule types
begin seed species
	S() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #S #S
end seed species
begin observables
	Species S S() #S
	Species T T() #T
	Species D D() #D
end observables
begin functions
	q_0() = q/(S^k5*h5 + 1)
	r1_0() = r1/(D^k1*h1 + 1)
	p2_0() = p2/(D^k4*h4 + 1)
	p1_0() = p1/(D^k2*h2 + 1)
	r2_0() = r2/(D^k3*h3 + 1)
	fRate0() = 2.0*S*p1_0*r1_0
	fRate1() = 2*T*q_0*r2_0*(1 - p2_0)
	fRate2() = 2*S*r1_0*(1 - p1_0)
	fRate3() = 2.0*T*p2_0*r2_0
	fRate4() = 2*T*r2_0*(1 - p2_0)*(1 - q_0)
end functions
begin reaction rules
	stem_cell_division: 0  <-> S() fRate0(),r1_0
	stem_cell_activation_products_0: 0  -> S() fRate1() #Modifiers(T)
	activation_cell_division_from_stem_products_0: 0  -> T() fRate2() #Modifiers(S)
	activation_cell_renewal: 0  <-> T() fRate3(),r2_0
	differentiation_of_cell_products_0: 0  -> D() fRate4() #Modifiers(T)
	differentiation_cell_death: D() -> 0  a
end reaction rules
end model
