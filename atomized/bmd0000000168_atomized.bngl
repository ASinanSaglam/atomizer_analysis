begin model
begin parameters
	aD_1 0.4
	k_1 0.05
	GF_1 6.3
	dD_1 0.4
	aE_1 0.16
	af_1 0.9
	theta_1 1.5
	dE_1 0.2
	pX_1 0.48
	RT_1 2.5
	pS_1 0.6
	pD_1 0.48
	qD_1 0.6
	pE_1 0.096
	qE_1 0.6
	aX_1 0.08
	f_1 0.2
	g_1 0.528
	dX_1 1.04
	qX_1 0.8
end parameters
begin molecule types
	X()
	#^ bqbiol:isVersionOf uniprot:P24864
	E()
	#^ bqbiol:isVersionOf uniprot:P06400
	R(s~S~0)
	#^ bqbiol:isVersionOf uniprot:P24385
	D()
end molecule types
begin seed species
	D() 0.1 #D #D_1
	E() 0.6 #E #E_1
	R(s~S) 1.0 #RS #RS_1
	R(s~0) 0.5 #R #R_1
	X() 0.7 #X #X_1
end seed species
begin observables
	Species D D() #D
	Species E E() #E
	Species RS R(s~S) #RS
	Species R R(s~0) #R
	Species X X() #X
end observables
begin functions
	unpho_RB() = R + RS
	RP_ar() = -R - RS + RT_1
	E2F_ar() = -RS + theta_1
	fRate0() = GF_1*aD_1*k_1/(GF_1*k_1 + 1.0)
	fRate1() = E*dD_1
	fRate2() = aE_1*(E2F_ar*af_1 + 1)
	fRate3() = X*dE_1
	fRate4() = RP_ar*X*pX_1/(RP_ar + X + qX_1)
	fRate5() = E2F_ar*pS_1
	fRate6() = D*pD_1/(D + RS + qD_1)
	fRate7() = E*pE_1/(E + RS + qE_1)
	fRate8() = E*X^2*g_1 + E*aX_1 + E2F_ar*f_1
end functions
begin reaction rules
	_1: 0  -> D() fRate0()
	cyclinCDK4degradation_1: D() -> 0  fRate1() #Modifiers(D_1, E_1)
	cyclin_1: 0  -> E() fRate2() #Modifiers(E2F_1)
	cycECDK2degradation_1: E() -> 0  fRate3() #Modifiers(E_1, X_1)
	pRBpdephosphorylation_1: 0  -> R(s~0) fRate4() #Modifiers(RP_1, X_1)
	pRBE2Fcomplexassociation_1: R(s~0) -> R(s~S) fRate5() #Modifiers(RS_1, R_1, E2F_1)
	pRBE2FcomplexdeassociationviacycDCDK4_1: R(s~S) -> 0  fRate6() #Modifiers(RS_1, D_1)
	_7: R(s~S) -> 0  fRate7() #Modifiers(RS_1, E_1)
	cycleprogression_1: 0  -> X() fRate8() #Modifiers(E_1, E2F_1, X_1)
	cyclebreak_1: X() -> 0  dX_1 #Modifiers(X_1)
end reaction rules
end model
