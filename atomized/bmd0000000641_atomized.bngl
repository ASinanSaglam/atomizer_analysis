begin model
begin parameters
	Kd2t 2.0
	Kti2t 10.0
	Kcc2ch 1.0
	Kt2cc 10.0
	Kcc2a 1.0
	Kd2ch 1.0
	Kch2cc 1.0
	Km1 1.0
	Km10 10.0
	Timerinact 6.022e+21 # (10.0 * 0.001)mol/L * 6.022e23/mol *1.0L
	CellCina 5.4198e+21 #original 9.0substance/volume
	Effectorina 6.022e+21 #original 10.0substance/volume
	Damage 6.022e+21 #original 10.0substance/volume
	CellCycletot 6.022e+21 #original 10.0substance/volume
	Effectortot 6.022e+21 #original 10.0substance/volume
	Timertot 6.022e+21 #original 10.0substance/volume
end parameters
begin molecule types
	NHEJ()
	HR()
	Effectoract()
	Timeract()
	CellCact()
end molecule types
begin seed species
	CellCact() 6.021999999999993e+20 #original 0.999999999999999substance/volume #CellCact #CellCact
	HR() 1.8066e+21 #original 3.0substance/volume #HR #HR
	NHEJ() 4.2154000000000003e+21 #original 7.0substance/volume #NHEJ #NHEJ
end seed species
begin observables
	Species Timeract Timeract() #Timeract
	Species CellCact CellCact() #CellCact
	Species Effectoract Effectoract() #Effectoract
	Species HR HR() #HR
	Species NHEJ NHEJ() #NHEJ
end observables
begin functions
	Damage_ar() = HR + NHEJ
	Effectorina_ar() = -Effectoract + Effectortot
	arRateEffectoract() = -CellCact*Effectoract*Kcc2ch/(Effectoract + Km10) + Damage_ar*Effectorina_ar*Kd2ch/(Effectorina_ar + Km10)
	Timerinact_ar() = -Timeract + Timertot
	arRateTimeract() = Damage_ar*Kd2t*Timerinact_ar/(Km1 + Timerinact_ar) - Kti2t*Timeract/(Km1 + Timeract)
	CellCina_ar() = -CellCact + CellCycletot
	arRateCellCact() = -CellCact*Effectoract*Kch2cc/(CellCact + Km10) - CellCact*Kt2cc*Timeract/(CellCact + Km10) + CellCina_ar*(CellCact + Kcc2a)/(CellCina_ar + Km10)
	arRateNHEJ() = -0.5*NHEJ
	arRateHR() = -0.2*HR
end functions
begin reaction rules
	0  -> Timeract() arRateTimeract #rateLaw
	0  -> CellCact() arRateCellCact #rateLaw
	0  -> Effectoract() arRateEffectoract #rateLaw
	0  -> HR() arRateHR #rateLaw
	0  -> NHEJ() arRateNHEJ #rateLaw
end reaction rules
end model
