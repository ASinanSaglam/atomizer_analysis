#@notes:'This is an atomized translation of an SBML model created on 12/08/2021. The original model has 4 molecules and 6 reactions. The translated model has 3 molecules and 7 rules'
###
begin model
begin parameters
	A 20.0 #units:psec
	B 40.0 #units:microMpsec
	C 1.1 #units:microMpsec
	D 2.0 #units:psec
	E 1.0 #units:microM4
	F 0.02 #units:psec
	k1 0.5 #units:microM
	k2 0.15 #units:microM
	k3 1.0 #units:microM
	L 0.01 #units:psec
	R 0.09
end parameters
begin compartments
	#volume units: L
	cytosol 3 1.0
	ER_store 3 1.0
end compartments
begin molecule types
	#^ bqbiol:isVersionOf go:GO:0019855
	g()
	#^ bqbiol:isVersionOf chebi:CHEBI:29108, kegg.compound:C00076
	CaI(CaS~CaS~0)
	#^ bqbiol:isVersionOf chebi:CHEBI:16595, kegg.compound:C01255
	IP3()
end molecule types
begin seed species
	@cytosol:CaI(CaS~0) 6.022000000000001e+22 # 0.1mol/L * 6.022e23/mol *1.0L #CaI #CaI
	@cytosol:IP3() 3.0110000000000005e+22 #original 0.05mol/L  #IP3 #IP3
	@ER_store:CaI(CaS~CaS) 6.6242e+26 #original 1100.0mol/L  #CaS #CaS
end seed species
begin observables
	Species CaI_cytosol @cytosol:CaI(CaS~0) #CaI
	Species IP3_cytosol @cytosol:IP3() #IP3
	Species CaS_ER_store @ER_store:CaI(CaS~CaS) #CaS
	Species g_cytosol @cytosol:g() #g
end observables
begin functions
	fRate0() = CaS_ER_store*(1 - g_cytosol)*(0.0625*A*IP3_cytosol^4/(0.5*IP3_cytosol + k1)^4 + L)
	fRate1() = 0.0001*B*CaI_cytosol/(0.0001*CaI_cytosol^2 + k2^2)
	fRate2() = C*(-k3/((0.01*CaI_cytosol + k3)*(R + 1)) + 1)
	fRate3() = 0.5*D
	fRate4() = 1.0e-8*CaI_cytosol^4*E
	fRate4m() = 1.0e-8*CaI_cytosol^4*E
	fRate5() = F
end functions
begin reaction rules
	JChannel_reactants_0: 0  -> CaI(CaS~CaS)@ER_store -1*(fRate0()) #Modifiers(IP3, g)
	JChannel_products_0: 0  -> CaI(CaS~0)@cytosol fRate0() #Modifiers(IP3, g)
	JPump: CaI(CaS~0)@cytosol -> CaI(CaS~CaS)@ER_store fRate1()
	kPLC_products_0: 0  -> IP3()@cytosol fRate2() #Modifiers(CaI)
	kPhosphatase: IP3()@cytosol -> 0  fRate3()
	inhibition_parameter1: 0  <-> g()@cytosol fRate4(),fRate4m() #Modifiers(CaI)
	inhibition_parameter2_reactants_0: 0  -> g()@cytosol -1*(fRate5()) #Modifiers(CaI)
end reaction rules
end model
