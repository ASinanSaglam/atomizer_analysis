begin model
begin parameters
	r1_rM 1.000000
	r1_k 1.000000
	r1_n 2.000000
	r2_rP 1.000000
	r2_m 3.000000
	r2_parameter_0000009 4.000000
	r3_qM 0.210000
	r4_qP 0.210000
end parameters
begin molecule types
	#^ bqbiol:isVersionOf chebi:CHEBI:33699, kegg.compound:C00046
	mRNA()
	#^ bqbiol:is kegg.compound:C00017, chebi:CHEBI:16541
	protein()
end molecule types
begin seed species
	mRNA() 1.0 #mRNA #M
	protein() 1.0 #protein #P
end seed species
begin observables
	Species mRNA mRNA() #mRNA
	Species protein protein() #protein
end observables
begin functions
	fRate0() = 1.0/(1.0*protein^2.0 + 1.0)
	fRate1() = 1.0*delay(mRNA, 4.0)^3.0
end functions
begin reaction rules
	TC: 0  -> mRNA() fRate0() #Modifiers(P)
	TL: 0  -> protein() fRate1() #Modifiers(M)
	Md: mRNA() -> 0  r3_qM
	Pd: protein() -> 0  r4_qP
end reaction rules
end model
