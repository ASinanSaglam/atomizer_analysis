begin model
begin parameters
	tau1 10.0 #units:time
	tau2 10.0 #units:time
	Vsp 0.5 #units:nM_per_hr
	K1 0.3 #units:nM
	Vsc 0.25 #units:nM_per_hr
	K2 0.1 #units:nM
	kdc 0.5 #units:per_hr
	kdp 0.5 #units:per_hr
end parameters
begin molecule types
	#^ bqbiol:hasPart uniprot:P07663, uniprot:O61735
	dClk()
	#^ bqbiol:is uniprot:P07663
	Per()
end molecule types
begin seed species
	Per() 0.30110000000000003 # (5e-16 * 1e-09)mol/L * 6.022e23/mol *1.0L #Per #Per
	dClk() 0.06022 #original 1e-16nanomole (new default)/L #dClk #dClk
end seed species
begin observables
	Species Per Per() #Per
	Species dClk dClk() #dClk
end observables
begin functions
	dClkF_tau2() =  if((-delay(Per<tau2+delay(dClk,tau2),0)),(0),(-delay(Per,tau2)+delay(dClk,tau2)))
	dClkF_tau1() =  if((-delay(Per<tau1+delay(dClk,tau1),0)),(0),(-delay(Per,tau1)+delay(dClk,tau1)))
	freedClk_ar() =  if((-Per+dClk<0),(0),(-Per+dClk))
	fRate0() = Vsp*dClkF_tau1/(K1 + dClkF_tau1)
	fRate1() = K2*Vsc/(K2 + dClkF_tau2)
end functions
begin reaction rules
	rPer: 0  -> Per() fRate0() #Modifiers(dClkF)
	rdClk: 0  -> dClk() fRate1() #Modifiers(dClkF)
	rPerD: Per() -> 0  kdc
	rdClkD: dClk() -> 0  kdp
end reaction rules
end model
