begin model
begin parameters
	r1_k1 0.600000
	r1_k2 0.100000
	r2_k1 0.014000
	r2_k2 0.200000
end parameters
begin molecule types
	#^ bqbiol:isVersionOf uniprot:P10276
	Receptor(ligand)
	#^ bqbiol:hasVersion chebi:CHEBI:42492, chebi:CHEBI:16670
	CoActivator()
	#^ bqbiol:hasPart chebi:CHEBI:15367, uniprot:P10276
	Ligand_Receptor_CoActivator()
	#^ bqbiol:is chebi:CHEBI:15367
	Ligand(receptor)
end molecule types
begin seed species
	Ligand(receptor) 301100000000000.06 # (0.0005 * 1e-06)mol/L * 6.022e23/mol *1.0L #Ligand #L
	Receptor(ligand) 2107700000000000.0 #original 0.0035substance/L  #Receptor #R
	CoActivator() 1.8066e+19 #original 30.0substance/L  #CoActivator #CA
end seed species
begin observables
	Species Ligand Ligand(receptor) #Ligand
	Species Ligand_Receptor Receptor(ligand!238).Ligand(receptor!238) #Ligand-Receptor
	Species Receptor Receptor(ligand) #Receptor
	Species CoActivator CoActivator() #CoActivator
	Species Ligand_Receptor_CoActivator Ligand_Receptor_CoActivator() #Ligand-Receptor-CoActivator
end observables
begin reaction rules
	LR_complx: Ligand(receptor) + Receptor(ligand) <-> Receptor(ligand!238).Ligand(receptor!238) r1_k1,r1_k2
	LRCA_complx: Receptor(ligand!238).Ligand(receptor!238) + CoActivator() <-> Ligand_Receptor_CoActivator() r2_k1,r2_k2
end reaction rules
end model
