	cgen_transcription: 0  -> cgen_mRNA()@Nucleus fRate24() #Modifiers(NFKB_nuc)
	cgen_mRNA_degradation: cgen_mRNA()@Nucleus -> 0  c3c
	IkB_degradation_1: IKKactive_IkB_NFKB()@Cytosol -> IKK_inact(IKK_active~IKK_active,IKK_neutral~0,ikb)@Cytosol + NFKB(_nuc~0,ikb)@Cytosol t2
	IkB_degradation_2: IKK_inact(IKK_active~IKK_active,IKK_neutral~0,ikb!843)@Cytosol.IkB(_nuc~0,ikk_inact!843,nfkb)@Cytosol -> IKK_inact(IKK_active~IKK_active,IKK_neutral~0,ikb)@Cytosol t1
