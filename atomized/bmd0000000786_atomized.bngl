	Species IKK_inact_Cytosol @Cytosol:IKK_inact(IKK_active~0,IKK_neutral~0,ikb) #IKK_inact
	Species IKKactive_IkB_Cytosol @Cytosol:IKK_inact(IKK_active~IKK_active,IKK_neutral~0,ikb!843).IkB(_nuc~0,ikk_inact!843,nfkb) #IKKactive_IkB
	Species IkB_Cytosol @Cytosol:IkB(_nuc~0,ikk_inact,nfkb) #IkB
	Species IKKactive_IkB_NFKB_Cytosol @Cytosol:IKKactive_IkB_NFKB() #IKKactive_IkB_NFKB
	Species IkB_NFKB_Cytosol @Cytosol:IkB(_nuc~0,ikk_inact,nfkb!842).NFKB(_nuc~0,ikb!842) #IkB_NFKB
