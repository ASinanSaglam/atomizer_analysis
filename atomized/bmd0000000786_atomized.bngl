	cgen_mRNA_amp() = 10000*cgen_mRNA_Nucleus
	A20_mRNA_amp() = 10000*A20_mRNA_Nucleus
	IkB_mRNA_amp() = 10000*IkB_mRNA_Nucleus
	TNF_ar() = TNF_R
	NFKB_total() = IKKactive_IkB_NFKB_Cytosol + IkB_NFKB_Cytosol + IkB_NFKB_nuc_Nucleus/ModelValue_1 + NFKB_Cytosol + NFKB_nuc_Nucleus/ModelValue_1
