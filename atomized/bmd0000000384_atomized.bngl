###
begin model
begin parameters
	Rp 3.2
	Nt 0.5
	alpha 0.3
	Q 1000.0
	Gamma 42.8926627111176
	teta 0.9
	Vcmax 2.53232284114507
	Kc 406.066146391504
	Ko 276.900084789612
end parameters
begin molecule types
	#^ bqbiol:isVersionOf obo.chebi:CHEBI:16710
	RuBP()
	#^ bqbiol:isVersionOf obo.chebi:CHEBI:16526
	CO2()
	#^ bqbiol:isVersionOf obo.chebi:CHEBI:17050
	PGA()
	#^ bqbiol:isVersionOf obo.chebi:CHEBI:25523
	NADP()
	#^ bqbiol:isVersionOf obo.chebi:CHEBI:15379
	O2()
	#^ bqbiol:isVersionOf obo.chebi:CHEBI:16474
	NADPH()
end molecule types
begin seed species
	$RuBP() 1.2044e+21 # (2.0 * 0.001)mol/L * 6.022e23/mol *1.0L #RuBP #RuBP
	PGA() 1.44528e+21 #original 2.4/L #PGA #PGA
	NADPH() 1.2646200000000002e+20 #original 0.21/L #NADPH #NADPH
	$CO2() 1.4753900000000001e+23 #original 245.0/L #CO2 #CO2
	$O2() 1.26462e+23 #original 210.0/L #O2 #O2
	NADP() -1.26462e+20 #original 0.29/L #NADP #NADP
end seed species
begin observables
	Species RuBP RuBP() #RuBP
	Species PGA PGA() #PGA
	Species NADPH NADPH() #NADPH
	Species CO2 CO2() #CO2
	Species O2 O2() #O2
	Species NADP NADP() #NADP
end observables
begin functions
	J() = 0.0307692307692308*Q*alpha/(10 - 9*teta)
	Vj() = CO2*J/(4*CO2 + 8*Gamma)
	phi() = 0.21*Kc*O2/(CO2*Ko)
	Vomax() = 0.21*Vcmax
	NADP_ar() = -NADPH + Nt
	Vc() = CO2*Vcmax/(CO2 + Kc*(1 + O2/Ko))
	fRate0() = Vc/2 + Vj/2 - abs(Vc - Vj)/2
	fRate1() = phi*(Vc + Vj - abs(Vc - Vj))/2
	fRate2() = NADPH*Vcmax/(Nt*Rp)
	fRate3() = 0.5*J/Nt
end functions
begin reaction rules
	PGA_prod_Vc_reactants_0: 0  -> RuBP() -1*(fRate0())
	PGA_prod_Vc_reactants_1: 0  -> CO2() -1*(fRate0())
	PGA_prod_Vc_reactants_2: 0  -> NADPH() -1*2.0*(fRate0())
	PGA_prod_Vc_products_0: 0  -> PGA() 2.0*fRate0()
	PGA_prod_Vo_reactants_0: 0  -> RuBP() -1*(fRate1())
	PGA_prod_Vo_reactants_1: 0  -> O2() -1*(fRate1())
	PGA_prod_Vo_reactants_2: 0  -> NADPH() -1*2.0*(fRate1())
	PGA_prod_Vo_products_0: 0  -> PGA() fRate1()
	PGA_cons: PGA() -> RuBP() fRate2() #Modifiers(NADPH)
	NADPH_prod: NADP() -> NADPH() fRate3()
end reaction rules
end model
