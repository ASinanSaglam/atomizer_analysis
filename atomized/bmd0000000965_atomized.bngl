begin model
begin parameters
	alpha1 40.0 #units:unit_0
	beta1 0.8 #units:unit_1
	alpha4 0.06 #units:unit_2
	beta4 0.01 #units:unit_1
	time_constant 100.0 #units:unit_4
	kflux 22.0 #units:unit_0
	Vp 0.2 #units:unit_0
	k3 0.5 #units:unit_2
	k2 0.5 #units:unit_2
	k_1 0.88 #units:unit_2
	Jinflux_0 0.025 #units:unit_0
	Kp 0.54 #units:unit_3
	k5 0.02 #units:unit_2
end parameters
begin molecule types
	#^ bqbiol:is obi:OBA:0000003
	#^ bqbiol:hasProperty go:GO:0070679
	Ca2pl()
	#^ bqbiol:hasPart chebi:CHEBI:139109, ncit:C580
	#^ bqbiol:hasVersion pato:PATO:0000608
	#^ bqbiol:is ncit:C47881
	S(I1~I1~0,I2~I2~0,O~O~0)
	T
end molecule types
begin seed species
	S(I1~0,I2~I2,O~0) 6.022e+23 # 1.0mol/L * 6.022e23/mol *1.0L #I2 #I2
	T 0
end seed species
begin observables
	Species S S(I1~0,I2~0,O~0) #S
	Species O S(I1~0,I2~0,O~O) #O
	Species I1 S(I1~I1,I2~0,O~0) #I1
	Species I2 S(I1~0,I2~I2,O~0) #I2
	Species Ca2pl Ca2pl() #Ca2+
	Species T T
end observables
begin functions
	p() = 0.6 - 0.6*exp(-T/time_constant)
	k4() = alpha4*p/(beta4 + p)
	arRateT() = 1.0
	k1() = Ca2pl^3*alpha1/(Ca2pl^3 + beta1^3)
	S_ar() = -I1 - I2 - O + 1
	fRate0() = k1*p
	fRate5() = O^4*kflux
	fRate6() = Ca2pl*Vp/(Ca2pl^2 + Kp^2)
end functions
begin reaction rules
	Shut_to_Open: S(I1~0,I2~0,O~0) <-> S(I1~0,I2~0,O~O) fRate0(),k_1
	Open_to_Inactivated1: S(I1~0,I2~0,O~O) -> S(I1~I1,I2~0,O~0) k2
	Inactivated1_to_Shut: S(I1~I1,I2~0,O~0) -> S(I1~0,I2~0,O~0) k3
	Inactivated1_to_Inactivated2: S(I1~I1,I2~0,O~0) -> S(I1~0,I2~I2,O~0) k4
	Inactivated2_to_Shut: S(I1~0,I2~I2,O~0) -> S(I1~0,I2~0,O~0) k5
	Jrel: 0  -> Ca2pl() fRate5() #Modifiers(O)
	Jpump: Ca2pl() -> 0  fRate6()
	Jinflux: 0  -> Ca2pl() Jinflux_0
	0  -> T() arRateT #rateLaw
end reaction rules
end model
