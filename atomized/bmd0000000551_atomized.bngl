###
begin model
begin parameters
	Sc 1.16
	Ki 0.0232
	k1 1.13
	r 0.43
	deltap 0.55
	epsilon 0.0206
	alpha 0.522
	aplasma 187.0
	bplasma 0.089
	tauplasma 1.66
	aCSF 4.92
	bCSF 0.259
	tauCSF 2.06
	J 0
end parameters
begin compartments
	#volume units: volume
	CSF 3 1.0
	Plasma 3 1.0
end compartments
begin molecule types
	C()
	P()
	l
end molecule types
begin seed species
	@CSF:C() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #C #C
	@Plasma:P() 6.022e+20 #original 1.0substance/volume #P #P
	l@CSF 1.0
end seed species
begin observables
	Species C_CSF @CSF:C() #C
	Species P_Plasma @Plasma:P() #P
	Species l l
end observables
begin functions
	f_t() =  if((time()<tauplasma),(aplasma*time()*exp(-bplasma*tauplasma)/tauplasma),(aplasma*exp(-bplasma*time())))
	arRatel() = epsilon*(-l + 1/(alpha*f_t + 1))
	g_t() =  if((time()<tauCSF),(aCSF*time()*exp(-bCSF*tauCSF)/tauCSF),(aCSF*exp(-bCSF*time())))
	arRateC() = -C_CSF*k1 + J + Sc/(1 + g_t/Ki)
	arRateP() = C_CSF*k1*r - J*r - P_Plasma*deltap*l
end functions
begin reaction rules
	0  -> C()@CSF arRateC #rateLaw
	0  -> P()@Plasma arRateP #rateLaw
	0  -> l()@CSF arRatel #rateLaw
end reaction rules
end model
