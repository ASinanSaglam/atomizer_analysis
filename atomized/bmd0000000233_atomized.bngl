begin model
begin parameters
	r1_k1 8.000000
	r2_k2 1.000000
	r3_k3 1.000000
	r4_k4 1.500000
end parameters
begin molecule types
	S()
	X(P~P~0)
	Y()
end molecule types
begin seed species
	$S() 6.022e+23 # 1.0mol/L * 6.022e23/mol *1.0L #S #S
	$X(P~P) 6.022e+23 #original 1.0mol/L  #P #P
	X(P~0) 6.022e+23 #original 1.0mol/L  #X #X
	Y() 6.022e+23 #original 1.0mol/L  #Y #Y
end seed species
begin observables
	Species S S() #S
	Species P X(P~P) #P
	Species X X(P~0) #X
	Species Y Y() #Y
end observables
begin reaction rules
	r1: S() + Y() -> X(P~0) + X(P~0) r1_k1
	r2: X(P~0) + X(P~0) -> X(P~0) + Y() r2_k2
	r3: X(P~0) + Y() -> X(P~P) + Y() r3_k3
	r4: X(P~0) -> X(P~P) r4_k4
end reaction rules
end model
