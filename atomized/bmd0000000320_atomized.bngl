###
begin model
begin parameters
	F_G 1.0 #units:dimensionless
	Q 0.828
	f_H 0.13 #units:dimensionless
	f_AADC 0.69 #units:dimensionless
	f_COMT 0.1 #units:dimensionless
	ki 0.00246
	CL_dopa0 0.823 #units:l_per_h
	ka_c 1.29
	ka_M 2.47
	ka_B 0.94
	F_B 0.022 #units:dimensionless
	CL_OMD 0.00895 #units:l_per_h
	CL_B 1.67 #units:l_per_h
	fm 0.15 #units:dimensionless
	CL_M 4.29 #units:l_per_h
	CL_dB 0.072 #units:l_per_h
	CL_dM 1.06 #units:l_per_h
	L_Dopa_per_kg_rat 404.0 #units:umol_per_kg
	Benserazide_per_kg_rat 78.0 #units:umol_per_kg
	rat_body_mass 0.25 #units:kilogram
	B_M_frac 0.07
	A_bens_tot (78.0) * (0.25)
end parameters
begin compartments
	#volume units: L
	gut 3 1.0
	Vdopa 3 0.496 #V_L_Dopa
	V_3_OMD 3 0.128
	V1_B 3 0.202
	V2_B 3 0.127
	V1_M 3 0.0691
	V2_M 3 3.2
end compartments
begin molecule types
	#^ bqbiol:is chebi:CHEBI:15765, kegg.compound:C00355, pubchem.compound:6047
	A_dopa(C_dopa~C_dopa~0)
	#^ bqbiol:isVersionOf chebi:CHEBI:25212
	#^ bqbiol:is pubchem.compound:188973
	A_M(C1_M~C1_M~0,C2_M~C2_M~0)
	#^ bqbiol:is kegg.drug:D03082, pubchem.compound:2327
	A_B(C1_B~C1_B~0,C2_B~C2_B~0)
	#^ bqbiol:is pubchem.compound:9307
	#^ bqbiol:isVersionOf chebi:CHEBI:25212
	C_3_OMD()
end molecule types
begin seed species
	@gut:A_dopa(C_dopa~0) (404.0) * (0.25) #A_dopa
	@gut:A_B(C1_B~0,C2_B~0) (0) * (1 -(0.07)) #A_B
	@gut:A_M(C1_M~0,C2_M~0) (0) * (0.07) #A_M
end seed species
begin observables
	Species A_dopa_gut @gut:A_dopa(C_dopa~0) #A_dopa
	Species A_B_gut @gut:A_B(C1_B~0,C2_B~0) #A_B
	Species A_M_gut @gut:A_M(C1_M~0,C2_M~0) #A_M
	Species C_dopa_Vdopa @Vdopa:A_dopa(C_dopa~C_dopa) #C_dopa
	Species C_3_OMD_V_3_OMD @V_3_OMD:C_3_OMD() #C_3-OMD
	Species C1_M_V1_M @V1_M:A_M(C1_M~C1_M,C2_M~0) #C1_M
	Species C2_M_V2_M @V2_M:A_M(C1_M~0,C2_M~C2_M) #C2_M
	Species C1_B_V1_B @V1_B:A_B(C1_B~C1_B,C2_B~0) #C1_B
	Species C2_B_V2_B @V2_B:A_B(C1_B~0,C2_B~C2_B) #C2_B
end observables
begin functions
	f_rest() = -f_AADC - f_COMT + 1
	CL_rest() = CL_dopa0*f_rest
	CL_AADC0() = CL_dopa0*f_AADC
	CL_AADC() = CL_AADC0/(C1_M_V1_M/ki + 1)
	CL_COMT() = CL_dopa0*f_COMT
	CL_dopa() = CL_AADC + CL_COMT + CL_rest
	CL_H() = CL_dopa*f_H
	F_H() = -CL_H/Q + 1
	F_c() = F_G*F_H
	fRate0() = F_c*ka_c
	fRate1m() = A_dopa_gut*F_c*ka_c
	fRate3() = F_B*ka_B
	fRate4m() = A_B_gut*F_B*ka_B
	fRate10() = -CL_B*fm/(fm - 1.0)
end functions
begin reaction rules
	l_dopa_absorption: A_dopa(C_dopa~0)@gut -> A_dopa(C_dopa~C_dopa)@Vdopa fRate0()
	l_dopa_blood_hepa_clearance: A_dopa(C_dopa~0)@gut <-> 0  ka_c,fRate1m()
	ro_04_5127_absorption: A_M(C1_M~0,C2_M~0)@gut -> A_M(C1_M~C1_M,C2_M~0)@V1_M ka_M
	benserazide_absorption: A_B(C1_B~0,C2_B~0)@gut -> A_B(C1_B~C1_B,C2_B~0)@V1_B fRate3()
	benserazide_gut_clearance: A_B(C1_B~0,C2_B~0)@gut <-> 0  ka_B,fRate4m()
	AADC_clearance: A_dopa(C_dopa~C_dopa)@Vdopa -> 0  CL_AADC
	COMT_clearance: A_dopa(C_dopa~C_dopa)@Vdopa -> C_3_OMD()@V_3_OMD CL_COMT
	rest_clearance: A_dopa(C_dopa~C_dopa)@Vdopa -> 0  CL_rest
	_3_OMD_clearance: C_3_OMD()@V_3_OMD -> 0  CL_OMD
	bens_clearance: A_B(C1_B~C1_B,C2_B~0)@V1_B -> 0  CL_B
	bens_metabolism: A_B(C1_B~C1_B,C2_B~0)@V1_B -> A_M(C1_M~C1_M,C2_M~0)@V1_M fRate10()
	ro_clearance: A_M(C1_M~C1_M,C2_M~0)@V1_M -> 0  CL_M
	bens_distribution: A_B(C1_B~C1_B,C2_B~0)@V1_B <-> A_B(C1_B~0,C2_B~C2_B)@V2_B CL_dB,CL_dB
	ro_distribution: A_M(C1_M~C1_M,C2_M~0)@V1_M <-> A_M(C1_M~0,C2_M~C2_M)@V2_M CL_dM,CL_dM
end reaction rules
end model
