begin model
begin parameters
	a 0.03
	b 0.25
	c -52.0
	Vthresh 30.0
	d 0
	i 0
end parameters
begin compartments
	#volume units: L
	cell 3 1.0
end compartments
begin molecule types
	v
	u
end molecule types
begin seed species
	v@cell -64.0
	u@cell -16.0
end seed species
begin observables
	Species v v
	Species u u
end observables
begin functions
	arRateu() = a*(b*v - u)
	arRatev() = i - u + 0.04*v^2 + 5*v + 140
end functions
begin reaction rules
	0  -> v()@cell arRatev #rateLaw
	0  -> u()@cell arRateu #rateLaw
end reaction rules
end model
