#@notes:'This is an atomized translation of an SBML model created on 12/08/2021. The original model has 27 molecules and 0 reactions. The translated model has 9 molecules and 9 rules'
###
begin model
begin parameters
	gNa_max 0.9
	gcabar 0.05
	gk_max 0.42
	gL_max 0.0024
	gKca 0.5
	ENa 50.0
	EK -70.0
	EL -75.0
	ECa 50.0
	En -40.0
	Em -42.0
	Eh -41.0
	Ct 0.04
	Cm 0.009
	Rs 15.0
	Am 200.0
	alpha_n_max 0.0229
	beta_n_max 0.09616
	v_alpha_m 10.0
	v_alpha_n 7.0
	v_alpha_h 14.7
	alpha_m_max 0.208
	beta_m_max 2.081
	v_beta_n 40.0
	v_beta_m 18.0
	v_beta_h 7.6
	alpha_h_max 0.0156
	beta_h_max 3.382
	kd 0.18
	alpha 4.5e-06
	kpmca 0.2
	pleak 0.0005
	kserca 0.4
	d1 0.84
	d2 1.0
	k1 0.18
	k2 0.011
	bbar 0.28
	abar 0.48
	fer 0.01
	vcytver 5.0
	fcyt 0.01
	Stimulus_Period 50.0
	Stimulus_Magnitude 2.0
	Stimulus_Start 5.0
	Stimulus_Duration 1.0
	beta_n 0.09616*exp(-1.0) #original 0.230675848267876substance/volume
	beta_m 2.081*exp(-2.33333333333333) #original 13.0160326809795substance/volume
	beta_h 3.382/(exp(-5.39473684210526) + 1.0) #original 0.038137484704699substance/volume
	alpha_n 0.916/(1.0 - exp(-5.71428571428571)) #original 0.00543709940740284substance/volume
	alpha_m 8.736/(1.0 - exp(-4.2)) #original 0.262861215247374substance/volume
	alpha_h 0.0156*exp(-4.1) #original 0.467439960739393substance/volume
	w 1.0 #original 0.286670947619484substance/volume
	jleak 6.0174835e+19 #original 0.099925substance/volume
	jserca 3.6132e+19 #original 0.06substance/volume
	jer 0 #original 0.039925substance/volume
end parameters
begin molecule types
	d()
	c()
	Vm()
	o()
	cer()
	Vt()
	h()
	m()
	n()
end molecule types
begin seed species
	h() 6.022e+20 # (1.0 * 0.001)mol/L * 6.022e23/mol *1.0L #h #h
	n() 6.022000000000001e+19 #original 0.1substance/volume #n #n
	o() 6.022000000000001e+19 #original 0.1substance/volume #o #o
	c() 9.033e+19 #original 0.15substance/volume #c #c
	cer() 1.2044000000000002e+23 #original 200.0substance/volume #cer #cer
end seed species
begin observables
	Species Vm Vm() #Vm
	Species m m() #m
	Species h h() #h
	Species n n() #n
	Species Vt Vt() #Vt
	Species d d() #d
	Species o o() #o
	Species c c() #c
	Species cer cer() #cer
end observables
begin functions
	IK_ar() = gk_max*n^4*(-EK + Vm)
	Stimulus_ar() =  if((time()>=Stimulus_Start&&-Stimulus_Period*min(rint(((-Stimulus_Start+time())/Stimulus_Period)-0.5),rint(((-Stimulus_Start+time())/Stimulus_Period)+0.5))-Stimulus_Start+time()<Stimulus_Duration),(Stimulus_Magnitude),(0))
	gca() = -Vm*gcabar/(exp(0.117*Vm) - 1)
	ICa_ar() = d^2*gca
	w_ar() = c^5/(c^5 + kd^5)
	IKCa_ar() = gKca*o*w_ar*(-EK + Vm)
	IL_ar() = gL_max*(-EL + Vm)
	INa_ar() = gNa_max*h*m^3*(-ENa + Vm)
	IT_ar() = (Vm - Vt)/Rs
	arRateVm() = (-ICa_ar - IKCa_ar - IK_ar - IL_ar - INa_ar - IT_ar + Stimulus_ar)/Cm
	jmem_ar() = -ICa_ar*alpha - c*kpmca
	jleak_ar() = pleak*(-c + cer)
	jserca_ar() = c*kserca
	jer_ar() = jleak_ar - jserca_ar
	arRatec() = fcyt*(jer_ar + jmem_ar)
	beta() = bbar/(c/(k2*exp(-0.0748769545457734*Vm*d2)) + 1)
	alp() = abar/(1 + k1*exp(-0.0748769545457734*Vm*d1)/c)
	tau() = 1/(alp + beta)
	ooinf() = alp*tau
	arRateo() = (-o + ooinf)/tau
	dinf() = 1/(exp(-0.0884955752212389*Vm - 2.17699115044248) + 1)
	taud() = 80*cosh(-0.031*Vm - 1.1501)
	betad() = (1 - dinf)/taud
	alphad() = dinf/taud
	arRated() = alphad*(1 - d) - betad*d
	beta_n_ar() = beta_n_max*exp((En - Vm)/v_beta_n)
	alpha_n_ar() = alpha_n_max*(-En + Vm)/(1 - exp((En - Vm)/v_alpha_n))
	arRaten() = alpha_n_ar*(1 - n) - beta_n_ar*n
	alpha_h_ar() = alpha_h_max*exp((Eh - Vm)/v_alpha_m)
	beta_h_ar() = beta_h_max/(exp((Eh - Vm)/v_beta_h) + 1)
	arRateh() = alpha_h_ar*(1 - h) - beta_h_ar*h
	alpha_m_ar() = alpha_m_max*(-Em + Vm)/(1 - exp((Em - Vm)/v_alpha_m))
	beta_m_ar() = beta_m_max*exp((Em - Vm)/v_beta_m)
	arRatem() = alpha_m_ar*(1 - m) - beta_m_ar*m
	arRatecer() = -fer*jer_ar*vcytver
	arRateVt() = (Vm - Vt)/(Ct*Rs)
end functions
begin reaction rules
	0  -> Vm() arRateVm #rateLaw
	0  -> m() arRatem #rateLaw
	0  -> h() arRateh #rateLaw
	0  -> n() arRaten #rateLaw
	0  -> Vt() arRateVt #rateLaw
	0  -> d() arRated #rateLaw
	0  -> o() arRateo #rateLaw
	0  -> c() arRatec #rateLaw
	0  -> cer() arRatecer #rateLaw
end reaction rules
end model
