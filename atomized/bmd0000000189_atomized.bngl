	Mdm2Synthesis: Mdm2(_mrna~_MRNA,arf,deg~0,mrnadeg~0,p53) -> Mdm2(_mrna~_MRNA,arf,deg~0,mrnadeg~0,p53) + Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53) + mdm2syn() ksynMdm2
	Mdm2mRNASynthesis: p53(deg~0,mdm2) -> p53(deg~0,mdm2) + Mdm2(_mrna~_MRNA,arf,deg~0,mrnadeg~0,p53) + Mdm2mRNAsyn() ksynMdm2mRNA
	Mdm2mRNADegradation: Mdm2(_mrna~_MRNA,arf,deg~0,mrnadeg~0,p53) -> Mdm2(_mrna~0,arf,deg~0,mrnadeg~MRNADEG,p53) kdegMdm2mRNA
