	P53_Mdm2Binding: p53(deg~0,mdm2) + Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53) -> Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53!332).p53(deg~0,mdm2!332) kbinMdm2p53
	P53_Mdm2Release: Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53!332).p53(deg~0,mdm2!332) -> p53(deg~0,mdm2) + Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53) krelMdm2p53
	DNAdamage: 0  -> damDNA() + totdamDNA() fRate8()
	DNArepair: damDNA() -> 0  krepair
	ARFactivation: damDNA() -> damDNA() + ARF(mdm2) kactARF
	ARF_Mdm2Binding: ARF(mdm2) + Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53) -> Mdm2(_mrna~0,arf!333,deg~0,mrnadeg~0,p53).ARF(mdm2!333) kbinARFMdm2
	ARF_Mdm2Degradation: Mdm2(_mrna~0,arf!333,deg~0,mrnadeg~0,p53).ARF(mdm2!333) -> ARF(mdm2) + Mdm2(_mrna~0,arf,deg~DEG,mrnadeg~0,p53) fRate12()
	ARFDegradation: ARF(mdm2) -> 0  fRate13()
end reaction rules
end model
