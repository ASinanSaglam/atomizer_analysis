	p53(deg~0,mdm2) 5.0 #p53 #p53
	Mdm2(_mrna~0,arf,deg~0,mrnadeg~0,p53!332).p53(deg~0,mdm2!332) 95.0 #Mdm2_p53 #Mdm2_p53
end seed species
begin observables
