begin functions
	totMdm2_ar() = ARF_Mdm2 + Mdm2 + Mdm2_p53
	totp53_ar() = Mdm2_p53 + p53
	fRate3() = kdegMdm2*kproteff
	fRate4() = Source*ksynp53
	fRate5() = kdegp53*kproteff
	fRate8() = IR*kdam
	fRate12() = kdegARFMdm2*kproteff
