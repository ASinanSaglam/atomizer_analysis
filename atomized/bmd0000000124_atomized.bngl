#@BQM_IS_DERIVED_FROM:['http://identifiers.org/pubmed/6305437', 'http://identifiers.org/pubmed/15294427']
#@BQB_HAS_TAXON:['http://identifiers.org/taxonomy/10114']
#@BQB_IS_VERSION_OF:['http://identifiers.org/go/GO:0016570', 'http://identifiers.org/go/GO:0015272', 'http://identifiers.org/go/GO:0019228']
#@creatorEmail:'enuo@caltech.edu'
#@creatorName:'He Enuo'
#@notes:'This is an atomized translation of an SBML model created on 12/08/2021. The original model has 2 molecules and 2 reactions. The translated model has 7 molecules and 7 rules'
###
begin model
begin parameters
	gca 1000.0
	gkca 900.0
	gk 1400.0
	gir 5.0
	vca 50.0
	vk -75.0
	vir -75.0
	cm 5300.0
	taun 16.0
	alpha 4.5e-06
	fcyt 0.01
	kpmca 0.2
	kd 0.3
	vn -16.0
	vm -20.0
	sn 5.0
	sm 12.0
	kserca 0.4
	dact 0.35
	dinact 0.4
	fer 0.01
	pleak 0.0005
	dip3 0.5
	vcytver 5.0
	sa 0.1
	r 0.14
	taua 300000.0
	gkatp 500.0
	ip3 0
end parameters
begin molecule types
	#^ bqbiol:is chebi:CHEBI:29108, kegg.compound:C00076
	cytosolicfreecaconcentration()
	#^ bqbiol:is chebi:CHEBI:29108, kegg.compound:C00076
	ERcaconcentration()
	nIR
	rIR
	n
	a
	V
end molecule types
begin seed species
	cytosolicfreecaconcentration() 6.022e+16 # (0.1 * 1e-06)mol/L * 6.022e23/mol *1.0L #cytosolicfreecaconcentration #c
	ERcaconcentration() 6.022e+19 #original 100.0micromole/L #ERcaconcentration #cer
	nIR 0.008
	rIR 0.282
	n 0.01
	a 0.46
	V -60.0
end seed species
begin observables
	Species cytosolicfreecaconcentration cytosolicfreecaconcentration() #cytosolic free ca concentration
	Species ERcaconcentration ERcaconcentration() #ER ca concentration
	Species nIR nIR
	Species rIR rIR
	Species n n
	Species a a
	Species V V
end observables
begin functions
	ik() = gk*n*(V - vk)
	minf() = 1/(exp((-V + vm)/sm) + 1)
	ica() = gca*minf*(V - vca)
	iir() = gir*nIR*rIR*(V - vir)
	ikatp() = a*gkatp*(V - vk)
	w() = cytosolicfreecaconcentration^5/(cytosolicfreecaconcentration^5 + kd^5)
	ikca() = gkca*w*(V - vk)
	arRateV() = (-ica - iir - ik - ikatp - ikca)/cm
	jleak() = pleak*(ERcaconcentration - cytosolicfreecaconcentration)
	jserca() = cytosolicfreecaconcentration*kserca
	oinf() = cytosolicfreecaconcentration*dinact*ip3/((cytosolicfreecaconcentration + dact)*(cytosolicfreecaconcentration + dinact)*(dip3 + ip3))
	jip3() = oinf*(ERcaconcentration - cytosolicfreecaconcentration)
	jer() = jip3 + jleak - jserca
	alphaIRr() = 30/(exp(0.04*V + 9.2) + 1)
	betaIRr() = 0.15/(exp(-0.05*V - 6.0) + 1)
	rIRinf() = 1/(1 + betaIRr/alphaIRr)
	tauIRr() = 1/(alphaIRr + betaIRr)
	arRaterIR() = (-rIR + rIRinf)/tauIRr
	alphaIRn() = 0.09/(exp(0.11*V + 11.0) + 1)
	betaIRn() = 0.00035*exp(0.07*V + 1.75)
	tauIRn() = 1/(alphaIRn + betaIRn)
	nIRinf() = 1/(1 + betaIRn/alphaIRn)
	arRatenIR() = (-nIR + nIRinf)/tauIRn
	ainf() = 1/(exp((-cytosolicfreecaconcentration + r)/sa) + 1)
	arRatea() = (-a + ainf)/taua
	ninf() = 1/(exp((-V + vn)/sn) + 1)
	arRaten() = (-n + ninf)/taun
	jmem() = -alpha*ica - cytosolicfreecaconcentration*kpmca
	fRate0() = fcyt*(jer + jmem)
	fRate1() = -fer*jer*vcytver
end functions
begin reaction rules
	reaction_0000005: 0  -> cytosolicfreecaconcentration() fRate0()
	reaction_0000006: 0  -> ERcaconcentration() fRate1()
	0  -> nIR() arRatenIR #rateLaw
	0  -> rIR() arRaterIR #rateLaw
	0  -> n() arRaten #rateLaw
	0  -> a() arRatea #rateLaw
	0  -> V() arRateV #rateLaw
end reaction rules
end model
