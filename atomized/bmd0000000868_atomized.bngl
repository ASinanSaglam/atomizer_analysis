begin model
begin parameters
	r1_k1 0.200000
	r2_k1 0.050000
	r3_k1 0.000380
	r4_k1 0.000380
	r5_k1 0.005000
	r5_k2 0.000240
end parameters
begin molecule types
	#^ bqbiol:is uniprot:Q00653
	p100(nik,t~T~0)
	#^ bqbiol:is uniprot:Q99558
	NIK(p100)
	#^ bqbiol:is uniprot:Q00653
	p52()
end molecule types
begin seed species
	$p100(nik,t~T) 1505500000000000.0 # (2.5 * 1e-09)mol/L * 6.022e23/mol *1.0L #p100t #p100t
