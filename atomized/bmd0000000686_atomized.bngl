begin model
begin parameters
	a0 0.1 #units:unit_1
	a1 0.2 #units:unit_1
	k 1.0 #units:unit_1
	u 1.0 #units:unit_1
	lambda 10.0 #units:unit_1
	d 0.1 #units:unit_1
	beta 0.1 #units:unit_1
	gamma 0.5 #units:unit_1
	phi 0.1 #units:unit_1
	eta 0.01 #units:unit_1
end parameters
begin compartments
	#volume units: L
	COMpartment 3 1.0 #Mouse
end compartments
begin molecule types
	x
	y0
	y1
	L
	v
end molecule types
begin seed species
	x@COMpartment 1.0 #units:unit_0
	y0@COMpartment 0
	y1@COMpartment 0
	L@COMpartment 0
	v@COMpartment 1.0 #units:unit_0
end seed species
begin observables
	Species x x
	Species y0 y0
	Species y1 y1
	Species L L
	Species v v
end observables
begin functions
	arRatev() = k*y1 - u*v
	arRateL() = -L*d - L*phi + gamma*v*x
	arRatey1() = -a1*y1 + eta*y0
	arRatey0() = L*phi - a0*y0 + beta*v*x - eta*y0
	arRatex() =  lambda -beta*v*x -d*x -gamma*v*x
	R0() =  lambda*eta*(beta + gamma*phi/(d + phi))/(a1*d*(a0 + eta))
end functions
begin reaction rules
	0  -> x()@COMpartment arRatex #rateLaw
	0  -> y0()@COMpartment arRatey0 #rateLaw
	0  -> y1()@COMpartment arRatey1 #rateLaw
	0  -> L()@COMpartment arRateL #rateLaw
	0  -> v()@COMpartment arRatev #rateLaw
end reaction rules
end model
