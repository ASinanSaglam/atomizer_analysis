begin model
begin parameters
	alpha 2.0
	k 0.2
	sigma 0.25
end parameters
begin molecule types
	#^ bqbiol:isVersionOf cl:CL:0001063
	x_Malignant_Cells()
	#^ bqbiol:is cl:CL:0000542
	y_Lymphocytes()
end molecule types
begin seed species
	x_Malignant_Cells() 3.19166e+21 # (5.3 * 0.001)mol/L * 6.022e23/mol *1.0L #x_Malignant_Cells #x_Malignant_Cells
	y_Lymphocytes() 4.0347400000000004e+21 #original 6.7substance/volume  #y_Lymphocytes #y_Lymphocytes
end seed species
begin observables
	Species x_Malignant_Cells x_Malignant_Cells() #x_Malignant_Cells
	Species y_Lymphocytes y_Lymphocytes() #y_Lymphocytes
end observables
begin functions
	fRate0() = alpha*x_Malignant_Cells
	fRate1() = y_Lymphocytes
	fRate3() = 1/alpha
	fRate4() = k*x_Malignant_Cells
end functions
begin reaction rules
	Malignant_Cells_Growth: 0  -> x_Malignant_Cells() fRate0()
	Malignant_Cells_Death_Lymphocytes: x_Malignant_Cells() -> y_Lymphocytes() fRate1()
	Lymphocytes_Production: 0  -> y_Lymphocytes() sigma
	Lymphocyte_Death: y_Lymphocytes() -> 0  fRate3()
	Lymphocyte_Elimination_Malignant_Cells_reactants_0: 0  -> y_Lymphocytes() -1*(fRate4()) #Modifiers(x_Malignant_Cells)
end reaction rules
end model
