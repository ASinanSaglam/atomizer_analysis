#@notes:'This is an atomized translation of an SBML model created on 13/08/2021. The original model has 8 molecules and 0 reactions. The translated model has 8 molecules and 8 rules'
###
begin model
begin parameters
	mu1 1.0
	mu2 0.1
	mu23 0.1
	mu3 0.1
	mu4 0.1
	k1 1.0
	k2 1.0
	k3 5.0
	k4 5.0
	mu5 1.0
end parameters
begin molecule types
	E1()
	Z3()
	Z2()
	E3()
	E4()
	E2()
	Z1()
	Z4()
end molecule types
begin seed species
	Z1() 3.011e+23 # 0.5mol/L * 6.022e23/mol *1.0L #Z1 #Z1
	Z2() 6.022e+24 #original 10.0mol/L  #Z2 #Z2
	Z3() 6.022e+24 #original 10.0mol/L  #Z3 #Z3
	Z4() 6.022e+25 #original 100.0mol/L  #Z4 #Z4
	E1()() 0.001 * (0.5) / 0.999 #E1
end seed species
begin observables
	Species Z1 Z1() #Z1
	Species Z2 Z2() #Z2
	Species Z3 Z3() #Z3
	Species Z4 Z4() #Z4
	Species E1 E1() #E1
	Species E2 E2() #E2
	Species E3 E3() #E3
	Species E4 E4() #E4
end observables
begin functions
	arRateE4() = E3*Z4*mu4 - E4*k4
	arRateE3() = -E3*k3 + Z3*(E2*mu23 + E4*mu3)
	arRateE2() = E1*Z2*mu2 - E2*k2
	arRateE1() = -E1*k1 + Z1*(E2*mu1 + E4*mu5)
	arRateZ4() = -E3*Z4*mu4 + E4*k4
	arRateZ3() = E3*k3 + Z3*(-E2*mu23 - E4*mu3)
	arRateZ2() = -E1*Z2*mu2 + E2*k2
	arRateZ1() = E1*k1 + Z1*(-E2*mu1 - E4*mu5)
end functions
begin reaction rules
	0  -> Z1() arRateZ1 #rateLaw
	0  -> Z2() arRateZ2 #rateLaw
	0  -> Z3() arRateZ3 #rateLaw
	0  -> Z4() arRateZ4 #rateLaw
	0  -> E1() arRateE1 #rateLaw
	0  -> E2() arRateE2 #rateLaw
	0  -> E3() arRateE3 #rateLaw
	0  -> E4() arRateE4 #rateLaw
end reaction rules
end model
