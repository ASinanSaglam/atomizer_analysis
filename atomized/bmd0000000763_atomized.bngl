begin model
begin parameters
	gamma 10.0
	p 0.5
	k 4.15
	sigma_H 0.5
	alpha 0.19
	Ntilde 0.04
	delta_H 1.0
	sigma_C 2.0
end parameters
begin molecule types
	#^ bqbiol:isVersionOf cl:CL:0000912
	T_H()
	#^ bqbiol:isVersionOf ncit:C12922
	N_Tumour()
	#^ bqbiol:isVersionOf cl:CL:0000910
	T_C()
end molecule types
begin seed species
	N_Tumour() 6.022000000000001e+18 # (0.01 * 0.001)mol/L * 6.022e23/mol *1.0L #N_Tumour #N_Tumour
	T_H() 3.6132e+20 #original 0.6substance/volume  #T_H #T_H
	T_C() 2.7099000000000007e+21 #original 4.5substance/volume  #T_C #T_C
end seed species
begin observables
	Species N_Tumour N_Tumour() #N_Tumour
	Species T_H T_H() #T_H
	Species T_C T_C() #T_C
end observables
begin functions
	fRate0() = N_Tumour*gamma
	fRate0m() = N_Tumour*gamma
	fRate1() = T_C*k*p
	fRate4() = T_C*T_H
	fRate5() = N_Tumour*T_H*alpha/(N_Tumour^2 + Ntilde^2)
	fRate7() = N_Tumour*k
	fRate7m() = N_Tumour*T_C*k*p
end functions
begin reaction rules
	Tumor_Growth: 0  <-> N_Tumour() fRate0(),fRate0m() #Modifiers(N_Tumour)
	Tumor_Elimination: N_Tumour() -> 0  fRate1() #Modifiers(T_C)
	T_Helper_Infiltration_Passive: 0  -> T_H() sigma_H
	T_Cytotoxic_Infiltration_Passive: 0  -> T_C() sigma_C
	T_Cytotoxic_Infiltration_Active: 0  -> T_C() fRate4() #Modifiers(T_C, T_H)
	T_Helper_Infiltration_Active: 0  -> T_H() fRate5() #Modifiers(T_H, N_Tumour)
	T_Cytotoxic_Death: T_C() -> 0  1.00000000000000
	T_Cytotoxic_Tumor_Infiltrate_Elimination: T_C() <-> 0  fRate7(),fRate7m() #Modifiers(N_Tumour)
	T_Helper_Death: T_H() -> 0  delta_H
end reaction rules
end model
